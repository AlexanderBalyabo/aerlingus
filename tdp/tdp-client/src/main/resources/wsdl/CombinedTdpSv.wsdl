               <xs:attribute name="ProductLongName" type="xs:string" use="optional"/>
               <xs:attribute name="ProductShortDescription" type="xs:string" use="optional"/>
               <xs:attribute name="ProductLongDescription" type="xs:string" use="optional"/>
               <xs:attribute name="ComponentShortName" type="xs:string" use="optional"/>
               <xs:attribute name="ComponentLongName" type="xs:string" use="optional"/>
               <xs:attribute name="ComponentShortDescription" type="xs:string" use="optional"/>
               <xs:attribute name="ComponentLongDescription" type="xs:string" use="optional"/>
