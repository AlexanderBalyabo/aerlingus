               <xs:complexContent>
                  <xs:extension base="RemoveSubRequest">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="BillingAllocationIdentifier"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="PaymentScheduleIdentifier"/>
                     </xs:sequence>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
