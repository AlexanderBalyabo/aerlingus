         <xs:element name="PaymentSchedule">
            <xs:annotation>
               <xs:documentation>A list of dates and amounts slated for payment by the customer.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="BillingAllocationLink">
                           <xs:complexType>
                              <xs:attribute name="BillingAllocationSequence" type="xs:positiveInteger" use="optional"/>
                              <xs:attribute name="BillingAllocationDocRef" type="xs:IDREF" use="optional"/>
                           </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                     <xs:attribute name="AmountDue" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>The amount due on the payment date.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="MilesAmountDue" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Mileage amount due for this payment schedule</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PaymentDueDate" type="xs:dateTime" use="required">
                        <xs:annotation>
                           <xs:documentation>The date payment is due.  Based on the time zone of the system.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
