         <xs:element name="TimeStamps">
            <xs:annotation>
               <xs:documentation>This can contain multiple timestamps to enable tracking of the transaction through various systems. Each system is responsible for creating their own timestamp.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="TimeStamp">
                     <xs:annotation>
                        <xs:documentation>This corresponds to one timestamp. When a transaction enters a system, one timestamp should be created immediately and should be updated as it leaves.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="TimeIn" type="xs:dateTime" use="required">
                           <xs:annotation>
                              <xs:documentation>This will specify the time the service was entered.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="TimeOut" type="xs:dateTime" use="required">
                           <xs:annotation>
                              <xs:documentation>This will specify the time the current service is exited.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="TransactionId" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>Used to uniquely identify the request/transaction that this Timestamp represents.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Source" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>This will specify which service the timestamp was generated by.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Target" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Used to identify the target system for this service</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="SessionInfo">
            <xs:annotation>
               <xs:documentation>Contains information related to the session. A SessionInfo object is usually established at the beginning of a session.</xs:documentation>
               <xs:appinfo>
                  <xs:has-property name="Owner" value="Copyright (c) Datalex PLC 2002, All Rights Reserved."/>
                  <xs:has-property name="Revision" value="$Revision: 1.5.14.1 $"/>
               </xs:appinfo>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="ns7:CustomProperties">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns35:MemberProfileRQ"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns7:Language"/>
                     </xs:sequence>
                     <xs:attribute name="Source" type="xs:string" use="optional"/>
                     <xs:attribute default="Consumer" name="UserType" use="optional">
