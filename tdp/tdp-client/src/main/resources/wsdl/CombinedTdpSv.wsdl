                                 </xs:sequence>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                  </xs:sequence>
                  <xs:attributeGroup ref="NegotiatedFareAttributes"/>
               </xs:extension>
            </xs:complexContent>
