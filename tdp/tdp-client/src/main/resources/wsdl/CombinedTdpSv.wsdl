      <xs:schema elementFormDefault="qualified" targetNamespace="urn:RoomStay.types.extensions.hotel.rqrs.ota.datalex.com" xmlns="urn:RoomStay.types.extensions.hotel.rqrs.ota.datalex.com">
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05/hotel"/>
         <xs:import namespace="urn:merch.rqrs.datalex.com"/>
         <xs:element name="TPA_Extensions">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" ref="ns4:OTA_HotelDescriptiveInfoRS"/>
                  <xs:element minOccurs="0" name="SequenceNumber" type="xs:positiveInteger">
                     <xs:annotation>
                        <xs:documentation>This identifies the sequencing order of the current RoomStay element</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="HotelDescriptiveInfoLink">
                     <xs:annotation>
                        <xs:documentation>This element contains the reference to the unique HotelDescriptiveContent.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="HotelDescriptiveContentDocRef" type="xs:IDREF" use="required">
                           <xs:annotation>
                              <xs:documentation>The reference to the unique HotelDescriptiveContent.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="HotelCallbackAvailable">
                     <xs:annotation>
                        <xs:documentation>This element indicates if a second hotel availability call can return price breakdown and/or policy information for this option. Some systems may be capable of returning this detail but not in the initial availability response.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="PoliciesAvailableInCallback" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>Value of true indicates a second call to availability service will result in policies being populated in the RoomStay element. Value of false indicates policies already in availability response or policies not available in second availablity request.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RateBreakdownAvailableInCallback" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>Value of true indicates a second call to availability service will result in rate breakdown being populated in the RoomStay element. Value of false indicates rate breakdown already in availability response or policies not available in second availablity request.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="QuoteKey" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Quote key to the rate</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns9:Persona"/>
                  <xs:element minOccurs="0" name="AmenityMask" type="xs:long"/>
                  <xs:element default="false" minOccurs="0" name="RedemptionEligible" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Flag to mark if the RoomStay to be removed or not</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="Offer" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>Boolean flag which indicates whether to offer this item or not. Used in the context of Merchandizing Service.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
