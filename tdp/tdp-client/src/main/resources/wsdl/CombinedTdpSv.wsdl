                        <xs:attribute name="Currency" type="AlphaLength3" use="required"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                        <xs:attribute name="ReasonCode" type="xs:string" use="optional"/>
                        <xs:attribute name="ReasonDescription" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="MinSuggestedPrice">
                     <xs:annotation>
                        <xs:documentation>The Minimum Suggested Price for this TotalFare.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Amount" type="Money" use="required"/>
                        <xs:attribute name="Currency" type="AlphaLength3" use="required"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                        <xs:attribute name="ReasonCode" type="xs:string" use="optional"/>
                        <xs:attribute name="ReasonDescription" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="AgentNetPrice">
                     <xs:annotation>
                        <xs:documentation>The Agent Net Price for this Fare.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Amount" type="Money" use="required"/>
                        <xs:attribute name="Currency" type="AlphaLength3" use="required"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                        <xs:attribute name="ReasonCode" type="xs:string" use="optional"/>
                        <xs:attribute name="ReasonDescription" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="RedemptionMiles">
                     <xs:annotation>
                        <xs:documentation>The redemption element which contains attributes describing the redemption/award miles amount required.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Amount" type="xs:decimal" use="required">
                           <xs:annotation>
                              <xs:documentation>This attribute contains the amount of miles required to redeem this award.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AwardCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is the award code associated with this redemption miles amount. This is informational.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="MoneyToMilesRatio" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>The RedemptionMiles@MoneyToMilesRatio attribute can be used in the web tier to dynamically calculate the number of miles required to pay for any monetary amount. It assumes the ratio is MoneyToMilesRatio :1. For example a base fare of $100 where MoneyToMilesRatio is 1000 requires 100,000 frequent flyer miles to pay for the full base fare (100 * 1000 = 100,000 miles). To take another example the miles required to pay for $50 of the base fare would be 50,000 miles.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:FareInfo.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:FareInfo.types.extensions.air.rqrs.ota.datalex.com">
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05"/>
         <xs:import namespace="urn:common.air.rqrs.datalex.com"/>
         <xs:import namespace="urn:policy.rqrs.datalex.com"/>
         <xs:import namespace="urn:BaseFare.types.extensions.air.rqrs.ota.datalex.com"/>
         <xs:import namespace="urn:EquivFare.types.extensions.air.rqrs.ota.datalex.com"/>
         <xs:simpleType name="StringLength1to8">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 8</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="8"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="StringLength1to16">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 16</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="16"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="CustomProperties">
            <xs:annotation>
               <xs:documentation>Used to inject name value pairs into elements (internal use only).</xs:documentation>
            </xs:annotation>
            <xs:attribute name="NameValues" type="xs:string" use="optional"/>
         </xs:complexType>
         <xs:simpleType name="StringLength1to64">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 64</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="64"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="Numeric1to999">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Numeric values, from 1 to 999 inclusive.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:integer">
               <xs:minInclusive value="1"/>
               <xs:maxInclusive value="999"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:attributeGroup name="LanguageGroup">
            <xs:attribute name="Language" type="xs:language" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Language identification.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:complexType name="FormattedTextTextType">
            <xs:simpleContent>
               <xs:extension base="xs:string">
                  <xs:attribute name="Formatted" type="xs:boolean" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Textual information, which may be formatted as a line of information, or unformatted, as a paragraph of text.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attributeGroup ref="LanguageGroup"/>
               </xs:extension>
            </xs:simpleContent>
         </xs:complexType>
         <xs:simpleType name="StringLength1to32">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 32</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="32"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:attributeGroup name="DateTimeStampGroup">
            <xs:annotation>
               <xs:documentation xml:lang="en">Creation date time, Creator Id, last modification date time and last Modifier Id.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="CreateDateTime" type="xs:dateTime" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Time stamp of the creation.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="CreatorID" type="StringLength1to32" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">ID of creator. The creator could be a software system identifier or an identifier of an employee resposible for the creation.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="LastModifyDateTime" type="xs:dateTime" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Time stamp of last modification.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="LastModifierID" type="StringLength1to32" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Identifies the last software system or person to modify a record.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:complexType name="ParagraphType">
            <xs:complexContent>
               <xs:extension base="CustomProperties">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">An indication of a new paragraph for a sub-section of a formatted text message.</xs:documentation>
                  </xs:annotation>
                  <xs:choice maxOccurs="99">
                     <xs:element name="Text" type="FormattedTextTextType"/>
                     <xs:element name="Image" type="xs:string"/>
                     <xs:element name="URL" type="xs:anyURI"/>
                     <xs:element name="ListItem">
                        <xs:complexType>
                           <xs:simpleContent>
                              <xs:extension base="FormattedTextTextType">
                                 <xs:attribute name="ListItem" type="xs:integer" use="optional"/>
                              </xs:extension>
                           </xs:simpleContent>
                        </xs:complexType>
                     </xs:element>
                  </xs:choice>
                  <xs:attribute name="Name" type="StringLength1to64" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">In many cases the description repeats, this will allow you to define the information that is being sent, typically used when multiple occurrences of ParagraphType are being sent.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ParagraphNumber" type="xs:nonNegativeInteger" use="optional"/>
                  <xs:attributeGroup ref="DateTimeStampGroup"/>
                  <xs:attributeGroup ref="LanguageGroup"/>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:complexType name="FormattedTextSubSectionType">
            <xs:complexContent>
               <xs:extension base="CustomProperties">
                  <xs:sequence>
                     <xs:element maxOccurs="99" name="Paragraph" type="ParagraphType"/>
                  </xs:sequence>
                  <xs:attribute name="SubTitle" type="StringLength1to64" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">This attribute may be used to provide a title for a sub-section of the formatted free text.  A sub-section may have multiple related paragraphs of information.  For example, if used to provide driving directions there may be multiple paragraphs, and these paragraphs may be grouped into a sub-section called "Driving from the North".  A second subsection may be titled"Driving from the South", and may contain several paragraphs to describe the driving directions when driving from the south.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="SubCode" type="StringLength1to8" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">An optional code that may be assigned to this sub-section of formatted free text.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="SubSectionNumber" type="Numeric1to999" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">This attribute may be used when there is a need to number all of the sub-sections of information that is to be presented.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:complexType name="FormattedTextType">
            <xs:complexContent>
               <xs:extension base="CustomProperties">
                  <xs:sequence>
                     <xs:element maxOccurs="99" name="SubSection">
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="FormattedTextSubSectionType"/>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                  </xs:sequence>
                  <xs:attribute name="Title" type="StringLength1to64" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">This attribute may be used to provide a title for the formatted free text,
for example, Driving Directions.  Each of the sub sections that are defined
to be a part of the formatted text would provide detailed information about
the subject identified by the title.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Language" type="xs:language" use="optional"/>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:simpleType name="RPH_Type">
            <xs:annotation>
               <xs:documentation xml:lang="en">(Reference Place Holder) - an index code to identify an instance in a collection of like items.. For example,  used to assign individual passengers or clients to particular itinerary items.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[0-9]{1,8}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="Money">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for amounts</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:decimal"/>
         </xs:simpleType>
         <xs:simpleType name="AlphaLength3">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Alphabetic Strings, length exactly 3</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[a-zA-Z]{3}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="NVOType">
            <xs:annotation>
               <xs:documentation>Not Valid Option Type consisting of "NVA" and "NVB" values</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="NVA"/>
               <xs:enumeration value="NVB"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="BaggageOption">
            <xs:annotation>
               <xs:documentation>The baggage option included for this fare. It contains optional baggage fee and tax information. One of BaggageCode, BaggageAllowancePieces or BaggageAllowanceWeight must be populated</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="BaggageCode" type="xs:string" use="optional"/>
               <xs:attribute name="BaggageAllowancePieces" type="xs:nonNegativeInteger" use="optional"/>
               <xs:attribute name="BaggageAllowanceWeight" type="xs:decimal" use="optional"/>
               <xs:attribute name="BaggageAllowanceWeightInPounds" type="xs:boolean" use="optional"/>
               <xs:attribute name="FlightSegmentReference" type="RPH_Type" use="optional"/>
               <xs:attribute name="Amount" type="Money" use="optional"/>
               <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional"/>
               <xs:attribute name="TaxAmount" type="Money" use="optional"/>
               <xs:attribute name="TaxCurrencyCode" type="AlphaLength3" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="BaseFare">
                     <xs:annotation>
                        <xs:documentation>The base price for this component of the fare, expressed in the currency of the Sales City</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element minOccurs="0" ref="ns218:TPA_Extension"/>
                        </xs:sequence>
                        <xs:attribute name="Amount" type="Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="EquivFare">
                     <xs:annotation>
                        <xs:documentation>Price of the inventory excluding taxes and fees in the payable currency.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element minOccurs="0" ref="ns219:TPA_Extension"/>
                        </xs:sequence>
                        <xs:attribute name="Amount" type="Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareInfoReference">
                     <xs:annotation>
                        <xs:documentation>This hold the reference to the unique fare info</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="UniqueFareInfoRef" type="xs:nonNegativeInteger" use="required"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="MarketPair" type="xs:string"/>
                  <xs:element minOccurs="0" ref="ns20:RedemptionFare"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="DiscountApplied">
                     <xs:annotation>
                        <xs:documentation>The Discount Applied to this fare. Attributes Amount, Currency and Decimal Places are all required if this element is present except when it's an Award, Money and Miles or the fare was filed only as private and therefore we cannot work out the discount amount. It will be echoed back in the response unless a price tolerance failure occurs.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" minOccurs="0" name="CombinablePromotion" type="xs:string">
                              <xs:annotation>
                                 <xs:documentation>Promotion that can be combined for discount.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is a record of the percentage value used to calculate the Amount. It will not be set when the discount is based on a flat amount. Instead AmountToApply is set.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AmountToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is the value of a flat amount discount.  DO NOT SET THIS IF DISCOUNT IS PERCENTAGE BASED.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Amount" type="Money" use="optional">
                           <xs:annotation>
                              <xs:documentation>The total amount of discount that was applied to this fare. This is set for both flat amount discounts and percentage based discounts.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RedemptionAmount" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>The total amount of redemption discount (miles) that was applied to this fare.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional">
                           <xs:annotation>
                              <xs:documentation>The 3 letter currency code of the discount amount.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>The number of decimal places the discount is expressed in</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DiscountCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is the internal promotion code.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DiscountDescription" type="xs:string" use="optional"/>
                        <xs:attribute default="true" name="DiscountDisplayed" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>If false, then the CUI should not display any promotion information. If it is true, or absent then the CUI can display promotion information.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CodeContext" type="xs:string" use="optional"/>
                        <xs:attribute name="EndorsementText" type="xs:string" use="optional"/>
                        <xs:attribute name="RoundingRule" use="optional">
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="ROUND_UP"/>
                                 <xs:enumeration value="ROUND_DOWN"/>
                                 <xs:enumeration value="ROUND_CEILING"/>
                                 <xs:enumeration value="ROUND_FLOOR"/>
                                 <xs:enumeration value="ROUND_HALF_UP"/>
                                 <xs:enumeration value="ROUND_HALF_DOWN"/>
                                 <xs:enumeration value="ROUND_HALF_EVEN"/>
                                 <xs:enumeration value="ROUND_UNNECESSARY"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="AppliedPromoCodes" type="xs:string" use="optional"/>
                        <xs:attribute name="ExternalPromotionCode" type="xs:string">
                           <xs:annotation>
                              <xs:documentation>If a promotion code was supplied by a user, it is stored here.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CertificateCode" type="xs:string">
                           <xs:annotation>
                              <xs:documentation>If a certificate code was supplied by a user, it is supplied here.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="PromotionReference" type="xs:IDREF">
                           <xs:annotation>
                              <xs:documentation>This is an aid to link a DiscountApplied element to a detailed Promotion element.  It permits direct access to the Promotion element.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CertificateReference" type="xs:IDREF">
                           <xs:annotation>
                              <xs:documentation>This is an aid to link a DiscountApplied element to a detailed Certificate element.  It permits direct access to the Certificate element.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute default="false" name="ApplyDiscount" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>Flag will be reset by BRE if Discount passes the BRE validation.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CertificateNumber" type="xs:string"/>
                        <xs:attribute name="RedemptionPercentToApply" type="xs:decimal">
                           <xs:annotation>
                              <xs:documentation>This is the percentage value to apply as a discount for a promotion which is for miles. The actualy mileage value will be in RedemptionAmount.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="MarkupApplied">
                     <xs:annotation>
                        <xs:documentation>Indicates that there is a markup applied to the original fare</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>percentage markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AmountToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>Flat amount markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional">
                           <xs:annotation>
                              <xs:documentation>Amount for flat amount markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional">
                           <xs:annotation>
                              <xs:documentation>Currency of markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>Decimal places for the currency used</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="MarkupCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Code of markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="MarkupDescription" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Description of the nature of markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CodeContext" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Code context TDP/GDS etc</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RoundingRule" use="optional">
                           <xs:annotation>
                              <xs:documentation>Rounding rule for the currency</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="ROUND_UP"/>
                                 <xs:enumeration value="ROUND_DOWN"/>
                                 <xs:enumeration value="ROUND_CEILING"/>
                                 <xs:enumeration value="ROUND_FLOOR"/>
                                 <xs:enumeration value="ROUND_HALF_UP"/>
                                 <xs:enumeration value="ROUND_HALF_DOWN"/>
                                 <xs:enumeration value="ROUND_HALF_EVEN"/>
                                 <xs:enumeration value="ROUND_UNNECESSARY"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="CommissionApplied">
                     <xs:complexType>
                        <xs:attribute name="PercentToApply" type="xs:decimal" use="optional"/>
                        <xs:attribute name="AmountToApply" type="xs:decimal" use="optional"/>
                        <xs:attribute name="Amount" type="Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                        <xs:attribute name="CommissionCode" type="xs:string" use="optional"/>
                        <xs:attribute name="CodeContext" type="xs:string" use="optional"/>
                        <xs:attribute name="CommissionDescription" type="xs:string" use="optional"/>
                        <xs:attribute name="EndorsementText" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="RuleKey" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Contains a key for retrieving the complete rule text from the Fare Shopping Engine</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="SpecialFareIdentifiers">
                     <xs:annotation>
                        <xs:documentation>Contains a list of special identifiers pertaining to this fare</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="10" name="SpecialFareIdentifier">
                              <xs:annotation>
                                 <xs:documentation>This is used to identify a FareInfo element as being a special type of fare, like a Fare Sale or an E-Fare. More than one of these identifiers can apply to a fare.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element minOccurs="0" name="NameContentKey" type="xs:string"/>
                                    <xs:element minOccurs="0" name="DetailsContentKey" type="xs:string"/>
                                    <xs:element minOccurs="0" name="GraphicIndLink" type="xs:string"/>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" name="SegmentAssociation">
                                       <xs:annotation>
                                          <xs:documentation>The FlightSegment RPH number with which this special fare is associated. A special fare may be associated with more than one segment.</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:attribute name="FlightSegmentReference" type="RPH_Type" use="required"/>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                                 <xs:attribute name="Priority" type="xs:nonNegativeInteger" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Specifies the priority of this special fare. Used by the client to determine which graphic etc. to use in the event that more than one special fare applies to an itinerary.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="Label" type="StringLength1to16" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The label for this special fare</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="PTCCode" type="StringLength1to8" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="SegmentAssociation">
                     <xs:annotation>
                        <xs:documentation>The FlightSegment RPH number with which this fare is associated. A fare may be associated with more than one segment.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="FlightSegmentReference" type="RPH_Type" use="required"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="PassengerAssociation">
                     <xs:annotation>
                        <xs:documentation>A passenger type and index with which this fare is associated. A fare may be associated with more than one passenger.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" minOccurs="0" name="RemoteSystemFareQuotePTCs">
                              <xs:annotation>
                                 <xs:documentation>This multiplexed element will store the remote fare quote system PTC and the remote system will be the code context.  The remote system PTC code(s) should copied to the reservation and subsequently used for re-price and ticketing operations.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="ContextCode" type="xs:string" use="required"/>
                                 <xs:attribute name="PassengerTypeCode" type="xs:string" use="required"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="PassengerTypeCode" type="xs:string" use="optional"/>
                        <xs:attribute name="ContextCode" type="xs:string" use="optional"/>
                        <xs:attribute name="PassengerReference" type="RPH_Type" use="optional"/>
                        <xs:attribute name="PassengerTypeMinAge" type="xs:int" use="optional"/>
                        <xs:attribute name="PassengerTypeMaxAge" type="xs:int" use="optional"/>
                        <xs:attribute name="PassengerTypeAge" type="xs:int" use="optional"/>
                        <xs:attribute name="QualifiedPassengerType" type="xs:boolean" use="optional"/>
                        <xs:attribute name="EligibilityID" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is a simple String attribute for traveler supplementary Eligibility Identification (i.e. ISIC card).</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="ItineraryReference" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>The itinerary reference returned from Bif. This is required to retrieve all further info (farerules, availrules etc.) for this fare contract combined with the FareContractSupplementalInfo.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns20:FareContractSupplementalInfo"/>
                  <xs:element minOccurs="0" name="Version" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>The version number associated with this fare.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="NetFare">
                     <xs:annotation>
                        <xs:documentation>The Published Fare less any applicable commission for this inventory</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Amount" type="Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="PlatingCarrier" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>The Plating Carrier code.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="Cat25Fare" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>This is a category 25 Fare - special type of private fare on Galileo / Apollo</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="Cat1922DiscountedFare" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>This is a fare that has been discounted by Cat19 (Child) or Cat22 (Special) discounts.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="Cat35Fare" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>This is a fare that has been constructed via Category 35 (Negotiated Fare)</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareTypeIndicator">
                     <xs:annotation>
                        <xs:documentation>The FareType of the Priced Option.</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="GDS_PUBLISHED"/>
                           <xs:enumeration value="GDS_PRIVATE"/>
                           <xs:enumeration value="LOW_COST_CARRIER"/>
                           <xs:enumeration value="PRIVATE_NETT"/>
                           <xs:enumeration value="PRIVATE_NETT_IT"/>
                           <xs:enumeration value="REDEMPTION"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="TicketSourceCode">
                     <xs:annotation>
                        <xs:documentation>This defines the type of ticket to be issued, whether it's Manual, Airline, Bulk, Tour, etc.</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="NR"/>
                           <xs:enumeration value="BT"/>
                           <xs:enumeration value="IT"/>
                           <xs:enumeration value="AA"/>
                           <xs:enumeration value="AL"/>
                           <xs:enumeration value="BU"/>
                           <xs:enumeration value="CE"/>
                           <xs:enumeration value="CO"/>
                           <xs:enumeration value="CS"/>
                           <xs:enumeration value="CY"/>
                           <xs:enumeration value="EC"/>
                           <xs:enumeration value="FV"/>
                           <xs:enumeration value="PB"/>
                           <xs:enumeration value="PP"/>
                           <xs:enumeration value="SA"/>
                           <xs:enumeration value="BO"/>
                           <xs:enumeration value="IO"/>
                           <xs:enumeration value="TI"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="ExtendedFareReference" type="StringLength1to16"/>
                  <xs:element minOccurs="0" name="ShellContractReference" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>The shell contract reference returned from BIF. This reference is required to retrieve all further info (contract verification, endorsements, etc.) for the shell contract applied to this GDS Private Fare.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareFamilySummaryReference">
                     <xs:annotation>
                        <xs:documentation>Holder for a refence to a particular Fare Family Summary.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="FareFamilyCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>This is the code of the fare family upon which this FareInfo is based.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="FareFamilySummaryReferenceID" type="xs:IDREF" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is a reference to a Fare Family which is contained in a pricing response.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="PriceByFareBasisCode">
                     <xs:complexType>
                        <xs:attribute name="PriceByFareBasisCode" type="xs:boolean" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="SalesComment" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Sales Comment used in CCUI against BIFA fare</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="SupplierCode" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Supplier Code used in handoff to dolphin against a BIFA Shell fare</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="BaggageOption"/>
                  <xs:element minOccurs="0" name="Surcharges">
                     <xs:annotation>
                        <xs:documentation>Surcharges associated with this fare.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="Fee">
                              <xs:annotation>
                                 <xs:documentation>The individual surcharge</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="NameValues" type="xs:string" use="optional"/>
                                 <xs:attribute name="TaxInclusive" type="xs:boolean">
                                    <xs:annotation>
                                       <xs:documentation xml:lang="en">Indicates whether taxes are included when figuring the fees.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="Type" type="ns8:AmountDeterminationType" use="optional">
                                    <xs:annotation>
                                       <xs:documentation xml:lang="en">An enumerated type that defines the type of fees applied to a rate.</xs:documentation>
                                       <xs:documentation xml:lang="en">Values: Inclusive, Exclusive, Cumulative.</xs:documentation>
                                       <xs:documentation xml:lang="en">Note: If FeeType= Cumulative, fees are accumulated on all previous fees in this ordered list.  For example, if this collection contains 5 different fees and the 4th fee is cumulative, the fee is based on an accumulation of the rate amount plus fees 1 through 3.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="Code" type="ns8:OTA_CodeType" use="optional">
                                    <xs:annotation>
                                       <xs:documentation xml:lang="en">Code identifying the fee (e.g.,agency fee, municipality fee, etc.). Refer to OTA Code List Fee Tax Type (FTT).</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="CodeModifier" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation xml:lang="en">Used if the Code needs a modifier appended to it, like '/', for use on the fare calc line. If the modifier is a number, then it's a pre-modifier.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="Percent" type="ns8:Percentage" use="optional">
                                    <xs:annotation>
                                       <xs:documentation xml:lang="en">Fee percentage; if zero, assume use of the Amount attribute (Amount or Percent must be a zero value).</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="Amount" type="Money" use="optional"/>
                                 <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional"/>
                                 <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional">
                                    <xs:annotation>
                                       <xs:documentation xml:lang="en">Indicates the number of decimal places for a particular currency. This is equivalent to the ISO 4217 standard "minor unit".</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="ApplicableOriginCity" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation xml:lang="en">The origin city to which this fee applies.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="ApplicableDestinationCity" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation xml:lang="en">The destination city to which this fee applies.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="TicketDesignator" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>If there is a ticket designator for this Fare info, it will be set in this field for the matching fare basis code.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns20:OriginatingTicketDesignator"/>
                  <xs:element minOccurs="0" ref="ns20:ResultingTicketDesignator"/>
                  <xs:element minOccurs="0" name="LatestTicketDates">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="LatestTicketDate" type="xs:date">
                              <xs:annotation>
                                 <xs:documentation>Ticketing deadline</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareRules" type="FormattedTextType">
                     <xs:annotation>
                        <xs:documentation>Contains a collection of fare rules that contains the category name, code and description for each rule</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="IsNonrefundable" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Whether the fare is nonrefundable or not.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns53:Policy"/>
                  <xs:element minOccurs="0" ref="ns20:AccountCode"/>
                  <xs:element minOccurs="0" name="ATPCOFareData">
                     <xs:annotation>
                        <xs:documentation>The ATPCO Fare Tariff data associated with this fare</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute default="false" name="Private" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>Inidicator that this tariff code is a private code.  This is usually obtainable from the G16 data</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Tariff" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>The tariff code itself.  This is the tariff number associated with a particular fare.  It is the equivalent of bytes 1-3 of the ATPCO Fares feed</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="FareTypeCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>The fare type code is generally provided within bytes 78-80 of the RECORD1 feed</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="TicketRequired" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Indicator is required to show if fare must be issued showing a cross ref to associated ticket number.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareSource" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>The fare source if the fare is quoted by TDP fares. (For a published fare this will be absent.) The default value is "TDP_FARES"; example values : "ATPCO_DOMESTIC", "ATPCO_INTERNATIONAL", "FROP" etc</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="ScheduleChangeEndorsementCode" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>The Schedule Change Endorsement  Code</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="DepartureMarkets">
                     <xs:annotation>
                        <xs:documentation>Contains a list of departure markets</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="MarketName" type="xs:string" use="required"/>
                        <xs:attribute name="MarketType" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="ArrivalMarkets">
                     <xs:annotation>
                        <xs:documentation>Contains a list of arrival markets</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="MarketName" type="xs:string" use="required"/>
                        <xs:attribute name="MarketType" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="NotValidOption">
                     <xs:annotation>
                        <xs:documentation>Not Valid Option either NVB or NVA for a flight segment</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Type" use="optional">
                           <xs:simpleType>
                              <xs:restriction base="NVOType"/>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="Date" type="xs:date" use="optional"/>
                        <xs:attribute name="FlightSegmentReference" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:common.air.rqrs.datalex.com" xmlns="urn:common.air.rqrs.datalex.com">
         <xs:simpleType name="AlphaLength3">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Alphabetic Strings, length exactly 3</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[a-zA-Z]{3}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="Money">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for amounts, max 3 decimals</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:decimal">
               <xs:fractionDigits value="3"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="RedemptionComboType">
            <xs:sequence>
               <xs:element minOccurs="0" name="MMoney">
                  <xs:annotation>
                     <xs:documentation>The Money part of the combo.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Provides a monetary
							amount and the code of the currency in which this amount is
							expressed.</xs:documentation>
                     </xs:annotation>
                     <xs:attribute name="Amount" type="Money" use="required"/>
                     <xs:attribute name="CurrencyCode" type="AlphaLength3" use="required"/>
                     <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                  </xs:complexType>
               </xs:element>
               <xs:element minOccurs="0" name="MMiles">
                  <xs:annotation>
                     <xs:documentation>The Miles part of the combo.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:attribute name="Amount" type="xs:nonNegativeInteger" use="required"/>
                     <xs:attribute name="IncludedPremium" type="xs:nonNegativeInteger">
                        <xs:annotation>
                           <xs:documentation>This value is used for tracking the amount of any premium which is included in the full amount. This attribute is used to determine the original value of an All Miles Price.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:complexType>
               </xs:element>
               <xs:element minOccurs="0" name="MMTaxes">
                  <xs:annotation>
                     <xs:documentation>The Taxes part of the combo.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element maxOccurs="99" name="MMTax">
                           <xs:annotation>
                              <xs:documentation>Tax(es) applied to the Money and Miles</xs:documentation>
                           </xs:annotation>
                           <xs:complexType>
                              <xs:attribute name="TaxCode" type="xs:string" use="required">
                                 <xs:annotation>
                                    <xs:documentation>Tax Code</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="Amount" type="Money" use="optional"/>
                              <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional"/>
                              <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                           </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
               <xs:element minOccurs="0" name="MMFees">
                  <xs:annotation>
                     <xs:documentation>The Fees part of the combo.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element maxOccurs="99" name="MMFee">
                           <xs:annotation>
                              <xs:documentation>Fee(s) applied to the Money and Miles</xs:documentation>
                           </xs:annotation>
                           <xs:complexType>
                              <xs:attribute name="FeeCode" type="xs:string" use="required">
                                 <xs:annotation>
                                    <xs:documentation>Fee Code</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="FeeSubCode" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>The sub code of the fee.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="Amount" type="Money" use="optional"/>
                              <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional"/>
                              <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                           </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
         </xs:complexType>
         <xs:complexType name="RedemptionFareType">
            <xs:annotation>
               <xs:documentation>The Redemption Money and Miles combo.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element maxOccurs="99" name="MoneyMiles">
                  <xs:annotation>
                     <xs:documentation>Redemption Point on Redemption slider</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:complexContent>
                        <xs:extension base="RedemptionComboType">
                           <xs:attribute default="false" name="TaxesFeesConvertedToMiles" type="xs:boolean">
                              <xs:annotation>
                                 <xs:documentation>Indicates whether or not the final MoneyMiles element contains taxes or fees which have been coverted from money to miles.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                        </xs:extension>
                     </xs:complexContent>
                  </xs:complexType>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="RedemptionProductDescription">
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element minOccurs="0" name="RedemptionProductLongDescription" type="xs:string"/>
                     </xs:sequence>
                     <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
                     <xs:attribute name="ProductName" type="xs:string" use="required"/>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
            <xs:attribute name="RedemptionProductCode" type="xs:string" use="optional"/>
            <xs:attribute name="IsMoneyConvertible" type="xs:boolean" use="optional"/>
         </xs:complexType>
         <xs:element name="RedemptionFare" type="RedemptionFareType">
            <xs:annotation>
               <xs:documentation>The Redemption Money and Miles combo.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="OriginatingTicketDesignator">
            <xs:annotation>
               <xs:documentation>The original Ticket Designator used for matching ATPCO data and for GDS fare retrieval</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string"/>
            </xs:simpleType>
         </xs:element>
         <xs:element name="FareContractSupplementalInfo">
            <xs:annotation>
               <xs:documentation>This data is needed so BIFA can determine things like the correct classes for feeder legs when requesting rules for an Itinerary Reference. It must be passed back in with the Itinerary reference if we want correct Rules from BIFA.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element minOccurs="0" name="OutboundAirpass">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="OutboundAirpassDetails">
                              <xs:complexType>
                                 <xs:attribute default="" name="OutboundAirpassKey" type="xs:string" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="InboundAirpass">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="InboundAirpassDetails">
                              <xs:complexType>
                                 <xs:attribute default="" name="InboundAirpassKey" type="xs:string" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="OutboundDomesticAddonReference" type="xs:string" use="optional"/>
               <xs:attribute name="OutboundInternationalAddonReference" type="xs:string" use="optional"/>
               <xs:attribute name="OutboundAirpassReference" type="xs:string" use="optional"/>
               <xs:attribute name="InboundDomesticAddonReference" type="xs:string" use="optional"/>
               <xs:attribute name="InboundInternationalAddonReference" type="xs:string" use="optional"/>
               <xs:attribute name="InboundAirpassReference" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="ResultingTicketDesignator">
            <xs:annotation>
               <xs:documentation>The returned Ticket Designator after processing. May be a Ticket Designator Modifier, or have the TDM appended to the Ticket Designator</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string"/>
            </xs:simpleType>
         </xs:element>
         <xs:element name="AccountCode">
            <xs:annotation>
               <xs:documentation>The returned account code after processing.</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
               <xs:restriction base="xs:string"/>
            </xs:simpleType>
         </xs:element>
         <xs:simpleType name="RedemptionAttribsType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Types of Exemptions</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="EXEMPTION-AGENCY_ALLMILES"/>
               <xs:enumeration value="PAYABLE-CUSTOMER_REDEMPTION"/>
               <xs:enumeration value="CONVERTIBLE-TO_MILES"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="RedemptionAttribs">
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="RedemptionAttrib" type="RedemptionAttribsType"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="OriginatingTicketDesignators">
            <xs:annotation>
               <xs:documentation>A wrapper element which contains 1 to many OriginatingTicketDesignator elements.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" ref="OriginatingTicketDesignator"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:policy.rqrs.datalex.com" xmlns="urn:policy.rqrs.datalex.com">
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:attributeGroup name="PolicyModifyGroup">
            <xs:annotation>
               <xs:documentation>The code and version of the Policy required to modify or delete attributes of a Policy.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="PolicyCode" type="xs:string" use="required"/>
            <xs:attribute name="PolicyVersion" type="xs:nonNegativeInteger" use="required"/>
            <xs:attribute name="RestrictToProvider" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>If filled, this policy is restricted for use by the provider code listed here.  This means the policy can not be shared with other providers and is not shown in system level maintenance.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:element name="PaymentPolicy">
            <xs:annotation>
               <xs:documentation>Details of a Payment policy</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="AssessPerGuestFlag" type="xs:boolean" use="optional"/>
               <xs:attribute name="DeadlineDaysOffset" type="xs:nonNegativeInteger" use="optional"/>
               <xs:attribute name="DeadlineHoursOffset" type="xs:nonNegativeInteger" use="optional"/>
               <xs:attribute name="RelativeDeadlineTimeOfDay" type="xs:time" use="optional">
                  <xs:annotation>
                     <xs:documentation>If a deposit or penalty date is based on an event such as booking or first consumption, this is the time of day to use when calculating the absolute date and time.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TimeZone" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Time zone of the deadline.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PaymentDeadlineOffsetBasisCode" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="COMP_START_DATE"/>
                        <xs:enumeration value="COMP_END_DATE"/>
                        <xs:enumeration value="PRIOR_TO_ARRIVAL"/>
                        <xs:enumeration value="AFTER_BOOKING"/>
                        <xs:enumeration value="AT_BOOKING"/>
                        <xs:enumeration value="ON_ARRIVAL"/>
                        <xs:enumeration value="ON_DEPARTURE"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="AbsoluteDeadline" type="xs:dateTime" use="optional"/>
               <xs:attribute name="AmountToApply" type="xs:decimal" use="optional"/>
               <xs:attribute name="CurrencyCode" type="xs:string" use="optional"/>
               <xs:attribute name="MilesAmountToApply" type="xs:decimal" use="optional"/>
               <xs:attribute name="PercentToApply" type="xs:decimal" use="optional"/>
               <xs:attribute name="MilesPercentToApply" type="xs:decimal" use="optional"/>
               <xs:attribute name="BalanceDueFlag" type="xs:boolean" use="optional"/>
               <xs:attribute name="PercentBasisCode" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="TOTAL_RESV"/>
                        <xs:enumeration value="ALL_UNITS"/>
                        <xs:enumeration value="LAST_UNIT"/>
                        <xs:enumeration value="FIRST_UNIT"/>
                        <xs:enumeration value="FIRST_LAST_UNIT"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute default="false" name="AssessPerBaseUnit" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>If TRUE, and the penalty or deposit is calculated on a number of base units of measure, use the unit of measure from the reservation component unit to assess the penalty.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="BaseUnitQuantity" type="xs:nonNegativeInteger" use="optional"/>
               <xs:attribute name="AssessmentBasisTypeCode" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="GROSS"/>
                        <xs:enumeration value="NET"/>
                        <xs:enumeration value="COST"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="DurationBasisTypeCode" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="FULL_DURATION"/>
                        <xs:enumeration value="LAST_UNIT"/>
                        <xs:enumeration value="FIRST_UNIT"/>
                        <xs:enumeration value="FIRST_LAST_UNIT"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="ConsumerPaysProvider" type="xs:boolean" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="CancelChangePolicy">
            <xs:annotation>
               <xs:documentation>Details of a Cancel or Change policy</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="DeadlineDaysPrior" type="xs:nonNegativeInteger" use="optional"/>
               <xs:attribute name="DeadlineHoursPrior" type="xs:nonNegativeInteger" use="optional"/>
               <xs:attribute name="RelativeDeadlineTimeOfDay" type="xs:time" use="optional">
                  <xs:annotation>
                     <xs:documentation>If a deposit or penalty date is based on an event such as booking or first consumption, this is the time of day to use when calculating the absolute date and time.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TimeZone" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Time zone of the deadline.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="DeadlineOffsetBasisCode" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="PRIOR_TO_ARRIVAL"/>
                        <xs:enumeration value="AFTER_BOOKING"/>
                        <xs:enumeration value="AT_BOOKING"/>
                        <xs:enumeration value="ON_ARRIVAL"/>
                        <xs:enumeration value="ON_DEPARTURE"/>
                        <xs:enumeration value="COMP_START_DATE"/>
                        <xs:enumeration value="COMP_END_DATE"/>
                        <xs:enumeration value="AFTER_TRAVEL_COMMENCED"/>
                        <xs:enumeration value="IROP_AFTER_TRAVEL_COMMENCED"/>
                        <xs:enumeration value="SCHEDULE_CHANGE_AFTER_TRAVEL_COMMENCED"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="AbsoluteDeadline" type="xs:dateTime" use="optional"/>
               <xs:attribute name="AmountToApply" type="xs:decimal" use="optional"/>
               <xs:attribute name="CurrencyCode" type="xs:string" use="optional"/>
               <xs:attribute name="MilesAmountToApply" type="xs:decimal" use="optional"/>
               <xs:attribute name="PercentToApply" type="xs:decimal" use="optional"/>
               <xs:attribute name="MilesPercentToApply" type="xs:decimal" use="optional"/>
               <xs:attribute name="PercentBasisCode" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="TOTAL_RESV"/>
                        <xs:enumeration value="ALL_UNITS"/>
                        <xs:enumeration value="LAST_UNIT"/>
                        <xs:enumeration value="FIRST_UNIT"/>
                        <xs:enumeration value="FIRST_LAST_UNIT"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute default="false" name="AssessPerBaseUnit" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>If TRUE, and the penalty or deposit is calculated on a number of base units of measure, use the unit of measure from the reservation component unit to assess the penalty.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="BaseUnitQuantity" type="xs:nonNegativeInteger" use="optional"/>
               <xs:attribute name="AssessmentBasisTypeCode" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="GROSS"/>
                        <xs:enumeration value="NET"/>
                        <xs:enumeration value="COST"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="DurationBasisTypeCode" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="FULL_DURATION"/>
                        <xs:enumeration value="LAST_UNIT"/>
                        <xs:enumeration value="FIRST_UNIT"/>
                        <xs:enumeration value="FIRST_LAST_UNIT"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="ConsumerPaysProvider" type="xs:boolean" use="optional"/>
               <xs:attribute name="TicketingMode" use="optional">
                  <xs:annotation>
                     <xs:documentation>Ticketing Mode. Possible values are:
						REVALIDATE -- the ticket should be re-validated;
						REISSUE -- re-pricing should be done using historical fares/taxes and the ticket exchanged;
						EXCHANGE -- re-pricing should be done using current fares/taxes and the ticket exchanged;
						QMANUAL -- the ticket should be queued.</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="REVALIDATE"/>
                        <xs:enumeration value="REISSUE"/>
                        <xs:enumeration value="EXCHANGE"/>
                        <xs:enumeration value="QMANUAL"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="AllowOverbook" type="xs:boolean" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="PolicyTypeDetails">
            <xs:annotation>
               <xs:documentation>Details for the specific type of policy. Depending on the type, either the cancel/change policy or payment policy is filled.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:choice>
                  <xs:element ref="CancelChangePolicy"/>
                  <xs:element ref="PaymentPolicy"/>
               </xs:choice>
               <xs:attribute name="PolicyType" use="required">
                  <xs:annotation>
                     <xs:documentation>Policy types define the type of policy within a group.  The current enumeration is:


Group Type = Payment
GUARANTEE
DEPOSIT
TICKET_TIME_LIMIT
PAYMENT
PREPAY
POSTPAY

Group Type = Cancellation
CANCELLATION
NOSHOWPENALTY
UNUSEDREFUND

Group Type = Change
SHORTSTAYPENALTY
BOOKINGCHANGE
PARTUSEDREFUND</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="GUARANTEE"/>
                        <xs:enumeration value="CANCELLATION"/>
                        <xs:enumeration value="DEPOSIT"/>
                        <xs:enumeration value="NOSHOWPENALTY"/>
                        <xs:enumeration value="SHORTSTAYPENALTY"/>
                        <xs:enumeration value="BOOKINGCHANGE"/>
                        <xs:enumeration value="PARTUSEDREFUND"/>
                        <xs:enumeration value="UNUSEDREFUND"/>
                        <xs:enumeration value="TICKET_TIME_LIMIT"/>
                        <xs:enumeration value="PAYMENT"/>
                        <xs:enumeration value="PREPAY"/>
                        <xs:enumeration value="POSTPAY"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="PolicyGroups">
            <xs:annotation>
               <xs:documentation>Links the Policy Groups to a Policy.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="PolicyGroupCode" type="xs:string" use="required"/>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="PolicyType">
            <xs:attribute name="PolicyGroupType" use="required">
               <xs:annotation>
                  <xs:documentation>This policy should belong to this kind of policy group.  This attribute is used to filter choices when assigning policies to groups.</xs:documentation>
               </xs:annotation>
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="CANCELLATION"/>
                     <xs:enumeration value="PAYMENT"/>
                     <xs:enumeration value="CHANGE"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
         </xs:complexType>
         <xs:element name="PolicyName">
            <xs:annotation>
               <xs:documentation>Name and description for a policy in the language specified.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
               <xs:attribute name="Name" type="xs:string" use="required"/>
               <xs:attribute name="ShortDescription" type="xs:string" use="optional"/>
               <xs:attribute name="LongDescription" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="PolicyApplicationLevelType">
            <xs:annotation>
               <xs:documentation>A Policy is tied to a unit on the reservation, thus giving different policies for different units. However some policies will be applicable across the whole component, and this attribute indicates such policies. The ApplicationPriority attribute will be used to choose the appropriate policy in such cases.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="FareInfoLevel"/>
               <xs:enumeration value="TicketLevel"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="GuaranteeNonChangingFaresType">
            <xs:annotation>
               <xs:documentation>This value is used when companies decide that they will guarantee a portion of the existing ticket during a change, if that portion of the trip isn't being changed.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:boolean"/>
         </xs:simpleType>
         <xs:simpleType name="PolicyResidualValueApplicationType">
            <xs:annotation>
               <xs:documentation>Indicates what should happen to any Residual Value of between the tickets being changed, if any exists - 
					None - this means there is no residual value of the ticket. This is the default value.
					PenaltyOnly - this means the residual value of the ticket can be used to pay the value of the penalty fee implied. Any remaining residual value is lost.
					FullValue - this means any remaining value after Fare Difference and Penalties are covered needs to be refunded/credited.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="None"/>
               <xs:enumeration value="PenaltyOnly"/>
               <xs:enumeration value="FullValue"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="ApplicationPriorityType">
            <xs:annotation>
               <xs:documentation>For TicketLevel policies, the ApplicationPriority is used to choose from the policies tied to the FareInfos of this Ticket (or in the Reservation world, tied to Units of the ResComponent).</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:int"/>
         </xs:simpleType>
         <xs:element name="Policy">
            <xs:annotation>
               <xs:documentation>This represents one Cancel,  Change or Payment Policy.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="PolicyType">
                     <xs:sequence>
                        <xs:element ref="PolicyTypeDetails"/>
                        <xs:element maxOccurs="unbounded" ref="PolicyName"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="PolicyGroups"/>
                     </xs:sequence>
                     <xs:attributeGroup ref="PolicyModifyGroup"/>
                     <xs:attribute default="FareInfoLevel" name="ApplicationLevel" type="PolicyApplicationLevelType" use="optional"/>
                     <xs:attribute name="ApplicationPriority" type="ApplicationPriorityType" use="optional"/>
                     <xs:attribute name="ResidualValueApplication" type="PolicyResidualValueApplicationType" use="optional"/>
                     <xs:attribute default="false" name="GuaranteeNonChangingFares" type="GuaranteeNonChangingFaresType" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="PolicyRS">
            <xs:annotation>
               <xs:documentation>Policy retrieval response element</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element ref="PolicyTypeDetails"/>
                  <xs:element maxOccurs="unbounded" ref="PolicyName"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="PolicyGroups"/>
               </xs:sequence>
               <xs:attributeGroup ref="PolicyModifyGroup"/>
               <xs:attribute default="FareInfoLevel" name="ApplicationLevel" type="PolicyApplicationLevelType" use="optional"/>
               <xs:attribute name="ApplicationPriority" type="ApplicationPriorityType" use="optional"/>
               <xs:attribute name="ResidualValueApplication" type="PolicyResidualValueApplicationType" use="optional"/>
               <xs:attribute name="GuaranteeNonChangingFares" type="GuaranteeNonChangingFaresType" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="PolicyGroupPointOfSale">
            <xs:annotation>
               <xs:documentation>Links a Policy Group to a Point of Sale</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="PointOfSaleCode" type="xs:string" use="required"/>
               <xs:attribute name="EffectiveFromDate" type="xs:date" use="required"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="PolicyGroupComponentType">
            <xs:annotation>
               <xs:documentation>Links a Component Type to the Policy Group.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="ComponentTypeCode" type="xs:string" use="required"/>
            </xs:complexType>
         </xs:element>
         <xs:attributeGroup name="PolicyGroupModifyGroup">
            <xs:annotation>
               <xs:documentation>The code and version of the Policy group required to modify or delete attributes of a Policy group.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="PolicyGroupCode" type="xs:string" use="required"/>
            <xs:attribute name="PolicyGroupVersion" type="xs:nonNegativeInteger" use="required"/>
            <xs:attribute name="RestrictToProvider" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>If filled, this policy group is restricted for use by the provider code listed here.  This means the policy group can not be shared with other providers and is not shown in system level maintenance.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:element name="ChangeType">
            <xs:annotation>
               <xs:documentation>This represents Change Types associated with the Policy Group.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="ChangeTypeCode" use="required">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="AIR_ROUTING"/>
                        <xs:enumeration value="AIR_PASSENGER_NAME"/>
                        <xs:enumeration value="AIR_AIRLINE"/>
                        <xs:enumeration value="AIR_REMARKS"/>
                        <xs:enumeration value="AIR_FLIGHT"/>
                        <xs:enumeration value="AIR_TIME"/>
                        <xs:enumeration value="AIR_CABIN_CLASS"/>
                        <xs:enumeration value="AIR_SEAT_NUMBER"/>
                        <xs:enumeration value="AIR_DELIVERY_OPTION"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="ChangePolicyGroup">
            <xs:annotation>
               <xs:documentation>Items used for Change policy groups</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="ChangeType"/>
               </xs:sequence>
               <xs:attribute name="ApplicablePolicyOptionCode" use="required">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="CLOSEST IN BY TIMEFRAME"/>
                        <xs:enumeration value="ALL"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="BasisTimeOfDay" type="xs:time" use="optional"/>
               <xs:attribute name="TimeZone" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="PaymentPolicyGroup">
            <xs:annotation>
               <xs:documentation>Items used forPayment policy groups</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="DeriveDeadlineFromVendor" type="xs:boolean" use="required"/>
               <xs:attribute name="BasisTimeOfDay" type="xs:time" use="optional"/>
               <xs:attribute name="TimeZone" type="xs:string" use="optional"/>
               <xs:attribute name="UseEarliestFlag" type="xs:boolean" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="CancelPolicyGroup">
            <xs:annotation>
               <xs:documentation>Items used for Cancel policy groups</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="ApplicablePolicyOptionCode" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="CLOSEST IN BY TIMEFRAME"/>
                        <xs:enumeration value="ALL"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="BasisTimeOfDay" type="xs:time" use="optional"/>
               <xs:attribute name="TimeZone" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="PolicyGroupTypeDetails">
            <xs:annotation>
               <xs:documentation>Policy group details specific to the type of policy group. Depending on the type of policy group, the cancel/change or payment policy element is filled.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:choice>
                  <xs:element ref="CancelPolicyGroup"/>
                  <xs:element ref="ChangePolicyGroup"/>
                  <xs:element ref="PaymentPolicyGroup"/>
               </xs:choice>
               <xs:attribute name="PolicyGroupType" use="required">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="CANCELLATION"/>
                        <xs:enumeration value="CHANGE"/>
                        <xs:enumeration value="PAYMENT"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="FlagResvLevelPolicyGroup" type="xs:boolean" use="required"/>
               <xs:attribute name="PolicyGroupVersion" type="xs:nonNegativeInteger" use="required"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="PolicyGroupName">
            <xs:annotation>
               <xs:documentation>Names and Descriptions for a policy group by language identifier.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
               <xs:attribute name="Name" type="xs:string" use="required"/>
               <xs:attribute name="ShortDescription" type="xs:string" use="optional"/>
               <xs:attribute name="LongDescription" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="ConsumeCalendarLink">
            <xs:annotation>
               <xs:documentation>Links a policy group to an existing Calendar. The calendar code must already exist.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="CalendarDatesCode" type="xs:string" use="required"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="PolicyGroupCalendar">
            <xs:annotation>
               <xs:documentation>Links a policy group to a calendar. In a create usage, the DateCalendar element can be used to create a new calendar. To use an existing calendar the CalendarLink is used. (Note: for policy groups, the booking date range is not used).</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:choice>
                  <xs:element ref="ns7:DateCalendarLink"/>
                  <xs:element ref="ConsumeCalendarLink"/>
               </xs:choice>
            </xs:complexType>
         </xs:element>
         <xs:element name="PolicyGroupRS">
            <xs:annotation>
