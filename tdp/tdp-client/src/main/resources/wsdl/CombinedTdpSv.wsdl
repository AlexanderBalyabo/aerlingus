               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="RoomRatesAttributes">
                     <xs:annotation>
                        <xs:documentation>Contains attributes that relate to RoomRates element.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="SimulatedPriceBreakdown" type="xs:boolean" use="required">
                           <xs:annotation>
                              <xs:documentation>If true this attribute indicates the RoomRates/RoomRate breakdown is not a true price breakdown from the remote system but simulated from TDP. If false this attribute indicates the RoomRates/RoomRate breakdown is an accurate reflection of rate breakdown from remote system.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
