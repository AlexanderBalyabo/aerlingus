                     <xs:attribute name="AlternativeExchangeRateSource" type="xs:string" use="optional"/>
                     <xs:attribute name="AlternativeRateMarginPercentage" type="xs:decimal" use="optional"/>
                     <xs:attribute name="CreditCardNumberMasked" type="xs:boolean" use="optional">
