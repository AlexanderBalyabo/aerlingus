               <xs:complexContent>
                  <xs:extension base="ModifyRequest">
                     <xs:attribute name="LastPrintedDate" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>Date On which Invoice was created or last updated.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
