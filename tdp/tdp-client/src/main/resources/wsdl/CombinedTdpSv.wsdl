                     <xs:attribute name="Estimated" type="xs:dateTime" use="optional"/>
                     <xs:attribute name="Actual" type="xs:dateTime" use="optional"/>
                  </xs:extension>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="ArrivalDateTime">
            <xs:complexType>
               <xs:simpleContent>
