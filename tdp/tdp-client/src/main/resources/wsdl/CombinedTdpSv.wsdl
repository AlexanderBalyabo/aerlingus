      <xs:schema elementFormDefault="qualified" targetNamespace="urn:Total.types.extensions.hotel.rqrs.ota.datalex.com" xmlns="urn:Total.types.extensions.hotel.rqrs.ota.datalex.com">
         <xs:import namespace="urn:redemption.rqrs.datalex.com"/>
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05/hotel"/>
         <xs:element name="TPA_Extensions">
            <xs:annotation>
               <xs:documentation>TPA_Extension for Total to include commission</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="Commission" type="ns4:CommissionType"/>
                  <xs:element minOccurs="0" name="TotalRemoteSystemPrice">
                     <xs:annotation>
                        <xs:documentation>The total price quoted from the remote system, exclusive of promotional, rounding and other rules applied in TDP</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="RemoteSystemPrice" type="xs:decimal" use="optional"/>
                        <xs:attribute name="RemoteSystemPriceCurrencyCode" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="AveragePrice">
                     <xs:annotation>
                        <xs:documentation>The average nightly price quoted from a hotel system.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="AverageNightlyPrice" type="xs:decimal" use="required">
                           <xs:annotation>
                              <xs:documentation>The amount equivalent to the average nightly price of the hotel.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>The currency code of the average nightly price.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="NetPrice">
                     <xs:annotation>
                        <xs:documentation>The average nightly price quoted from a hotel system.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="NetPrice" type="xs:decimal" use="required">
                           <xs:annotation>
                              <xs:documentation>The cost price amount of the hotel.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>The currency code of the average nightly price.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="TotalFees">
                     <xs:annotation>
                        <xs:documentation>The total amount of fee/s charged in a room stay.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="TotalFeesAmount" type="xs:decimal" use="required">
                           <xs:annotation>
                              <xs:documentation>The amount equivalent to the sum of fee/s charged in a room stay.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>The currency code of the total amount of fee/s.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="RedemptionPointsEarned">
                     <xs:annotation>
                        <xs:documentation>An indication of the number of redemption / loyalty points earned or potentially earned for booking this room stay.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="LoyaltyPlanCode" type="xs:string" use="required">
