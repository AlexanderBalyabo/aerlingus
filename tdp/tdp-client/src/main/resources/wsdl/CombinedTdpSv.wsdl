         <xs:element name="TPA_Extensions">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element name="GuestRoom">
                     <xs:annotation>
                        <xs:documentation>TPA Extensions for guest rooms, including distribution sources/POS, TDP attributes, media content and calendar room associations.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element minOccurs="0" ref="ns7:PointOfSales"/>
                           <xs:element minOccurs="0" ref="ns7:Attributes"/>
                           <xs:element minOccurs="0" ref="ns29:MediaContentUsages"/>
                           <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns7:DateTimeCalendarLink"/>
                           <xs:element minOccurs="0" ref="ns7:RemoteSystems"/>
                        </xs:sequence>
                        <xs:attribute default="1" name="MinOccupancy" type="xs:positiveInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>Defines the minimum occupancy for a room.  This is defaulted to 1.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DefaultFlag" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>This flag indicates that this is the default guest room to be used.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DisplaySequence" type="xs:positiveInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>This sequence can be used to determine what order is used to display room types.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ProviderCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>TDP Providers are entities that supply travel products to the TDP system owner for resale</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:HotelDescriptiveInfo.types.extensions.hotel.rqrs.ota.datalex.com" xmlns="urn:HotelDescriptiveInfo.types.extensions.hotel.rqrs.ota.datalex.com">
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:import namespace="urn:media.rqrs.datalex.com"/>
         <xs:import namespace="urn:attribute.rqrs.datalex.com"/>
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05/hotel"/>
         <xs:element name="TPA_Extensions">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="Address" type="ns4:AddressType">
                     <xs:annotation>
                        <xs:documentation>The element is the standard OTA address element.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="EssentialInformationItems">
                     <xs:annotation>
                        <xs:documentation>This element holds descriptions of holtel infromation with temporal context.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="EssentialInformationItem">
                              <xs:complexType>
                                 <xs:attribute name="EffectiveDate" type="xs:dateTime" use="required"/>
                                 <xs:attribute name="ExpiresDate" type="xs:dateTime" use="required"/>
                                 <xs:attribute name="Code" type="xs:string" use="required"/>
                                 <xs:attribute name="ShortDescription" type="xs:string" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="MealPlans">
                     <xs:annotation>
                        <xs:documentation>Meal plans available at a hotel</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" minOccurs="0" name="MealPlan">
                              <xs:annotation>
                                 <xs:documentation>Info about various aspects of a meal plan supplied by a hotel</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element minOccurs="0" name="MealPlanItems">
                                       <xs:annotation>
                                          <xs:documentation>collection of all the meals provided by the meal plan</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:sequence>
                                             <xs:element maxOccurs="unbounded" minOccurs="0" name="MealPlanItem">
                                                <xs:annotation>
                                                   <xs:documentation>Info about a specific meal for the meal plan</xs:documentation>
                                                </xs:annotation>
                                                <xs:complexType>
                                                   <xs:sequence>
                                                      <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns57:AttributeAdditionalInfo"/>
                                                   </xs:sequence>
                                                   <xs:attribute name="Code" type="xs:string" use="required">
                                                      <xs:annotation>
                                                         <xs:documentation>This identifies the meal as breakfast, lunch, dinner, snacks, etc.</xs:documentation>
                                                      </xs:annotation>
                                                   </xs:attribute>
                                                   <xs:attribute name="Description" type="xs:string" use="optional">
                                                      <xs:annotation>
                                                         <xs:documentation>Further describes the type of meal, values such as buffet, a la carte, gourmet, etc.</xs:documentation>
                                                      </xs:annotation>
                                                   </xs:attribute>
                                                   <xs:attribute name="LanguageCode" type="xs:string" use="optional"/>
                                                </xs:complexType>
                                             </xs:element>
                                          </xs:sequence>
                                       </xs:complexType>
                                    </xs:element>
                                    <xs:element minOccurs="0" ref="ns7:Attributes"/>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns7:DateCalendarLink"/>
                                 </xs:sequence>
                                 <xs:attribute name="Code" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Used to identify the meal plan</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="Description" type="xs:string" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns7:PointOfSales"/>
                  <xs:element minOccurs="0" ref="ns7:RemoteSystems"/>
                  <xs:element minOccurs="0" ref="ns7:Attributes"/>
                  <xs:element minOccurs="0" ref="ns29:AdvertisementUsages"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns7:DateTimeCalendarLink"/>
                  <xs:element minOccurs="0" ref="ns7:Regions"/>
                  <xs:element minOccurs="0" ref="ns29:MediaContentUsages"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="HotelRatings">
                     <xs:annotation>
                        <xs:documentation>Used to return customer and other organization ratings for a hotel.  For example Trip Advisor ratings.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="RatingType" use="required">
                           <xs:annotation>
                              <xs:documentation>Indicates the type author rating given to the hotel.  Values are:
"CONSUMER"  -- Rating given by a people who statyed at the hotel
"SELLER" -- Rating given by the seller of the room stay like an agency or airline.
"ORGANIZATION"  -- A third party organization like national hotel rating organizations and media like Conde Nast
"PROVIDER"  -- Ranking given by the providerr of the hotel.  Either the wholesale merchant or the hotel chain or property themselves.</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="CONSUMER"/>
                                 <xs:enumeration value="SELLER"/>
                                 <xs:enumeration value="ORGANIZATION"/>
                                 <xs:enumeration value="PROVIDER"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="RatingOrganizationName" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Name of the organization like "Trip Advisor" or "Yelp"</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Rating" type="xs:float" use="required">
                           <xs:annotation>
                              <xs:documentation>Floating numeric value of the rating given to this hotel.  The rating is only relative to other rating numbers within this organization and can not be compared across organizations.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RatingMaximum" type="xs:float" use="optional">
                           <xs:annotation>
                              <xs:documentation>Indicates the maximum rating from this rating body (if any) for example: 3 out of 5 where 5 is the maximum.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ReviewCount" type="xs:integer" use="optional">
                           <xs:annotation>
                              <xs:documentation>Indicates the number of reviews given the hotel by this organization or group</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RatingURL" type="xs:anyURI" use="optional">
                           <xs:annotation>
                              <xs:documentation>A URI (URL) to locate an actual review page.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="HotelNames">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="HotelName">
                              <xs:complexType>
                                 <xs:attribute name="LanguageCode" type="xs:string" use="optional"/>
                                 <xs:attribute name="HotelName" type="xs:string" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="Providers">
                     <xs:annotation>
                        <xs:documentation>Collection of providers and how a product is identified by each one.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="Provider">
                              <xs:annotation>
                                 <xs:documentation>Code and type of the provider and the code used by the provider to identify the product.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="ProviderCode" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>3rd party vendor code</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="ProviderProductCode" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Code used by the provider to identify the product</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="ProviderType" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Type of provider: content or pricing provider.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                       <xs:restriction base="xs:string">
                                          <xs:enumeration value="CONTENT"/>
                                          <xs:enumeration value="PRICE"/>
                                       </xs:restriction>
                                    </xs:simpleType>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="DisplayPriority" type="xs:integer" use="optional">
                  <xs:annotation>
                     <xs:documentation>This is used to control the display order for a list of hotels</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ProviderCode" type="xs:string" use="optional"/>
               <xs:attribute name="ProductGroup" type="xs:string" use="optional"/>
               <xs:attribute name="ExpiresDate" type="xs:date" use="optional"/>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:social.rqrs.datalex.com" xmlns="urn:social.rqrs.datalex.com">
         <xs:simpleType name="SocialContentTypeEnum">
            <xs:restriction base="xs:string">
               <xs:enumeration value="TEXT"/>
               <xs:enumeration value="URL"/>
               <xs:enumeration value="IMAGE"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="SocialTextType">
