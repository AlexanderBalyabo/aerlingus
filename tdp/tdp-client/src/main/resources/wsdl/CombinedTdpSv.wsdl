                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AccountID" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Account Id for Payment System.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AccountPassword" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Account Password for Payment System.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PaymentDescription" type="xs:string" use="optional">
