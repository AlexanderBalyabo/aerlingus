         </xs:element>
         <xs:simpleType name="SocialMediaServiceEnum">
            <xs:restriction base="xs:string">
               <xs:enumeration value="FACEBOOK"/>
               <xs:enumeration value="TWITTER"/>
               <xs:enumeration value="MYSPACE"/>
               <xs:enumeration value="LINKEDIN"/>
               <xs:enumeration value="DIGG"/>
               <xs:enumeration value="PINTEREST"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="SocialConfiguration">
            <xs:annotation>
               <xs:documentation>Object for saving SocialConfigurations</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="SocialContentValue"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="Description"/>
                  <xs:element maxOccurs="unbounded" name="SocialMediaService">
                     <xs:complexType>
                        <xs:attribute name="SocialMediaService" type="SocialMediaServiceEnum" use="required"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="SocialConfigurationCode" type="xs:string"/>
               <xs:attribute name="Version" type="xs:nonNegativeInteger" use="optional"/>
            </xs:complexType>
         </xs:element>
      </xs:schema>
