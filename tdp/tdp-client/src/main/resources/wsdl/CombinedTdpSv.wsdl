            <xs:restriction base="xs:string">
               <xs:enumeration value="AISLE_SEAT"/>
               <xs:enumeration value="SEAT_WITH_FACILITIES_FOR_HANDICAPPED/INCAPACITATED_PASSENGER"/>
               <xs:enumeration value="SEAT_SUITABLE_FOR_ADULT_WITH_AN_INFANT"/>
               <xs:enumeration value="BULKHEAD_SEAT"/>
               <xs:enumeration value="SEAT_WITH_BASSINET_FACILITY"/>
               <xs:enumeration value="NO_SMOKING_SEAT"/>
               <xs:enumeration value="SMOKING_SEAT"/>
               <xs:enumeration value="SEAT_SUITABLE_FOR_UNACCOMPANIED_MINOR"/>
               <xs:enumeration value="WINDOW_SEAT"/>
