                                       <xs:enumeration value="FIRST_LAST_UNIT"/>
                                    </xs:restriction>
                                 </xs:simpleType>
                              </xs:attribute>
                              <xs:attribute name="AssessmentBasisUOMCode" type="xs:string" use="optional">
                                 <xs:annotation>
