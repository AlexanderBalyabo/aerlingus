                  <xs:extension base="xs:anyType"/>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:Access.types.extensions.profile.rqrs.ota.datalex.com" xmlns="urn:Access.types.extensions.profile.rqrs.ota.datalex.com">
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:complexContent mixed="false">
                  <xs:extension base="xs:anyType"/>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:AddressInfo.types.extensions.profile.rqrs.ota.datalex.com" xmlns="urn:AddressInfo.types.extensions.profile.rqrs.ota.datalex.com">
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:complexContent mixed="false">
                  <xs:extension base="xs:anyType"/>
