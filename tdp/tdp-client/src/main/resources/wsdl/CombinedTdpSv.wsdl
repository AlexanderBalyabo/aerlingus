         <xs:element name="CarSegment">
            <xs:annotation>
               <xs:documentation>Contains domain specific information for a car rental segment.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="PricedCoverage">
                     <xs:annotation>
                        <xs:documentation>Contains a list of optional and included car rental insurance coverage descriptions and prcing</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:complexContent>
                           <xs:extension base="SequencedType">
                              <xs:annotation>
                                 <xs:documentation>Free text that is provided as part of coverage (insurance) information.</xs:documentation>
                              </xs:annotation>
                              <xs:sequence>
                                 <xs:element maxOccurs="unbounded" minOccurs="0" ref="TaxFee"/>
                              </xs:sequence>
                              <xs:attribute name="CoverageCode" type="xs:string" use="required">
                                 <xs:annotation>
                                    <xs:documentation>The vendor specific insurance coverage code.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="CoverageType" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>OTA Specific type of vehicle insurance coverage.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="CoverageDescription" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">Free text that is provided as part of coverage (insurance) information.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="CoveragePrice" type="xs:decimal" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>Amount of the coverage.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="CurrencyCode" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>Currency code of the coverage amount</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute default="false" name="IncludedInRate" type="xs:boolean" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">An indication if this amount is included in the base vehicle rate, or is an additonal cost.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="TaxIncluded" type="xs:boolean" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">The indicator if Tax is already included in the price or not.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="Required" type="xs:boolean" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>If TRUE indicated that the customer must purchace this coverage.  Purchase may be made at booking time (if supported by the seller) or paid at the counter.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="MinimumCoverage" type="xs:decimal" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">The minimum amount covered.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="MinCurrencyCode" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">Currency code of the Minimum Coverage amount.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="MaximumCoverage" type="xs:decimal" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">The maximum amount covered.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="MaxCurrencyCode" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">Currency code of the Maximum Coverage amount.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                           </xs:extension>
                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="AdditionalCharge">
                     <xs:annotation>
                        <xs:documentation>Information on the other charges associated with this vehicle.  Such charges may include additional mileage amounts, fuel costs, etc.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:complexContent>
                           <xs:extension base="SequencedType">
                              <xs:sequence>
                                 <xs:element maxOccurs="unbounded" minOccurs="0" ref="TaxFee"/>
                              </xs:sequence>
                              <xs:attribute name="ChargeCode" type="xs:string" use="required">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">Industry-specific code that indicates the type of charge.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="ChargeDescription" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">Descriptive information for the additional charge.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="ChargeAmount" type="xs:decimal" use="optional"/>
                              <xs:attribute name="CurrencyCode" type="xs:string" use="optional"/>
                              <xs:attribute name="IncludedInRate" type="xs:boolean" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">An indication if this amount is included in the base vehicle rate, or is an additonal cost.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="TaxInclusive" type="xs:boolean" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">Indicates if tax is included in the charge amount.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                           </xs:extension>
                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="PickupDateTime" type="xs:dateTime" use="optional"/>
               <xs:attribute name="DropoffDateTime" type="xs:dateTime" use="optional"/>
               <xs:attribute name="DurationInBaseUOM" type="xs:integer" use="optional"/>
               <xs:attribute name="PickupLocationTypeCode" type="xs:string" use="optional"/>
               <xs:attribute name="PickupLocationCode" type="xs:string" use="optional"/>
               <xs:attribute name="PickupLocationName" type="xs:string" use="optional"/>
               <xs:attribute name="DropoffLocationTypeCode" type="xs:string" use="optional"/>
               <xs:attribute name="DropOffLocationCode" type="xs:string" use="optional"/>
               <xs:attribute name="DropOffLocationName" type="xs:string" use="optional"/>
               <xs:attribute name="ShortDescription" type="xs:string" use="optional"/>
               <xs:attribute name="LongDescription" type="xs:string" use="optional"/>
               <xs:attribute name="VendorCode" type="xs:string" use="optional"/>
               <xs:attribute name="VendorName" type="xs:string" use="optional"/>
               <xs:attribute name="HoursOfOperation-DropOff" type="xs:string" use="optional"/>
               <xs:attribute name="HoursOfOperation" type="xs:string" use="optional"/>
               <xs:attribute name="DistanceType" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Description of mileage/km charges paid directly to rental agency.  Usually in the form Unlimited, 0.20$ per mile above 200 miles, etc.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="RoundTrip" type="xs:boolean" use="optional"/>
               <xs:attribute name="AirConditionInd" type="xs:boolean" use="optional"/>
               <xs:attribute name="VehicleTransmissionType" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="AUTOMATIC"/>
                        <xs:enumeration value="MANUAL"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="VehicleCategory" type="xs:string" use="optional"/>
               <xs:attribute name="VehicleClass" type="xs:string" use="optional"/>
               <xs:attribute name="DoorCount" type="xs:positiveInteger" use="optional"/>
               <xs:attribute name="Size" type="xs:string" use="optional"/>
               <xs:attribute name="PassengerQuantity" type="xs:positiveInteger" use="optional"/>
               <xs:attribute name="BaggageQuantity" type="xs:positiveInteger" use="optional"/>
               <xs:attribute name="ModelName" type="xs:string" use="optional"/>
               <xs:attribute name="ModelCode" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="LodgingSegment">
            <xs:annotation>
               <xs:documentation>Contains domain specific information for a lodging segment.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
