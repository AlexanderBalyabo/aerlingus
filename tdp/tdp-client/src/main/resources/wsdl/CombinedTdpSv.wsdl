                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="LocationCode" type="xs:string" use="optional"/>
               <xs:attribute name="LocationName" type="xs:string" use="optional"/>
               <xs:attribute name="LocationTypeCode" type="xs:string" use="optional"/>
               <xs:attribute name="StartDateTime" type="xs:dateTime" use="optional"/>
               <xs:attribute name="EndDateTime" type="xs:dateTime" use="optional"/>
               <xs:attribute name="DurationInBaseUOM" type="xs:integer" use="optional"/>
               <xs:attribute name="LongDescription" type="xs:string" use="optional"/>
               <xs:attribute name="ShortDescription" type="xs:string" use="optional"/>
               <xs:attribute name="VendorCode" type="xs:string" use="optional"/>
               <xs:attribute name="VendorName" type="xs:string" use="optional"/>
               <xs:attribute name="PropertyCode" type="xs:string" use="optional"/>
               <xs:attribute name="Longitude" type="xs:decimal" use="optional"/>
               <xs:attribute name="Latitude" type="xs:decimal" use="optional"/>
