      <xs:schema elementFormDefault="qualified" targetNamespace="urn:FlightSegment.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:FlightSegment.types.extensions.air.rqrs.ota.datalex.com">
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:simpleType name="MealServiceType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Enumerated List (Meal Code in brackets):
					Breakfast				(B)
					Snack  					(S)
					Dinner					(D)
					Hot Meal				(H)
					Lunch					(L)
					Refreshments			(R)
					Complimentary Liquor	(C)
					Meal					(M)
					Liquor for Purchase		(P)
					Food for Purchase		(F)
					Cold Meal				(O)
					No Meal Service			(-)</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="Breakfast"/>
               <xs:enumeration value="Snack"/>
               <xs:enumeration value="Dinner"/>
               <xs:enumeration value="Hot Meal"/>
               <xs:enumeration value="Lunch"/>
               <xs:enumeration value="Refreshments"/>
               <xs:enumeration value="Complimentary Liquor"/>
               <xs:enumeration value="Meal"/>
               <xs:enumeration value="Liquor for Purchase"/>
               <xs:enumeration value="Food for Purchase"/>
               <xs:enumeration value="Cold Meal"/>
               <xs:enumeration value="No Meal Service"/>
               <xs:enumeration value="Snack for Purchase"/>
               <xs:enumeration value="Ted Snack Box"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="MealServices">
            <xs:annotation>
               <xs:documentation>Contains a list of all the meals served on this flight</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="99" name="MealService" type="MealServiceType"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="CabinCapacity" type="xs:int">
            <xs:annotation>
               <xs:documentation>Number of Actual Seats in the physical cabin.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="BookClass" type="xs:string">
            <xs:annotation>
               <xs:documentation>Class to be booked in this cabin.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MaxSellCnt" type="xs:int">
            <xs:annotation>
               <xs:documentation>Number of Seats Authorized to be sold for this cabin.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SeatWithholdCnt" type="xs:int">
            <xs:annotation>
               <xs:documentation>Number of seats to be withheld from sale
for this cabin.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="StandbyPsgrCnt" type="xs:int">
            <xs:annotation>
               <xs:documentation>Number of Non-Revenue passengers (Z/)
booked in this cabin.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="DepartureAirport">
            <xs:complexType mixed="true">
               <xs:annotation>
                  <xs:documentation>Used to inject name value pairs into elements (internal use only).</xs:documentation>
               </xs:annotation>
               <xs:attribute name="LocationCode" use="required">
                  <xs:annotation>
                     <xs:documentation>The IATA 3 letter departure airport code of the flight segment.
Example: ATL</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="8"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute default="IATA" name="CodeContext" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="8"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="RPH_Type">
            <xs:annotation>
               <xs:documentation xml:lang="en">(Reference Place Holder) - an index code to identify an instance in a collection of like items.. For example,  used to assign individual passengers or clients to particular itinerary items.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[0-9]{1,8}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="ResBookDesigStatusCodeType">
