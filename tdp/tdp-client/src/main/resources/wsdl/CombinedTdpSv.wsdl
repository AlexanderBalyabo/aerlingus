               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="AgencySequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique sequence of a reservation agency contained in this reservation.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AgentSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique sequence of an agent within an agency contained in this reservation.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CommisionAmount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Commission amount earned by this agent and agency.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CurrencyCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Currency code.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AgentRoleCode" type="AgentRoles" use="required">
                        <xs:annotation>
                           <xs:documentation>Indicated the agents role in the reservation process for this component.  The values are:

RECEIVING -- Agent that processed the remote reservation.
DOCUMENTING -- The agent who issued travel documents and tickets.
SELLING -- The agent credited with the sale.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AgentDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique agent.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="Numeric0to4">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Numeric values, from 0 to 4 inclusive</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:integer">
               <xs:minInclusive value="0"/>
               <xs:maxInclusive value="4"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="AirSegmentType">
            <xs:annotation>
               <xs:documentation>Identifies a particular type of flight - Direct, Stopover etc.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="Nonstop"/>
               <xs:enumeration value="Direct"/>
               <xs:enumeration value="Connection"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="ResComponentSearchPref">
            <xs:annotation>
               <xs:documentation>Contains the search criteria used to create this reservation component</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element minOccurs="0" name="AirSearchPref">
                           <xs:annotation>
                              <xs:documentation>Contains the search criteria used to create an air specific reservation component</xs:documentation>
                           </xs:annotation>
                           <xs:complexType>
                              <xs:complexContent>
                                 <xs:extension base="SequencedType">
                                    <xs:sequence>
                                       <xs:element minOccurs="0" name="AirSegmentPref">
                                          <xs:annotation>
                                             <xs:documentation>Origin and Destination location, and time information for the air search</xs:documentation>
                                          </xs:annotation>
                                          <xs:complexType>
                                             <xs:complexContent>
                                                <xs:extension base="SequencedType">
                                                   <xs:sequence>
                                                      <xs:element maxOccurs="unbounded" minOccurs="0" name="AirSegmentConnectPref">
                                                         <xs:annotation>
                                                            <xs:documentation>Connecting airports</xs:documentation>
                                                         </xs:annotation>
                                                         <xs:complexType>
                                                            <xs:complexContent>
                                                               <xs:extension base="SequencedType">
                                                                  <xs:attribute name="AirportCode" type="xs:string" use="optional"/>
                                                               </xs:extension>
                                                            </xs:complexContent>
                                                         </xs:complexType>
                                                      </xs:element>
                                                   </xs:sequence>
                                                   <xs:attribute name="StartAirportCode" type="xs:string" use="optional"/>
                                                   <xs:attribute name="EndAirportCode" type="xs:string" use="optional"/>
                                                   <xs:attribute name="DepartureDateTime" type="xs:dateTime" use="optional"/>
                                                   <xs:attribute name="DepartureDateTimeAfter" type="xs:dateTime" use="optional"/>
                                                   <xs:attribute name="DepartureDateTimeBefore" type="xs:dateTime" use="optional"/>
                                                   <xs:attribute name="ArrivalDateTime" type="xs:dateTime" use="optional"/>
                                                   <xs:attribute name="ArrivalDateTimeAfter" type="xs:dateTime" use="optional"/>
                                                   <xs:attribute name="ArrivalDateTimeBefore" type="xs:dateTime" use="optional"/>
                                                </xs:extension>
                                             </xs:complexContent>
                                          </xs:complexType>
                                       </xs:element>
                                       <xs:element minOccurs="0" name="AirTravelPref">
                                          <xs:annotation>
                                             <xs:documentation>The air search preference information</xs:documentation>
                                          </xs:annotation>
                                          <xs:complexType>
                                             <xs:complexContent>
                                                <xs:extension base="SequencedType">
                                                   <xs:sequence>
                                                      <xs:element maxOccurs="unbounded" minOccurs="0" name="AirCarrierPref">
                                                         <xs:annotation>
                                                            <xs:documentation>Specify vendors to include and exclude from the search</xs:documentation>
                                                         </xs:annotation>
                                                         <xs:complexType>
                                                            <xs:complexContent>
                                                               <xs:extension base="SequencedType">
                                                                  <xs:attribute name="CarrierCode" type="xs:string" use="required"/>
                                                               </xs:extension>
                                                            </xs:complexContent>
                                                         </xs:complexType>
                                                      </xs:element>
                                                      <xs:element minOccurs="0" name="AirFlightTypePref">
                                                         <xs:annotation>
                                                            <xs:documentation>The flight type preference information</xs:documentation>
                                                         </xs:annotation>
                                                         <xs:complexType>
                                                            <xs:complexContent>
                                                               <xs:extension base="SequencedType">
                                                                  <xs:attribute name="AirSegmentType" type="AirSegmentType" use="optional"/>
                                                                  <xs:attribute name="MaxConnections" type="xs:nonNegativeInteger" use="optional"/>
                                                               </xs:extension>
                                                            </xs:complexContent>
                                                         </xs:complexType>
                                                      </xs:element>
                                                      <xs:element maxOccurs="unbounded" minOccurs="0" name="AirClassOfServicePref">
                                                         <xs:complexType>
                                                            <xs:complexContent>
                                                               <xs:extension base="SequencedType">
                                                                  <xs:sequence>
                                                                     <xs:element maxOccurs="unbounded" minOccurs="0" name="AirInventoryClassPref">
                                                                        <xs:annotation>
                                                                           <xs:documentation>The service class sought</xs:documentation>
                                                                        </xs:annotation>
                                                                        <xs:complexType>
                                                                           <xs:complexContent>
                                                                              <xs:extension base="SequencedType">
                                                                                 <xs:attribute name="InventoryClass" type="xs:string" use="optional"/>
                                                                              </xs:extension>
                                                                           </xs:complexContent>
                                                                        </xs:complexType>
                                                                     </xs:element>
                                                                  </xs:sequence>
                                                                  <xs:attribute name="ClassOfServiceCode" type="xs:string" use="optional"/>
                                                               </xs:extension>
                                                            </xs:complexContent>
                                                         </xs:complexType>
                                                      </xs:element>
                                                      <xs:element minOccurs="0" name="AirFareTypePref">
                                                         <xs:annotation>
                                                            <xs:documentation>The fare type sought</xs:documentation>
                                                         </xs:annotation>
                                                         <xs:complexType>
                                                            <xs:complexContent>
                                                               <xs:extension base="SequencedType">
                                                                  <xs:attribute name="PrivateFare" type="xs:boolean" use="optional"/>
                                                                  <xs:attribute name="PublishedFare" type="xs:boolean" use="optional"/>
                                                                  <xs:attribute name="CharterFare" type="xs:boolean" use="optional"/>
                                                               </xs:extension>
                                                            </xs:complexContent>
                                                         </xs:complexType>
                                                      </xs:element>
                                                   </xs:sequence>
                                                   <xs:attribute default="false" name="SpecificFlights" type="xs:boolean" use="optional"/>
                                                   <xs:attribute default="false" name="DirectFlightsOnly" type="xs:boolean" use="optional"/>
                                                   <xs:attribute name="MaxStopsQuantity" type="Numeric0to4" use="optional">
                                                      <xs:annotation>
                                                         <xs:documentation>Request flights that have no more than the requested number of stops.</xs:documentation>
                                                      </xs:annotation>
                                                   </xs:attribute>
                                                </xs:extension>
                                             </xs:complexContent>
                                          </xs:complexType>
                                       </xs:element>
                                    </xs:sequence>
                                 </xs:extension>
                              </xs:complexContent>
                           </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="BookRemoteRef">
            <xs:annotation>
               <xs:documentation>The reservation component's unique identifier in the vendor remote system. For example RECORD_LOCATOR, XY6HI9</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="RemoteRefType">
                     <xs:attribute name="RemoteRefType" use="optional">
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="HOST_PNR_REF"/>
                              <xs:enumeration value="GDS_PNR_REF"/>
                              <xs:enumeration value="BOOKING_SYSTEM_CONFIRMATION"/>
                              <xs:enumeration value="BOOKING_SYSTEM_CANCEL_CONFIRMATION"/>
                              <xs:enumeration value="PROVIDER_CONFIRMATION"/>
                              <xs:enumeration value="PROVIDER_CANCEL_CONFIRMATION"/>
                              <xs:enumeration value="HOST_QUEUE_NUMBER"/>
                              <xs:enumeration value="HOST_PSEUDO_CITY_CODE"/>
                              <xs:enumeration value="HOST_QUEUE_CATEGORY"/>
                              <xs:enumeration value="GDS_FARE_QUOTE_REF"/>
                              <xs:enumeration value="TICKETING_FOLDER_NUMBER"/>
                              <xs:enumeration value="HOST_AAA_CITY_CODE"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="InternalAgentLink">
            <xs:annotation>
               <xs:documentation>Linkage to indicate an internal agents participation in a reservation component</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ExtraInformation"/>
                     </xs:sequence>
                     <xs:attribute name="InternalAgentSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique sequence of the reservation internal agent.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CommisionAmount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Commission amount earned by this agent.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CurrencyCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Currency code.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AgentRoleCode" type="AgentRoles" use="required">
                        <xs:annotation>
                           <xs:documentation>Indicated the agents role in the reservation process for this component.  The values are:RECEIVING -- Agent that processed the remote reservation.DOCUMENTING -- The agent who issued travel documents and tickets.SELLING -- The agent credited with the sale.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="InternalAgentDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique internal agent.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="ItinerarySegmentLink">
            <xs:annotation>
               <xs:documentation>Linkage to indicate an itinerary segments inclusion in a reservation component.  By definition all guests who participate in this reservation component share all itierary segments connected to it.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="ItinerarySegmentSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique sequence of the reservation itinerary segment this component is linked to.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ItinerarySegmentDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique itinerary segment.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SegmentType" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indicate if the segment (as applied to this component) is an ORIGIN, CONNECTION, STOPOVER.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="ORIGIN"/>
                              <xs:enumeration value="CONNECTION"/>
                              <xs:enumeration value="STOPOVER"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="DisplayOrder" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>UI Display Order of Itinerary Segment.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="ReferencedExchangedDocument">
            <xs:annotation>
               <xs:documentation>If the payment form is a document like an airline ticket, this is a pointer to that document that exists in M3.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="ReservationCode" type="xs:string" use="required">
                  <xs:annotation>
                     <xs:documentation>The unique Reservation Code which can be used to retrieve the reservation.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ReservationComponentSequence" type="xs:positiveInteger" use="optional">
                  <xs:annotation>
                     <xs:documentation>The unique sequence of the component within the reservation that owns the document to be exchanges.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="DocumentSequence" type="xs:positiveInteger" use="optional">
                  <xs:annotation>
                     <xs:documentation>The unique sequence of the document within the reservation component that is being exchanged.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="ExchangedDocumentPaymentData">
            <xs:annotation>
               <xs:documentation>If the payment data associated with the Document to be exchanged is not in M3, enter it here. This node is exclusive to ReservationDocumentSvRQ requests.  Its purpose is to facilitate remote system transactions.This node will not be persisted to the database.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="CreditCardType">
                     <xs:attribute name="FormOfPaymentTypeCode" type="FormOfPaymentTypeCodeEnum"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="ExchangedDocument">
            <xs:annotation>
               <xs:documentation>If the document is not in M3 enter it here. This node is exclusive to ReservationDocumentSvRQ requests. Its purpose is to add Documents originating outside M3 to ResComponents. This node will not be persisted to the database.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element ref="Document"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="BillingAddressType">
            <xs:attribute name="AddressLine1" type="xs:string" use="required"/>
            <xs:attribute name="AddressLine2" type="xs:string" use="optional"/>
            <xs:attribute name="AddressLine3" type="xs:string" use="optional"/>
            <xs:attribute name="CompanyName" type="xs:string" use="optional"/>
            <xs:attribute name="TimeZone" type="xs:string" use="optional"/>
            <xs:attribute name="CityCode" type="xs:string" use="optional"/>
            <xs:attribute name="CityName" type="xs:string" use="optional"/>
            <xs:attribute name="PostalCode" type="xs:string" use="optional"/>
            <xs:attribute name="StateCountyProvince" type="xs:string" use="optional"/>
            <xs:attribute name="StateCode" type="xs:string" use="optional"/>
            <xs:attribute name="CountryName" type="xs:string" use="optional"/>
            <xs:attribute name="CountryCode" type="xs:string" use="optional"/>
         </xs:complexType>
         <xs:element name="ProviderPayment">
            <xs:annotation>
               <xs:documentation>Details payments made (or to be made) to a vendor.  Typically, these are deposits or prepayments or forms of payments for tickets.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="CreditCardType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ExtraInformation"/>
                        <xs:choice minOccurs="0">
                           <xs:element ref="ReferencedExchangedDocument"/>
                           <xs:element ref="ExchangedDocument"/>
                        </xs:choice>
                        <xs:element minOccurs="0" ref="ExchangedDocumentPaymentData"/>
                        <xs:element minOccurs="0" name="BillingAddress" type="BillingAddressType">
                           <xs:annotation>
                              <xs:documentation>The billing address associated with a provider payment. Note that this is the same construct as any other reservation address except that it is non-sequenced, as there can only be one.</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                     </xs:sequence>
                     <xs:attribute name="AmountPaid" type="xs:decimal" use="required">
                        <xs:annotation>
                           <xs:documentation>Amount paid (to pay).</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CurrencyCode" type="xs:string" use="required"/>
                     <xs:attribute name="AlternativeAmountPaid" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Payment Amount in an alternative currency. Used with AlternativeCurrency and AlternativeExchangeRate to allow for a payment to be processed in a different currency from the POS. Allows for any credit card FX charges to be removed from foreign (to the POS) customers by allowing them to pay in the currency of their card.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AlternativeCurrency" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Currency for the AlternativeAmountPaid. Allows for a payment to be processed in a different currency from the POS. Allows for any credit card FX charges to be removed from foreign (to the POS) customers by allowing them to pay in the currency of their card.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AlternativeExchangeRate" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Exchange Rate for the AlternativeAmountPaid. Allows for a payment to be processed in a different currency from the POS. Allows for any credit card FX charges to be removed from foreign (to the POS) customers by allowing them to pay in the currency of their card.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FormOfPaymentTypeCode" type="FormOfPaymentTypeCodeEnum" use="optional">
                        <xs:annotation>
                           <xs:documentation>Form of payment type code valid in M3 table.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ProviderCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Provider code valid in M3 database.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="DatePaid" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>Date payment transmitted to provider.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ProviderPaymentID" type="xs:ID" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique ID referencing the payment in the provider's system.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="VoidDateTime" type="xs:dateTime" use="optional"/>
                     <xs:attribute name="VoidUser" type="xs:string" use="optional"/>
                     <xs:attribute name="VoidReason" type="xs:string" use="optional"/>
                     <xs:attribute name="EventSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>This is a read-only column that indicates the event that was raised as a result of this ProviderPayment</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PolicySequence" type="xs:positiveInteger" use="optional"/>
                     <xs:attribute name="PolicyDocRef" type="xs:IDREF" use="optional"/>
                     <xs:attribute name="TransactionID" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>A reference returned via the payment gateway which uniqely identifies a transaction</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="RemoteImage">
            <xs:annotation>
               <xs:documentation>The string image of the reservation in the remote system.  Example, the PNR image in a GDS.  This element is used to monitor for changes to the remote reservation</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="RemoteImage" type="xs:base64Binary" use="required"/>
                     <xs:attribute name="CreationDateTime" type="xs:dateTime" use="required"/>
                     <xs:attribute name="Name" type="xs:string" use="optional"/>
                     <xs:attribute name="GuestCompositionSequence" type="xs:positiveInteger" use="optional"/>
                     <xs:attribute name="GuestCompositionDocRef" type="xs:IDREF" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="Error">
            <xs:annotation>
               <xs:documentation>Represents an error condition which occurred in the context of the parent system event.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="DateTimeOccurred" type="xs:dateTime" use="required"/>
                     <xs:attribute name="Severity" type="xs:positiveInteger" use="required"/>
                     <xs:attribute name="FormattedLevel1Text" type="xs:string" use="optional"/>
                     <xs:attribute name="FormattedLevel2Text" type="xs:string" use="optional"/>
                     <xs:attribute name="MessageCode" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="EventType">
            <xs:complexContent>
               <xs:extension base="SequencedType">
                  <xs:sequence>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="RemoteRef"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="ExtraInformation"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="Error"/>
                  </xs:sequence>
                  <xs:attribute name="EventScheduledDateTime" type="xs:dateTime" use="optional">
                     <xs:annotation>
                        <xs:documentation>The date and time the event is scheduled to occur.  If this is a system tracking type record this would the time tracking began. This value should be expressed in the time zone of the M3 reservation system.  It is important to note that if a deposit date / time is being set, that the deadline from the provider be converted (if necessary) to the local system time zone.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="EventTypeCode" type="xs:string" use="required">
                     <xs:annotation>
                        <xs:documentation>Describes the type of event being scheduled.  For example, PROVIDER_DEPOSIT_DUE or RESERVATION_LOCK.  Different events are processed in different ways unique to their type.	Event Types are valid in the master table Event Type.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ResultingStatus" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The status code to be assigned to the reservation or reservation component at successful completion of the event.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ConfirmationTrackingCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Used to log the confirmation or tracking code from a remote system.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="EventEnteredDateTime" type="xs:dateTime" use="optional">
                     <xs:annotation>
                        <xs:documentation>This is the timestamp of when the event was added to the reservation or reservation component. Set by the reservation servant on insert of an event.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="EventCompletedDateTime" type="xs:dateTime" use="optional">
                     <xs:annotation>
                        <xs:documentation>This is the timestamp of when the event was completed. For system-managed events, this is set by M3 for client managed events, this is set by the client.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="LastUpdateDateTime" type="xs:dateTime" use="optional">
                     <xs:annotation>
                        <xs:documentation>This is a timestamp of when the event was last updated after insert.  Set by the reservation servant on update of an event.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Process" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Free text description of the process or program responsible for this event. Can be used to track which processes raised an event or as a further classification of event type.  It is user defined.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="NumberOfRetries" type="xs:integer" use="optional">
                     <xs:annotation>
                        <xs:documentation>Set to the number of times the client wishes the event processing to be retried upon failure.  Each failure decrements the count by 1.  At zero retries an error is thrown. Currently for client use only.  Uses 0 by default.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Source" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The system source which created this event.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Username" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The system username which created this event.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="OriginatingInternalAgentSequence" type="xs:integer" use="optional">
                     <xs:annotation>
                        <xs:documentation>The identification of the reservation agent who is assigning this event.	Client supplied.  Events can be used to assign processing from one agent to another.  This is the agent assigning the event.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="OriginatingInternalAgentDocRef" type="xs:IDREF" use="optional"/>
                  <xs:attribute name="AssignedToInternalAgentSequence" type="xs:integer" use="optional">
                     <xs:annotation>
                        <xs:documentation>The identification of the reservation  agent that has been assigned this event.	Client supplied.  Events can be used to assign processing from one agent to another.  This is the agent that has been assigned the event.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="AssignedToInternalAgentDocRef" type="xs:IDREF" use="optional"/>
                  <xs:attribute name="OriginatingEventSequence" type="xs:positiveInteger" use="optional"/>
                  <xs:attribute name="Description" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>A description or note or comment that describes the event action. Used for manual processes to communicate instructions.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="EventID" type="xs:ID" use="optional"/>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:element name="Event" type="EventType">
            <xs:annotation>
               <xs:documentation>Reservation events serve three general purposes.  They allow system and manual processes to be scheduled and tracked; they act as a history and control record for locking and updating a reservation; they act as a control record to confirm that external system processes have completed. Events may be connected to a reservation and / or to a reservation component.

For the most part, client software raises events that schedule system and manual processes.  Locking and external system tracking events are managed by the Reservations Servants.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AddToResComponent">
            <xs:annotation>
               <xs:documentation>Adds sub-elements to a reservation component.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="AddSubRequest">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Event"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ExtraInformation"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="RemoteImage"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="BookRemoteRef"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Policy"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="AgentLink"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="InternalAgentLink"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ItinerarySegmentLink"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="SpecialRequest"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Remark"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ProviderPayment"/>
                        <xs:element minOccurs="0" ref="ResComponentSearchPref"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="AirSupplementaryElement"/>
                     </xs:sequence>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="MediaContentLink">
            <xs:annotation>
               <xs:documentation>Linkage to indicate media content inclusion in a reservation component.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="MediaContentSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Unique sequence of the media content this component is linked to.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="MediaContentDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Client supplied reference to the unique media content.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="AttributeTypeLink">
            <xs:annotation>
               <xs:documentation>Linkage to indicate attribute inclusion in a reservation component.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="AttributeTypeSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Unique sequence of the attribute type this component is linked to.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AttributeTypeDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Client supplied reference to the unique attribute.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="ActivityInfo">
            <xs:annotation>
               <xs:documentation>Contains guest specific activity itinerary information.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="ConfirmNumber" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="CruiseInfo">
            <xs:annotation>
               <xs:documentation>Contains guest specific cruise itinerary information</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="BerthNumber" type="xs:string" use="optional"/>
               <xs:attribute name="DiningPreference" type="xs:string" use="optional"/>
               <xs:attribute name="TableSizePreference" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="FormOfIdentificationLink">
            <xs:annotation>
               <xs:documentation>Links this guest composition element to a reservation guest form of identification used for this reservation component.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="RemoteRef"/>
                     </xs:sequence>
                     <xs:attribute name="FormOfIdentificationSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique sequence of the reservation guest form of identification.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FormOfIdentificationDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique reservation guest form of identification.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SyncStatus" type="RemoteSynchStatusType" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indication of the synchronization status of the referenced information with the vendor remote system.  The values are:PENDING_ADD ??? the information is in the M3 database, but not transmitted to the vendor remote system.PENDING_REMOVE ??? the information is slated from removal from the vendor remote system, but the action has not yet been performed.  The client should suppress this information from display to the user.SYNCHED ??? the information on the vendor remote system is the same as in the M3 database.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="AirInfo">
            <xs:annotation>
               <xs:documentation>Contains guest/segment level information specific to this segment of an itinerary such as seat numbers, fare codes, segment status, etc.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="SeatPreference1" type="SeatCharacteristicType" use="optional">
                  <xs:annotation>
                     <xs:documentation>First generic seat preference to be used to reserve seats for the GuestCompostion passenger. This indicates a required seat characteristic.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="SeatPreference2" type="SeatCharacteristicType" use="optional">
                  <xs:annotation>
                     <xs:documentation>Second generic seat preference to be used to reserve seats for the GuestCompostion passenger. This indicates a required seat characteristic. This attribute should be used if two seat characteristics are needed to specify the required seat type.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="SeatPreference3" type="SeatCharacteristicType" use="optional">
                  <xs:annotation>
                     <xs:documentation>Second generic seat preference to be used to reserve seats for the GuestCompostion passenger. This indicates a required seat characteristic. This attribute should be used if three seat characteristics are needed to specify the required seat type.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="SeatNumber" type="xs:string" use="optional"/>
               <xs:attribute name="MarketingClassificationCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>The marketing classification given to the seat such as Stretch Seat, or United Economy Plus</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TicketNumber" type="xs:string" use="optional"/>
               <xs:attribute name="SeatAssignmentStatus" type="SeatAssignmentStatusType" use="optional"/>
               <xs:attribute name="SeatSynchStatus" type="RemoteSynchStatusType" use="optional"/>
               <xs:attribute name="FareBasisCode" type="xs:string" use="optional"/>
               <xs:attribute name="InventoryClass" type="xs:string" use="optional"/>
               <xs:attribute name="ClassOfService" type="xs:string" use="optional"/>
               <xs:attribute name="CodeShareClassOfService" type="xs:string" use="optional"/>
               <xs:attribute name="BaggageAllowancePieces" type="xs:nonNegativeInteger" use="optional"/>
               <xs:attribute name="BaggageAllowanceWeight" type="xs:decimal" use="optional"/>
               <xs:attribute name="BaggageAllowanceWeightInPounds" type="xs:boolean" use="optional"/>
               <xs:attribute name="ChangeOfGaugeSeatPreference1" type="SeatCharacteristicType" use="optional">
                  <xs:annotation>
                     <xs:documentation>First generic change of gauge seat preference to be used to reserve seats for the GuestCompostion passenger. This indicates a required seat characteristic.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ChangeOfGaugeSeatPreference2" type="SeatCharacteristicType" use="optional">
                  <xs:annotation>
                     <xs:documentation>Second generic change of gauge seat preference to be used to reserve seats for the GuestCompostion passenger. This indicates a required seat characteristic. This attribute should be used if two seat characteristics are needed to specify the required change of gauge seat type.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ChangeOfGaugeSeatPreference3" type="SeatCharacteristicType" use="optional">
                  <xs:annotation>
                     <xs:documentation>Third generic change of gauge seat preference to be used to reserve seats for the GuestCompostion passenger. This indicates a required seat characteristic. This attribute should be used if three seat characteristics are needed to specify the required change of gauge seat type.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ChangeOfGaugeSeatNumber" type="xs:string" use="optional"/>
               <xs:attribute name="ChangeOfGaugeSeatAssignmentStatus" type="SeatAssignmentStatusType" use="optional"/>
               <xs:attribute name="ChangeOfGaugeSeatSynchStatus" type="RemoteSynchStatusType" use="optional"/>
               <xs:attribute name="ChangeOfGaugeFirstLegClassOfService" type="xs:string" use="optional"/>
               <xs:attribute name="ChangeOfGaugeSecondLegClassOfService" type="xs:string" use="optional"/>
               <xs:attribute name="HostSeatNumber" type="xs:string" use="optional"/>
               <xs:attribute name="HostChangeOfGaugeSeatNumber" type="xs:string" use="optional"/>
               <xs:attribute name="CodeTicketTracing" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Used for tracing for payment.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="OldSeatNumber" type="xs:string" use="optional"/>
               <xs:attribute name="ChangeOfGauge" type="SeatCharacteristicType" use="optional">
                  <xs:annotation>
                     <xs:documentation>Change of gauge. Not implemented yet</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="NotValidBefore" type="xs:date" use="optional"/>
               <xs:attribute name="NotValidAfter" type="xs:date" use="optional"/>
               <xs:attribute name="OldMarketingClassificationCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>The old marketing classification given to the seat such as Stretch Seat, or United Economy Plus</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="OverrideSeatFlag" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>This attribute is use to determine the latest seat assignment done by the user.  For example: RT 5A outbound, 4A inbound; changed outbound seat to 23A; changed again to 5A</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="GroundTransportTransportationInfo">
            <xs:annotation>
               <xs:documentation>Contains guest specific transportation itinerary information.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="ConfirmNumber" type="xs:string" use="optional"/>
               <xs:attribute name="IsLead" type="xs:boolean"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="RailInfo">
            <xs:annotation>
               <xs:documentation>Contains guest specific rail itinerary information.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="SeatNumber" type="xs:string" use="optional"/>
               <xs:attribute name="SeatPreference" type="xs:string" use="optional"/>
               <xs:attribute name="TicketNumber" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="CarInfo">
            <xs:annotation>
               <xs:documentation>Contains guest specific car itinerary information.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="ConfirmNumber" type="xs:string" use="optional"/>
               <xs:attribute name="IsDriver" type="xs:boolean" use="optional"/>
               <xs:attribute name="CountryOfCitizenship" use="optional">
                  <xs:annotation>
                     <xs:documentation>2 character ISO code of the country of citizenship of the driver.</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:maxLength value="2"/>
                        <xs:minLength value="2"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="LoyaltyPlanInfoLink">
            <xs:annotation>
               <xs:documentation>Links this guest composition element to a reservation guest loyalty plan used for this reservation component.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="RemoteRef"/>
                     </xs:sequence>
                     <xs:attribute name="LoyaltyPlanInfoDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique reservation guest loyalty plan.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="LoyaltyPlanInfoSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Pointer to a unique reservation guest loyalty plan element.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SyncStatus" type="RemoteSynchStatusType" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indication of the synchronization status of the referenced information with the vendor remote system.  The values are:PENDING_ADD ??? the information is in the M3 database, but not transmitted to the vendor remote system.PENDING_REMOVE ??? the information is slated from removal from the vendor remote system, but the action has not yet been performed.  The client should suppress this information from display to the user.SYNCHED ??? the information on the vendor remote system is the same as in the M3 database.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="UnitLink">
            <xs:annotation>
               <xs:documentation>Links this reservation guest itinerary sequence to a reservation component unit.  Used to link guest air segments to a fare basis.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="UnitSequence" type="xs:positiveInteger" use="optional">
                  <xs:annotation>
                     <xs:documentation>Pointer to unique reservation component unit.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="UnitDocRef" type="xs:IDREF" use="optional">
                  <xs:annotation>
                     <xs:documentation>Client supplied reference to the unique reservation component unit.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="LodgingInfo">
            <xs:annotation>
               <xs:documentation>Contains guest specific lodging itinerary information.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="ConfirmNumber" type="xs:string" use="optional"/>
               <xs:attribute name="RoomNumber" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="GuestItinerarySegment">
            <xs:annotation>
               <xs:documentation>Container of domain specific segment information for a guest and linkage to reservation itinerary segment.  Contains guest specific info only such as seat and class assignments</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:choice minOccurs="0">
                           <xs:element ref="AirInfo"/>
                           <xs:element ref="CarInfo"/>
                           <xs:element ref="LodgingInfo"/>
                           <xs:element ref="ActivityInfo"/>
                           <xs:element ref="GroundTransportTransportationInfo"/>
                           <xs:element ref="RailInfo"/>
                           <xs:element ref="CruiseInfo"/>
                        </xs:choice>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="FormOfIdentificationLink"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="LoyaltyPlanInfoLink"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="SpecialRequest"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Remark"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="AirSupplementaryElement"/>
                        <xs:element minOccurs="0" ref="UnitLink"/>
                     </xs:sequence>
                     <xs:attribute name="ItinerarySegmentSequence" type="xs:integer" use="optional">
                        <xs:annotation>
                           <xs:documentation>Points to Reservation Itinerary Segment Sequence number which is unique for the reservation.  Links guest to the itinerary segment so that she shares all attributes of the itinerary segment.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="Status" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Status of this guest on this itinerary segment in the remote system.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="Smoking" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>If TRUE, this is a smoking segment.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ItinerarySegmentDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique reservation itinerary segment.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="GuestItinerarySegmentDocID" type="xs:ID" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique ID supplied by the client when adding a guest itinerary segment and sub-elements in the same request.  The sub-elements will reference this ID in order to relate the two in the database insert transaction.  When adding subsequent sub-elements the client only need reference the sequence of the guest itinerary segment.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="RemoteSystemFareQuotePTC">
            <xs:annotation>
               <xs:documentation>This multiplexed element will store the remote fare quote system PTC and the remote system will be the code context.  The remote system PTC code(s) should copied to the reservation and subsequently used for re-price and ticketing operations.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="ContextCode" type="xs:string" use="required"/>
                     <xs:attribute name="PassengerTypeCode" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="GuestComposition">
            <xs:annotation>
               <xs:documentation>Linkage to indicate a guest's  participation in a reservation component.  Acts as a placeholder to indicate guest type and position in a reservation component before guests are added to the reservation.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Price"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="GuestItinerarySegment"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="RemoteRef"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="RemoteSystemFareQuotePTC"/>
                     </xs:sequence>
                     <xs:attribute name="IncrementalGuestNumber" type="xs:integer" use="required">
                        <xs:annotation>
                           <xs:documentation>Defines the position of the guest within her guest type.  For example Adult 3.  This is used for incremental guest pricing for a component.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AgeAtFirstConsume" type="xs:integer" use="optional">
                        <xs:annotation>
                           <xs:documentation>Age at the date of first consumption.  If guest type code  or date of birth was not used.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="GuestTypeCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>Guest type code.  If age or date of birth were not used.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="QuotedPassengerTypeCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The passenger type code of the fare that was returned in the fare quote.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="DateOfBirth" type="xs:date" use="optional"/>
                     <xs:attribute name="OverrideFirstName" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name as sent (or to be sent) to the remote reservation system.  For example reservation guest name is Joe Smith.  Air record name needs to be Joseph Smith to match passport.

Also used to process name changes subsequent to initial booking.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideMiddleName" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name as sent (or to be sent) to the remote reservation system.  For example reservation guest name is Joe Smith.  Air record name needs to be Joseph Smith to match passport.

Also used to process name changes subsequent to initial booking.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideLastName" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name as sent (or to be sent) to the remote reservation system.  For example reservation guest name is Joe Smith.  Air record name needs to be Joseph Smith to match passport.

Also used to process name changes subsequent to initial booking.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideCompleteName" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name as sent (or to be sent) to the remote reservation system.  For example reservation guest name is Joe Smith.  Air record name needs to be Joseph Smith to match passport.

Also used to process name changes subsequent to initial booking.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideTitle" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name as sent (or to be sent) to the remote reservation system.  For example reservation guest name is Joe Smith.  Air record name needs to be Joseph Smith to match passport.

Also used to process name changes subsequent to initial booking.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideSalutation" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name as sent (or to be sent) to the remote reservation system.  For example reservation guest name is Joe Smith.  Air record name needs to be Joseph Smith to match passport.

Also used to process name changes subsequent to initial booking.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideNameSuffix" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name as sent (or to be sent) to the remote reservation system.  For example reservation guest name is Joe Smith.  Air record name needs to be Joseph Smith to match passport.

Also used to process name changes subsequent to initial booking.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideStartConsumeDateTime" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>If this guest starts consumption on a different date / time than other guests, this is his start date / time. Used for split-stay logic.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SyncStatus" type="RemoteSynchStatusType" use="optional"/>
                     <xs:attribute name="OverridePhoneNumber" type="xs:string" use="optional"/>
                     <xs:attribute name="PhoneNumberSyncStatus" type="RemoteSynchStatusType" use="optional"/>
                     <xs:attribute name="OverrideEndConsumeDateTime" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>If this guest ends consumption on a different date / time than other guests, this is his end date / time. Used for split-stay logic.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="GuestSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique sequence of the reservation guest.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="GuestDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique reservation guest.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="GuestCompositionID" type="xs:ID" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique ID supplied by the client when adding a reservation guest composition element and sub-elements in the same request.  The sub-elements will reference this ID in order to relate the two in the database insert transaction.  When adding subsequent sub-elements the client only need reference the sequence of the reservation guest composition element.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="HostFirstName" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name as sent (or to be sent) to the remote reservation system.  For example reservation guest name is Joe Smith.  Air record name needs to be Joseph Smith to match passport.

Also used to process name changes subsequent to initial booking.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="HostLastName" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name as sent (or to be sent) to the remote reservation system.  For example reservation guest name is Joe Smith.  Air record name needs to be Joseph Smith to match passport.

Also used to process name changes subsequent to initial booking.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="HostTitle" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name as sent (or to be sent) to the remote reservation system.  For example reservation guest name is Joe Smith.  Air record name needs to be Joseph Smith to match passport.

Also used to process name changes subsequent to initial booking.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="ResComponentType">
            <xs:complexContent>
               <xs:extension base="SequencedType">
                  <xs:sequence>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="Event"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="ExtraInformation"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="BookRemoteRef"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="AgentLink"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="InternalAgentLink"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="ItinerarySegmentLink"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="TaxFee"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="Policy"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="SellingPromotion"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="Unit"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="GuestComposition"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="RemoteImage"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="SpecialRequest"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="Remark"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="ProviderPayment"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="Document"/>
                     <xs:element minOccurs="0" ref="ResComponentSearchPref"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="AirSupplementaryElement"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="MediaContentLink"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="AttributeTypeLink"/>
                  </xs:sequence>
                  <xs:attribute name="Quantity" type="xs:integer" use="required">
                     <xs:annotation>
                        <xs:documentation>The number of components with the same pricing, policies, itineraries reserved.  This value is usually 1 unless identical components are to be purchased.  Multiple components that are not identical should be represented by two reservation components.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="BookingMethod" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The adaptor interface used to book this component with the vendor's remote reservation system.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="SourceOfBusinessCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>A user definable code indicating the Source of Business eg: sales channel, or partner.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="SellingComponentCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Selling Component Code valid in the M3 database.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="CostComponentCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Cost Component Code valid in the M3 database.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ProductCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Product Code valid in the M3 database.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="SourceCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Source code valid in the M3 database.  This is the source that initially inserted this reservation component.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ComponentTypeCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Component Type Code valid in the M3 database.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ComponentSubTypeCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Component Sub Type Code valid in the M3 database.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ProductShortName" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Product short name.  For M3 products, this is copied from the corresponding product element and attribute.  For non-M3 products this is user-definable.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ProductLongName" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Product long name.  For M3 products, this is copied from the corresponding product element and attribute.  For non-M3 products this is user-definable.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ProductShortDescription" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Product short description.  For M3 products, this is copied from the corresponding product element and attribute.  For non-M3 products this is user-definable.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ProductLongDescription" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Product long description.  For M3 products, this is copied from the corresponding product element and attribute.  For non-M3 products this is user-definable.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ComponentShortName" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Component short name.  For M3 products, this is copied from the corresponding product element and attribute.  For non-M3 products this is user-definable.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ComponentLongName" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Component long name.  For M3 products, this is copied from the corresponding product element and attribute.  For non-M3 products this is user-definable.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ComponentShortDescription" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Component short description.  For M3 products, this is copied from the corresponding product element and attribute.  For non-M3 products this is user-definable.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ComponentLongDescription" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Component long description.  For M3 products, this is copied from the corresponding product element and attribute.  For non-M3 products this is user-definable.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Status" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>User definable reservation component status.  No M3 calculation or processing is based on this status.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="InternalStatus" type="InternalStatusType" use="optional">
                     <xs:annotation>
                        <xs:documentation>M3 system status for this reservation component.  This should not be altered by the client.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ResComponentDocID" type="xs:ID" use="optional">
                     <xs:annotation>
                        <xs:documentation>Unique ID supplied by the client when adding a reservation component and sub-elements in the same request.  The sub-elements will reference this ID in order to relate the two in the database insert transaction.  When adding subsequent sub-elements the client only need reference the sequence of the reservation component.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="CreateDateTime" type="xs:dateTime" use="optional">
                     <xs:annotation>
                        <xs:documentation>The time that this ResComponent was added to the Reservation.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="LastUpdatedDateTime" type="xs:dateTime" use="optional">
                     <xs:annotation>
                        <xs:documentation>The time that this Component was last updated.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="InventorySystem" type="xs:string" use="optional"/>
                  <xs:attribute name="ProviderCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>A provider code which is a TDP system code representing the owner of the inventory represented by the ResComponent.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="AncillaryAirComponentCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>A unique code representing an Ancillary Air Component. This value is non-null if this component came from a stand alone (outside of fare family) ancillary air component sale.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="AncillaryAirComponentSequence" type="xs:positiveInteger" use="optional"/>
                  <xs:attribute name="AncillaryAirComponentDocRef" type="xs:IDREF" use="optional"/>
                  <xs:attribute name="Touchpoint" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The application/sales channel that created this res component.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="SessionKey" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The browser session key captured when this component was created.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="TransferFromResCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The code of the reservation that the res component is transferred from</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="TransferFromResComponentSequence" type="xs:positiveInteger" use="optional"/>
                  <xs:attribute name="TransferToResCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The code of the reservation that the res component has been transferred to</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="TransferToResComponentSequence" type="xs:positiveInteger" use="optional"/>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:element name="ResComponent">
            <xs:annotation>
               <xs:documentation>Contains pricing, policies, taxes, events, remote reservation IDs, and linkages to itinerary segments, guests, customers, and promotions for each chargeable unit of a reservation.  For example, an air ticket, a hotel room, etc.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="ResComponentType">
                     <xs:attribute name="ProductProviderCode" type="xs:string" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="AddResComponent">
            <xs:annotation>
               <xs:documentation>Adds a reservation component and all sub-elements.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="AddRequest">
                     <xs:sequence>
                        <xs:element ref="ResComponent"/>
                     </xs:sequence>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="ResComponentRQ">
            <xs:annotation>
               <xs:documentation>Contains one or more requests to add, add to, remove, remove from, or modify reservation components.  These operations are only valid for non-booked components.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="UpdateRQBase">
                     <xs:choice>
                        <xs:element ref="AddResComponent"/>
                        <xs:element ref="AddToResComponent"/>
                        <xs:element ref="RemoveFromResComponent"/>
                        <xs:element ref="RemoveResComponent"/>
                        <xs:element ref="ModifyResComponent"/>
                        <xs:element ref="AddToGuestItinerarySegment"/>
                     </xs:choice>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="RemoveGuestRelation" type="RemoveRequest">
            <xs:annotation>
               <xs:documentation>Remove a guest relationship</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="GuestRelation">
            <xs:annotation>
               <xs:documentation>Linkage to indicate a relationship between two guests eg: Parent and Infant etc...</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="PrimaryGuestItinSegmentSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Link to the primary Guest in this relation.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PrimaryGuestItinSegmentDocRef" type="xs:IDREF" use="optional"/>
                     <xs:attribute name="PrimaryResComponentSequence" type="xs:positiveInteger" use="optional"/>
                     <xs:attribute name="PrimaryGuestCompositionSequence" type="xs:positiveInteger" use="optional"/>
                     <xs:attribute name="SecondaryGuestItinSegmentSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Link to the secondary Guest in this relation.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SecondaryGuestItinSegmentDocRef" type="xs:IDREF" use="optional"/>
                     <xs:attribute name="SecondaryResComponentSequence" type="xs:positiveInteger" use="optional"/>
                     <xs:attribute name="SecondaryGuestCompositionSequence" type="xs:positiveInteger" use="optional"/>
                     <xs:attribute name="TypeCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>Code indicating the type of relationship between the two guests.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SyncStatus" type="RemoteSynchStatusType" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="AddGuestRelation">
            <xs:annotation>
               <xs:documentation>Add a guest relationship</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="AddRequest">
                     <xs:sequence>
                        <xs:element ref="GuestRelation"/>
                     </xs:sequence>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="GuestRelationRQ">
            <xs:annotation>
               <xs:documentation>Contains one or more requests to add or remove a guest relationship.  Guest relationships indicate such things as parent-infant relationships.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="UpdateRQBase">
                     <xs:choice>
                        <xs:element ref="AddGuestRelation"/>
                        <xs:element ref="RemoveGuestRelation"/>
                     </xs:choice>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="AncillaryAirComponent">
            <xs:annotation>
               <xs:documentation>Each AncillaryAirCompoment elements represents one benefit.  Benefits are things such as Change Policy, Cancel Policy, Mileage Earned, Seat Selection Policy, Check-in Policies.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="AncillaryAirComponentDocID" type="xs:ID" use="optional"/>
                     <xs:attribute name="AncillaryAirComponentCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>A unique code representing an Ancillary Air Component.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AncillaryAirComponentName" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>This is a human readable name of an Ancillary Air Component.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AncillaryAirComponentDescription" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>A description relating to an AncillaryAirComponent.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ItineraryChangeAction" use="optional">
                        <xs:annotation>
                           <xs:documentation>Determines the allowable actions for changing an itinerary with this Ancillary Component.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="PROHIBIT">
                                 <xs:annotation>
                                    <xs:documentation>Indicates that change functionality is not allowed on any itinerary with this ancillary item.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="PROTECT">
                                 <xs:annotation>
                                    <xs:documentation>Indicates that this ancillary item should be maintained across any change to the itinerary.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="DROP">
                                 <xs:annotation>
                                    <xs:documentation>Indicates that this ancillary item will be dropped when changing an itinerary.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="Mandatory" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>Denotes whether or not this Ancillary Air Component is mandatory, or if the user can decide for themself whether or not they want the component.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="URL" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>This is a link to an external resource which should contain further information relating to an Ancillary Air Component.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="FareFamily">
