                     <xs:documentation>Warning to highlight issue while performing reservation transfer</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
               <xs:enumeration value="INCOMPLETE_REMOTE_RESERVATION_RECORD">
                  <xs:annotation>
                     <xs:documentation>No Fare or valid ticket in PNR</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
