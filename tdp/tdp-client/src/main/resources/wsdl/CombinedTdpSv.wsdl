               </xs:attribute>
               <xs:attribute name="EmbarkLocationType" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>If using M3 valid locations, this is the location type for example SeaPort.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="EmbarkLocationCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>When using M3 locations, this is the location code, for example the sea port code.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="EmbarkLocationDescription" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>When not using M3 locations, this is the string description of the port.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="DebarkDateTime" type="xs:dateTime" use="required">
                  <xs:annotation>
                     <xs:documentation>Ship's date of disembark</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="DebarkLocationType" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>If using M3 valid locations, this is the location type for example SeaPort.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="DebarkLocationCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>When using M3 locations, this is the location code, for example the sea port code.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="DebarkLocationDescription" type="xs:string" use="optional">
                  <xs:annotation>
