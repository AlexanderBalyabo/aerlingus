                        <xs:sequence>
                           <xs:element minOccurs="0" name="Request" type="xs:string"/>
                           <xs:element minOccurs="0" name="Response" type="xs:string"/>
                        </xs:sequence>
                        <xs:attribute name="TransactionNumber" type="xs:int" use="required"/>
                        <xs:attribute name="StartTime" type="xs:string" use="required"/>
                        <xs:attribute name="StopTime" type="xs:string" use="required"/>
                        <xs:attribute name="ConnectionInfo" type="xs:string" use="optional"/>
                        <xs:attribute name="SessionKey" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:OTA_AirLowFareSearchRQ.extensions.air.rqrs.ota.datalex.com" xmlns="urn:OTA_AirLowFareSearchRQ.extensions.air.rqrs.ota.datalex.com">
         <xs:simpleType name="SpecialCodeTypes">
            <xs:restriction base="xs:string">
               <xs:enumeration value="ECERT"/>
               <xs:enumeration value="TSF"/>
               <xs:enumeration value="BCC"/>
               <xs:enumeration value="UPGRADE"/>
               <xs:enumeration value="CVA"/>
               <xs:enumeration value="DISCENG"/>
               <xs:enumeration value="COUPON"/>
               <xs:enumeration value="PACKAGE"/>
               <xs:enumeration value="RESIDENT_DISC">
                  <xs:annotation>
                     <xs:documentation>Used for Spanish airlines/routes.  Residents of off-shore Spanish territories
						are entitled to discounts when flying domestic routes to/from these territories.</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
               <xs:enumeration value="FAMILY_DISC">
                  <xs:annotation>
                     <xs:documentation>Used for Spanish airlines/routes.  Large families are entitled to discounts of fares when 
						flying domestic routes.</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
               <xs:enumeration value="CERTIFICATE"/>
               <xs:enumeration value="INTERNALPROMOTIONCODE"/>
