            <xs:annotation>
               <xs:documentation>Modify specific fields within a Lodging Segment.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="RoomNumber" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="RailInfoMod">
            <xs:annotation>
               <xs:documentation>Modify specific fields within a Rail Segment</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="SeatNumber" type="xs:string" use="optional"/>
               <xs:attribute name="SeatPreference" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
