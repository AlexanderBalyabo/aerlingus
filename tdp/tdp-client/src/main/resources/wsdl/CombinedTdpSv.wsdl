                                                      <xs:enumeration value="NOSHOW"/>
                                                      <xs:enumeration value="RENTALCONDITION"/>
                                                   </xs:restriction>
                                                </xs:simpleType>
                                             </xs:attribute>
                                          </xs:extension>
                                       </xs:complexContent>
                                    </xs:complexType>
                                 </xs:element>
                                 <xs:element minOccurs="0" ref="ns341:TPA_Extension"/>
                              </xs:sequence>
                              <xs:attributeGroup ref="ns342:PenaltyDetail">
                                 <xs:annotation>
                                    <xs:documentation>@deprecated</xs:documentation>
                                 </xs:annotation>
                              </xs:attributeGroup>
                           </xs:extension>
                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
