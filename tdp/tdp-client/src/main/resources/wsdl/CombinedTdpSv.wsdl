      <xs:schema elementFormDefault="qualified" targetNamespace="urn:BasicPropertyInfo.types.extensions.hotel.rqrs.ota.datalex.com" xmlns="urn:BasicPropertyInfo.types.extensions.hotel.rqrs.ota.datalex.com">
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05/hotel"/>
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:element name="TPA_Extension">
            <xs:complexType>
               <xs:sequence minOccurs="0">
                  <xs:element minOccurs="0" name="PropertyId" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Reference ID when cross referencing properties with third party backend systems. To be deprecated soon.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="VendorId" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Reference ID for vendor id.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns7:Attributes">
                     <xs:annotation>
                        <xs:documentation>An array of attributes used for search and filtering operations. These can include Hotel Amenities, Room Amentities, Physically Challenged Features, Classification Categories, and other hotel features.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="HotelRatings">
                     <xs:annotation>
                        <xs:documentation>Used to return customer and other organization ratings for a hotel.  For example Trip Advisor ratings.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="RatingType" use="required">
                           <xs:annotation>
                              <xs:documentation>Indicates the type author rating given to the hotel.  Values are:
"CONSUMER"  -- Rating given by a people who statyed at the hotel
"SELLER" -- Rating given by the seller of the room stay like an agency or airline.
"ORGANIZATION"  -- A third party organization like national hotel rating organizations and media like Conde Nast
"PROVIDER"  -- Ranking given by the provider of the hotel.  Either the wholesale merchant or the hotel chain or property themselves.</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="CONSUMER"/>
                                 <xs:enumeration value="SELLER"/>
                                 <xs:enumeration value="ORGANIZATION"/>
                                 <xs:enumeration value="PROVIDER"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="RatingOrganizationName" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Name of the organization like "Trip Advisor" or "Yelp"</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Rating" type="xs:float" use="required">
                           <xs:annotation>
                              <xs:documentation>Floating numeric value of the rating given to this hotel.  The rating is only relative to other rating numbers within this organization and can not be compared across organizations.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RatingMaximum" type="xs:float" use="optional">
                           <xs:annotation>
                              <xs:documentation>Indicates the maximum rating from this rating body (if any) for example: 3 out of 5 where 5 is the maximum.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ReviewCount" type="xs:integer" use="optional">
                           <xs:annotation>
                              <xs:documentation>Indicates the number of reviews given the hotel by this organization or group</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RatingURL" type="xs:anyURI" use="optional">
                           <xs:annotation>
                              <xs:documentation>A URI (URL) to locate an actual review page.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="MultimediaObjects" type="ns4:MultimediaObjectsType">
                     <xs:annotation>
                        <xs:documentation>Multimedia objects necessary for display on a list response only.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="HotelNames">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="HotelName">
                              <xs:complexType>
                                 <xs:attribute name="LanguageCode" type="xs:string" use="optional"/>
                                 <xs:attribute name="HotelName" type="xs:string" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns7:ProviderProducts"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:policy.types.extensions.hotel.rqrs.ota.datalex.com" xmlns="urn:policy.types.extensions.hotel.rqrs.ota.datalex.com">
