         <xs:element name="ItinerarySegment">
            <xs:annotation>
               <xs:documentation>Container for all information related to a single segment in an itinerary.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:choice>
                           <xs:element ref="AirSegment"/>
                           <xs:element ref="CarSegment"/>
                           <xs:element ref="LodgingSegment"/>
                           <xs:element ref="ActivitySegment"/>
                           <xs:element ref="ParkingSegment"/>
                           <xs:element ref="RailSegment"/>
                           <xs:element ref="GroundTransportationSegment"/>
                           <xs:element ref="CruiseSegment"/>
                           <xs:element ref="InsuranceSegment"/>
                        </xs:choice>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ExtraInformation"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Address"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Phone"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Email"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="SpecialRequest"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Remark"/>
                     </xs:sequence>
                     <xs:attribute name="StartDateTime" type="xs:dateTime" use="required">
                        <xs:annotation>
                           <xs:documentation>Start date and time of this segment.</xs:documentation>
                        </xs:annotation>
