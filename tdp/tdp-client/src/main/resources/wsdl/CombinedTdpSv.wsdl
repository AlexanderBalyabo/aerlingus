                                       <xs:sequence>
                                          <xs:element name="Description" type="ParagraphType"/>
                                       </xs:sequence>
                                       <xs:attribute name="PetsPolicyCode" type="OTA_CodeType" use="optional">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">Use OTA Code Table Pet Policy Code list (PET).</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                       <xs:attributeGroup ref="UnitsOfMeasureGroup">
                                          <xs:annotation>
                                             <xs:documentation xml:lang="en">This is used to designate the maximum pet weight and unit of weight measure (e.g., pounds or kilograms).</xs:documentation>
                                          </xs:annotation>
                                       </xs:attributeGroup>
                                       <xs:attribute name="MaxPetQuantity" type="Numeric0to99" use="optional">
                                          <xs:annotation>
