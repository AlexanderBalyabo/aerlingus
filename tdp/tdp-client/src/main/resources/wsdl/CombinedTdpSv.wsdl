                                                </xs:annotation>
                                             </xs:element>
                                          </xs:sequence>
                                          <xs:attribute name="ParagraphTitle" type="xs:string" use="optional"/>
                                          <xs:attribute name="ParagraphNumber" type="xs:int" use="optional"/>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                                 <xs:attribute name="SubTitle" use="required">
                                    <xs:simpleType>
                                       <xs:restriction base="xs:string"/>
                                    </xs:simpleType>
                                 </xs:attribute>
                                 <xs:attribute name="SubCode" use="optional">
                                    <xs:simpleType>
                                       <xs:restriction base="xs:string">
                                          <xs:minLength value="1"/>
                                          <xs:maxLength value="8"/>
                                       </xs:restriction>
                                    </xs:simpleType>
                                 </xs:attribute>
                                 <xs:attribute name="SubSectionNumber" use="optional">
                                    <xs:simpleType>
                                       <xs:restriction base="xs:int">
