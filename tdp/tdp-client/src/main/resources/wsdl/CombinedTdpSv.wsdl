         <xs:simpleType name="DefaultWelcomePageType">
            <xs:annotation>
               <xs:documentation>Default Welcome Page type.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="ADMIN_WELCOME_PAGE"/>
               <xs:enumeration value="AGENT_WELCOME_PAGE"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="Money">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for amounts, max 3 decimals</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:decimal">
               <xs:fractionDigits value="3"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="AccountingStatementType">
            <xs:annotation>
               <xs:documentation>Accounting statement for an agent.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="YTDSales" type="Money"/>
            <xs:attribute name="MTDSales" type="Money"/>
            <xs:attribute name="YTDCommissions" type="Money"/>
            <xs:attribute name="MTDCommissions" type="Money"/>
            <xs:attribute name="CurrencyCode" type="xs:string"/>
         </xs:complexType>
         <xs:element name="AgentAccountingStatement" type="AccountingStatementType">
            <xs:annotation>
               <xs:documentation>Contains the Agent total sales and commissions.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:attributeGroup name="AgentModifyGroup">
            <xs:annotation>
               <xs:documentation>Attributes used to modify an agent</xs:documentation>
            </xs:annotation>
            <xs:attribute name="Sequence" type="xs:nonNegativeInteger" use="optional"/>
            <xs:attribute name="AgentIdentifier" type="xs:string" use="optional"/>
         </xs:attributeGroup>
         <xs:attributeGroup name="AgentAttributes">
            <xs:annotation>
               <xs:documentation>Attributes used to identify an agent</xs:documentation>
            </xs:annotation>
            <xs:attribute name="NamePrefix" type="xs:string" use="optional"/>
            <xs:attribute name="FirstName" type="xs:string" use="optional"/>
            <xs:attribute name="MiddleInitial" type="xs:string" use="optional"/>
            <xs:attribute name="LastName" type="xs:string" use="optional"/>
            <xs:attribute name="NameSuffix" type="xs:string" use="optional"/>
         </xs:attributeGroup>
         <xs:simpleType name="AgentStatusType">
            <xs:annotation>
               <xs:documentation>Agent status type.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="ACTIVE"/>
               <xs:enumeration value="INACTIVE"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="AgencyAgentType">
            <xs:annotation>
               <xs:documentation>Agent information.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element minOccurs="0" ref="ns7:Phones"/>
               <xs:element minOccurs="0" ref="ns7:Emails"/>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns7:ExtraInformation"/>
               <xs:element minOccurs="0" ref="ns7:Addresses"/>
               <xs:element minOccurs="0" ref="ns49:User"/>
               <xs:element minOccurs="0" ref="AgentAccountingStatement"/>
            </xs:sequence>
            <xs:attribute name="AgentCode" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>This is the internal unique ID for the Agent. Agent Identifier is the code for the Agent provided by the Agency.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Status" type="AgentStatusType">
               <xs:annotation>
                  <xs:documentation>Travel Agent status.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DefaultWelcomePage" type="DefaultWelcomePageType" use="optional">
               <xs:annotation>
                  <xs:documentation>It determines the Welcome page for the Agent when the agent logs in.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attributeGroup ref="AgentModifyGroup"/>
            <xs:attributeGroup ref="AgentAttributes"/>
         </xs:complexType>
         <xs:element name="AgencyAgent" type="AgencyAgentType">
            <xs:annotation>
               <xs:documentation>Contains all information related to a travel agent within a reservation agency.  The agent is uniquely identified by sequence or Agent Identifier.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:simpleType name="AgencyStatusType">
            <xs:annotation>
               <xs:documentation>Status of the Agency</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="ENROLLMENT_PENDING"/>
               <xs:enumeration value="ACTIVE"/>
               <xs:enumeration value="INACTIVE"/>
               <xs:enumeration value="SUSPENDED"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:attributeGroup name="AgencyAttributesGroup">
            <xs:annotation>
               <xs:documentation>Attributes associated with an agency</xs:documentation>
            </xs:annotation>
            <xs:attribute name="AgencyCode" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency code.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="AgencyName" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency name.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="IATANumber" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency IATA number.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="TaxIdentifier" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency tax identifier.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="AgencyType" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency type</xs:documentation>
               </xs:annotation>
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="RETAIL"/>
                     <xs:enumeration value="CONSORTIUM"/>
                     <xs:enumeration value="AGENCY_MAIN_OFFICE"/>
                     <xs:enumeration value="AGENCY_BRANCH_OFFICE"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="AgencyVersion" type="xs:nonNegativeInteger" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency version.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ParentAgencyCode" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Parent agency code.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Status" type="AgencyStatusType" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency status.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="TaxCountryCode" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency tax country code.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="TaxCountryName" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency tax country name</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="TaxStateProvinceCode" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency tax state province code.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="TaxStateProvinceName" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Agency tax province name.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="LastSaveDate" type="xs:date" use="optional">
               <xs:annotation>
                  <xs:documentation>Last save date.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:element name="Provider">
            <xs:annotation>
               <xs:documentation>Represents a Provider associated with the agency.  The name is only used for display purposes.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="ProviderCode" type="xs:string" use="required"/>
               <xs:attribute name="ProviderName" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="AccessesType">
            <xs:sequence>
               <xs:element maxOccurs="unbounded" name="Access">
                  <xs:complexType>
                     <xs:attribute name="Username" type="xs:string"/>
                     <xs:attribute name="DateTime" type="xs:dateTime"/>
                     <xs:attribute name="ActionType">
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="CREATE"/>
                              <xs:enumeration value="UPDATE"/>
                              <xs:enumeration value="DELETE"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
         </xs:complexType>
         <xs:element name="Accesses" type="AccessesType">
            <xs:annotation>
               <xs:documentation>Describes the actions taken on the record.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AgencyAccountingStatement">
            <xs:annotation>
               <xs:documentation>Contains the overall Agency and Branch total sales and commissions.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="BranchYTDSales" type="Money"/>
               <xs:attribute name="BranchMTDSales" type="Money"/>
               <xs:attribute name="BranchYTDCommissions" type="Money"/>
               <xs:attribute name="BranchMTDCommissions" type="Money"/>
               <xs:attribute name="OverallAgencyYTDSales" type="Money"/>
               <xs:attribute name="OverallAgencyMTDSales" type="Money"/>
               <xs:attribute name="OverallAgencyYTDCommissions" type="Money"/>
               <xs:attribute name="OverallAgencyMTDCommissions" type="Money"/>
               <xs:attribute name="CurrencyCode" type="xs:string"/>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="PointOfSalesType">
            <xs:annotation>
               <xs:documentation>Assigned points of sale.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="PointOfSaleCode" type="xs:string" use="required"/>
            <xs:attribute name="PointOfSaleName" type="xs:string"/>
            <xs:attribute name="DefaultPointOfSale" type="xs:boolean"/>
         </xs:complexType>
         <xs:element name="Consortium">
            <xs:annotation>
               <xs:documentation>Represents a Consortium associated with the agency. The name is only used for display purposes.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="ConsortiumCode" type="xs:string" use="required"/>
               <xs:attribute name="ConsortiumName" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="CommissionPlan">
            <xs:annotation>
               <xs:documentation>Commission plan assigned to the aggency. The name is used for display only; ignored in a create or update request.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="CommissionPlanCode" type="xs:string" use="required"/>
               <xs:attribute name="CommissionPlanName" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="FareDiscountType">
            <xs:annotation>
               <xs:documentation>Fare Discount Type.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="Sequence" type="xs:int"/>
            <xs:attribute name="PromotionCode" type="xs:string"/>
         </xs:complexType>
         <xs:element name="FareDiscounts" type="FareDiscountType">
            <xs:annotation>
               <xs:documentation>These are the Promotion codes available for the Agency.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:complexType name="IssuerNameType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Name of bank or organization issuing the card (e.g., alumni association, bank, fraternal organization, etc.).</xs:documentation>
            </xs:annotation>
            <xs:attribute name="BankID" type="xs:string">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Code of bank issuing the card.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:complexType>
         <xs:simpleType name="MMYYDate">
            <xs:annotation>
               <xs:documentation xml:lang="en">Month and year information.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="(0[1-9]|1[0-2])[0-9][0-9]"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:attributeGroup name="PaymentCardDateGroup">
            <xs:attribute name="EffectiveDate" type="MMYYDate" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Indicates the starting date.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ExpireDate" type="MMYYDate" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Indicates the ending date.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:simpleType name="ISO3166">
            <xs:annotation>
               <xs:documentation xml:lang="en">2 character country code as defined in ISO3166.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[a-zA-Z]{2}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="CountryNameType">
            <xs:annotation>
               <xs:documentation xml:lang="en">The name or code of a country (e.g. as used in an address or to specify citizenship of a traveller).</xs:documentation>
            </xs:annotation>
            <xs:simpleContent>
               <xs:extension base="xs:string">
                  <xs:attribute name="Code" type="ISO3166" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">ISO 3166 code for a country.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:extension>
            </xs:simpleContent>
         </xs:complexType>
         <xs:complexType name="StateProvType">
            <xs:annotation>
               <xs:documentation xml:lang="en">State, province, or region name or code needed to identify location.</xs:documentation>
            </xs:annotation>
            <xs:simpleContent>
               <xs:extension base="xs:string">
                  <xs:attribute name="StateCode">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">The postal service standard code or abbreviation for the state, province, or region.</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:minLength value="2"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
               </xs:extension>
            </xs:simpleContent>
         </xs:complexType>
         <xs:complexType name="StreetNmbrType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Street name; number on street.</xs:documentation>
            </xs:annotation>
            <xs:simpleContent>
               <xs:extension base="xs:string">
                  <xs:attribute name="PO_Box" type="xs:string">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Defines a Post Office Box number.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:extension>
            </xs:simpleContent>
         </xs:complexType>
         <xs:attributeGroup name="FormattedInd">
            <xs:annotation>
               <xs:documentation xml:lang="en">Identifies if the associated data is formatted into its individual pieces, or exists as a single entity.</xs:documentation>
            </xs:annotation>
            <xs:attribute default="false" name="FormattedInd" type="xs:boolean" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Specifies if the associated data is formatted or not. If true, then it is formatted, if false, then not formatted.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:attributeGroup name="PrivacyGroup">
            <xs:annotation>
               <xs:documentation xml:lang="en">The two Privacy attributes allow for control of the sharing of data between parties.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="ShareSynchInd" use="optional">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:documentation xml:lang="en">value="Inherit" Permission for sharing data for synchronization of information held by other travel service providers.</xs:documentation>
                  </xs:annotation>
                  <xs:restriction base="xs:NMTOKEN">
                     <xs:enumeration value="Yes"/>
                     <xs:enumeration value="No"/>
                     <xs:enumeration value="Inherit"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="ShareMarketInd" use="optional">
               <xs:simpleType>
                  <xs:annotation>
                     <xs:documentation xml:lang="en">value="Inherit" Permission for sharing data for marketing purposes.</xs:documentation>
                  </xs:annotation>
                  <xs:restriction base="xs:NMTOKEN">
                     <xs:enumeration value="Yes"/>
                     <xs:enumeration value="No"/>
                     <xs:enumeration value="Inherit"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:complexType name="AddressType">
            <xs:sequence>
               <xs:element minOccurs="0" name="StreetNmbr" type="StreetNmbrType">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Street Name and Number within the address</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="BldgRoom" type="xs:string">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Building name, room, apartment, or suite number.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="5" minOccurs="0" name="AddressLine" type="xs:string"/>
               <xs:element minOccurs="0" name="CityName" type="xs:string">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">City name eg. Dublin</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="PostalCode" type="xs:string">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Post Office Code number.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="County" type="xs:string">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">County Name eg. Fairfax</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="StateProv" type="StateProvType">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">State name eg. Texas</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="CountryName" type="CountryNameType">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Country name eg. Ireland</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
            <xs:attributeGroup ref="FormattedInd"/>
            <xs:attributeGroup ref="PrivacyGroup"/>
            <xs:attribute name="Type" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Defines the type of address (e.g. home, business, other). Refer to OTA Code List Communication Location Type (CLT).</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:complexType>
         <xs:complexType name="PaymentCardType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Identification about a specific credit card</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element minOccurs="0" name="CardHolderName" type="xs:string"/>
               <xs:element minOccurs="0" name="CardIssuerName" type="IssuerNameType"/>
               <xs:element minOccurs="0" name="Address" type="AddressType"/>
            </xs:sequence>
            <xs:attributeGroup ref="PrivacyGroup"/>
            <xs:attribute name="CardType" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Indicates the type of magnetic striped card. Refer to OTA Code ListCard Type (CDT).</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="CardCode" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Code indicating the type of credit card. Refer to OTA Code List Card Type(CDT).</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="CardNumber" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Credit card number embossed on the card.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="SeriesCode" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Verification digits printed on the card following the embossed number. This may also accommodate the customer identification/batch number (CID), card verification value (CVV2 ), card validation code number (CVC2) on credit card.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attributeGroup ref="PaymentCardDateGroup">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Date the card becomes valid for use (optional) and the date the card expires (required) in ISO 8601 prescribed format.</xs:documentation>
               </xs:annotation>
            </xs:attributeGroup>
         </xs:complexType>
         <xs:element name="AccountingDetails">
            <xs:complexType>
               <xs:attribute name="CreditLimit" type="xs:decimal"/>
               <xs:attribute name="ChargesToDate" type="xs:decimal"/>
               <xs:attribute name="TotalRemaining" type="xs:decimal"/>
               <xs:attribute name="CurrencyCode" type="xs:string"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="AccountsReceivable">
            <xs:complexType>
               <xs:sequence>
                  <xs:element minOccurs="0" ref="AccountingDetails"/>
               </xs:sequence>
               <xs:attribute name="AccountReceivableNumber" type="xs:string"/>
               <xs:attribute name="AccountName" type="xs:string"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="BSPPayment">
            <xs:complexType>
               <xs:sequence>
                  <xs:element minOccurs="0" ref="AccountingDetails"/>
               </xs:sequence>
               <xs:attribute name="IATAOrTIDSNumber" type="xs:string"/>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="AgencyFormsOfPaymentType">
            <xs:annotation>
               <xs:documentation>Agency Forms of Payment Type.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element minOccurs="0" ref="AccountsReceivable"/>
               <xs:element minOccurs="0" ref="BSPPayment"/>
               <xs:element minOccurs="0" name="UATPPayment" type="PaymentCardType"/>
               <xs:element minOccurs="0" name="AgencyCreditCard" type="PaymentCardType"/>
            </xs:sequence>
            <xs:attribute name="AcceptCustomerFOP" type="xs:boolean" use="required"/>
            <xs:attribute name="AcceptAccountsReceivable" type="xs:boolean" use="required"/>
            <xs:attribute name="AcceptBSPPayment" type="xs:boolean" use="required"/>
            <xs:attribute name="AcceptUATPCard" type="xs:boolean" use="required"/>
            <xs:attribute name="AcceptAgencyCreditCard" type="xs:boolean" use="required"/>
         </xs:complexType>
         <xs:element name="AgencyFormsOfPayment" type="AgencyFormsOfPaymentType">
            <xs:annotation>
               <xs:documentation>Forms of payment applicable for agency.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:complexType name="AgencyUISettingsType">
            <xs:annotation>
               <xs:documentation>UI settings for this agency.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element minOccurs="0" name="AgencyLogo">
                  <xs:complexType>
                     <xs:attribute name="URL" type="xs:string"/>
                  </xs:complexType>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="MediaOutlet">
                  <xs:complexType>
                     <xs:attribute name="MediaOutletCode" type="xs:string"/>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
         </xs:complexType>
         <xs:element name="AgencyUISettings" type="AgencyUISettingsType">
            <xs:annotation>
               <xs:documentation>UI settings for this agency.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:complexType name="AgencyType">
            <xs:sequence>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="AgencyAgent"/>
               <xs:element minOccurs="0" ref="ns7:Addresses"/>
               <xs:element minOccurs="0" ref="ns7:Phones"/>
               <xs:element minOccurs="0" ref="ns7:Emails"/>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns7:RemoteRef"/>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns7:ExtraInformation"/>
               <xs:element minOccurs="0" ref="Provider"/>
               <xs:element minOccurs="0" ref="Consortium"/>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="CommissionPlan"/>
               <xs:element maxOccurs="unbounded" minOccurs="0" ref="FareDiscounts"/>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="PointOfSales" type="PointOfSalesType">
                  <xs:annotation>
                     <xs:documentation>List of Point of Sales for this agency.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" ref="AgencyFormsOfPayment">
                  <xs:annotation>
                     <xs:documentation>Acceptable forms of payment for the agency.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" ref="AgencyAccountingStatement"/>
               <xs:element minOccurs="0" ref="AgencyUISettings"/>
               <xs:element minOccurs="0" ref="Accesses">
                  <xs:annotation>
                     <xs:documentation>Describes the actions taken on that version of the record.</xs:documentation>
                  </xs:annotation>
