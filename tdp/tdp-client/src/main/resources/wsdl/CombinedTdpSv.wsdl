                              <xs:attribute name="BonusMiles" type="xs:integer" use="optional"/>
                              <xs:attribute name="DiscountPercentage" type="xs:decimal" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>The actual percentage to apply with this special code.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                           </xs:extension>
                        </xs:simpleContent>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:TravelPreferences.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:TravelPreferences.types.extensions.air.rqrs.ota.datalex.com">
         <xs:simpleType name="StringLength1to8">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 8</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="8"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="FlightTypeIndicators">
                     <xs:annotation>
                        <xs:documentation>Used to specify that we really non-stop flights only, as MaxStopsQuantity is relative.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="NonStopFlightsOnly" type="xs:boolean" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="PreventReverseCodeShare">
                     <xs:annotation>
                        <xs:documentation>Used to prevent a primary carrier from being the operational carrier if they are not also the marketing carrier.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="CodeShareCarrier" type="StringLength1to8" use="required"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="MarketingCabin">
                     <xs:annotation>
                        <xs:documentation>This is Used for specifying a complex Cabin Class not defined withing Cabin Preferences.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Cabin" type="xs:string" use="required"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:TravelerInfoSummary.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:TravelerInfoSummary.types.extensions.air.rqrs.ota.datalex.com">
         <xs:import namespace="urn:common.air.rqrs.datalex.com"/>
         <xs:simpleType name="RPH_Type">
            <xs:annotation>
               <xs:documentation xml:lang="en">(Reference Place Holder) - an index code to identify an instance in a collection of like items.. For example,  used to assign individual passengers or clients to particular itinerary items.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[0-9]{1,8}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="StringLength1to8">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 8</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="8"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="Money">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for amounts, max 3 decimals</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:decimal">
               <xs:fractionDigits value="3"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" ref="ns20:OriginatingTicketDesignators"/>
                  <xs:element minOccurs="0" name="FaringCarriers">
                     <xs:annotation>
                        <xs:documentation>If present, only show fares that have been filed by these carriers.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="FaringCarrier" type="StringLength1to8"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="PublicAndPrivateFares">
                     <xs:complexType>
                        <xs:attribute name="PublicFares" type="xs:boolean" use="required">
                           <xs:annotation>
                              <xs:documentation>When set to true this indicates that the faring system is for published fares on a GDS</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="PrivateFares" type="xs:boolean" use="required">
                           <xs:annotation>
                              <xs:documentation>When set to true this indicates that the faring system is for private fares on a GDS</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute default="true" name="NonGDSFares" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>When set to true this indicates that the faring system is a non-GDS system.  Default value is true</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Cat25Fares" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>Category 25 (Net Fares) which are a subset of GDS privates fares are required.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="RedemptionFares">
                     <xs:annotation>
                        <xs:documentation>This element conatins attributes relevant to the quoting of redemption/award fares.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="RedemptionFaresOnly" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>When set to true this indicates that the client is requesting the faring system for redemption fare processing only.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="MoneyAndMiles" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>When set to true this indicates that the client is requesting the faring system for miles and money amount.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="Discount">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="10" minOccurs="0" name="ApplicablePassengerType" type="xs:string"/>
                        </xs:sequence>
                        <xs:attribute name="DiscountType" use="required">
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="FlatRate"/>
                                 <xs:enumeration value="Percentage"/>
                                 <xs:enumeration value="Amount"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="DiscountValue" type="Money" use="required"/>
                        <xs:attribute name="DiscountCurrency" type="StringLength1to8" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="PricePoint">
                     <xs:annotation>
                        <xs:documentation>Specifies the maximum and minimum fare the user is willing to accept. Either or both may be specified. The unit of currency is assumed to be that of the POS Selling City.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Minimum" type="Money" use="optional"/>
                        <xs:attribute name="Maximum" type="Money" use="optional"/>
                        <xs:attribute default="USD" name="Currency" type="StringLength1to8" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="NegotiatedFareSupplementalInfo">
                     <xs:annotation>
                        <xs:documentation>Additional custom properties needed to retrieve private fares based on the pricing system (Only used internally)</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Name" type="xs:string" use="required"/>
                        <xs:attribute name="Value" type="xs:string" use="required"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="OverridingPassengerAssociation">
                     <xs:annotation>
                        <xs:documentation>This optional passenger association element has been added to support STA 1V. It should be used to override the PTC passenger types of a Private Net Fares response when building the SHELLRQ to BIFA.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="PassengerTypeCode" type="xs:string" use="required"/>
                        <xs:attribute name="ContextCode" type="xs:string" use="optional"/>
                        <xs:attribute name="PassengerReference" type="RPH_Type" use="optional"/>
                        <xs:attribute name="PassengerTypeMinAge" type="xs:int" use="optional"/>
                        <xs:attribute name="PassengerTypeMaxAge" type="xs:int" use="optional"/>
                        <xs:attribute name="PassengerTypeAge" type="xs:int" use="optional"/>
                        <xs:attribute name="QualifiedPassengerType" type="xs:boolean" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="RepriceWithBookingClass" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>If 'true' this element indicates a pricing handler should return a list of all fares avail for the booking class not just the cheapest. Used in conjunction with PriceRequestInformation@Reprice=false.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:AirTravelerAvail.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:AirTravelerAvail.types.extensions.air.rqrs.ota.datalex.com">
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:complexContent mixed="false">
                  <xs:extension base="xs:anyType"/>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:Address.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:Address.types.extensions.air.rqrs.ota.datalex.com">
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:complexContent mixed="false">
                  <xs:extension base="xs:anyType"/>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:PersonName.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:PersonName.types.extensions.air.rqrs.ota.datalex.com">
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="HostReferences">
                     <xs:complexType>
                        <xs:attribute name="AbsNameNum" type="xs:integer" use="optional">
                           <xs:annotation>
                              <xs:documentation>Host absolute name number reference.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="LNameNum" type="xs:integer" use="optional">
                           <xs:annotation>
                              <xs:documentation>Host last name number reference</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="FNameNum" type="xs:integer" use="optional">
                           <xs:annotation>
                              <xs:documentation>Host first name number reference</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:CustLoyalty.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:CustLoyalty.types.extensions.air.rqrs.ota.datalex.com">
         <xs:simpleType name="OperationType">
            <xs:restriction base="xs:string">
               <xs:enumeration value="ADD"/>
               <xs:enumeration value="REMOVE"/>
               <xs:enumeration value="CHANGE"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>A TPA Extension that specifies a qualitative keyword relating to the type of operation that is to be performed on the loyalty program. For instance, remove the given loyalty card from the PNR or add the given loyalty card to the PNR</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="Operation" type="OperationType">
                     <xs:annotation>
                        <xs:documentation>Contains a list of all actions to be performed on a loyalty program "ADD", "REMOVE","CHANGE"</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="MilesBalance" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>This simple element conatins  the loyalty members air miles balance. This miles balance can be used in the redeeming of award flights.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="MoneyToMilesOverride">
                     <xs:annotation>
                        <xs:documentation>The MoneyToMilesRatio dictates the ratio of money to mile, for example a value of 100 means 100 miles for every unit of money</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="MoneyToMilesRatio" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>The MoneyToMilesRatio dictates the ratio of money to mile, for example a value of 100 means 100 miles for every unit of money</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AwardCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is the award code associated with this redemption miles amount. This is informational.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:AirTraveler.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:AirTraveler.types.extensions.air.rqrs.ota.datalex.com">
         <xs:simpleType name="StringLength1">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="1"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:attributeGroup name="TravelDocMultiPaxAttributes">
            <xs:annotation>
               <xs:documentation xml:lang="en">attributes realted to name used on the travel doc</xs:documentation>
            </xs:annotation>
            <xs:attribute name="MultiPaxIndicator" use="optional">
               <xs:simpleType>
                  <xs:restriction base="StringLength1">
                     <xs:maxLength value="1"/>
                     <xs:enumeration value="H"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="InfantIndicator" use="optional">
               <xs:simpleType>
                  <xs:restriction base="StringLength1">
                     <xs:maxLength value="1"/>
                     <xs:enumeration value="I"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:simpleType name="StringLength1to35">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 35</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="35"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="StringLength1to64">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 64</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="64"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="StringLength1to3">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 3</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="3"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:attributeGroup name="TravelDocIssueAttributes">
            <xs:annotation>
               <xs:documentation xml:lang="en">attributes related to travel doc issuing</xs:documentation>
            </xs:annotation>
            <xs:attribute name="DocIssueCountry" type="StringLength1to3" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Indicates the location where the document was issued.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DocIssueLocation" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Indicates the location where the document was issued.</xs:documentation>
               </xs:annotation>
               <xs:simpleType>
                  <xs:restriction base="StringLength1to35">
                     <xs:maxLength value="35"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="DocIssueAuthority" type="StringLength1to64" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Indicates the group or association that granted the document</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DocIssueDate" type="xs:date" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Indicates the group or association that granted the document</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DocIssueExpiry" type="xs:date" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Indicates the group or association that granted the document</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DocApplicableCountry" type="StringLength1to3" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Indicates the country to which a travel doc applies to</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:simpleType name="StringLength0to64">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 2</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="0"/>
               <xs:maxLength value="64"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="CountryNameType">
            <xs:annotation>
               <xs:documentation xml:lang="en">The name or code of a country (e.g. as used in an address or to specify citizenship of a traveller).</xs:documentation>
            </xs:annotation>
            <xs:simpleContent>
               <xs:extension base="StringLength0to64">
                  <xs:attribute name="Code" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">ISO 3166 code for a country.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:extension>
            </xs:simpleContent>
         </xs:complexType>
         <xs:complexType name="StateProvType">
            <xs:annotation>
               <xs:documentation xml:lang="en">State, province, or region name or code needed to identify location.</xs:documentation>
            </xs:annotation>
            <xs:simpleContent>
               <xs:extension base="StringLength0to64">
                  <xs:attribute name="StateCode" type="xs:string">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">The postal service standard code or abbreviation for the state, province, or region.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:extension>
            </xs:simpleContent>
         </xs:complexType>
         <xs:complexType name="DestinationAddressType">
            <xs:sequence>
               <xs:element maxOccurs="5" minOccurs="0" name="AddressLine" type="xs:string"/>
               <xs:element minOccurs="0" name="CityName" type="xs:string">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">City name eg. Dublin</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="PostalCode" type="xs:string">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Post Office Code number.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="StateProv" type="StateProvType">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">State name eg. Texas</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="CountryName" type="CountryNameType">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Country name eg. Ireland</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:complexType>
         <xs:simpleType name="StringLength1to30">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 30</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="30"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:attributeGroup name="TravelDocNameAttributes">
            <xs:annotation>
               <xs:documentation xml:lang="en">attributes related to name used on the travel doc</xs:documentation>
            </xs:annotation>
            <xs:attribute name="DocHolderFirstName" type="StringLength1to30" use="optional"/>
            <xs:attribute name="DocHolderMiddleName" type="StringLength1to30" use="optional"/>
            <xs:attribute name="DocHolderLastName" type="StringLength1to30" use="optional"/>
         </xs:attributeGroup>
         <xs:attributeGroup name="AirTravelerBirthAttributes">
            <xs:annotation>
               <xs:documentation xml:lang="en">attributes related to birth date and place</xs:documentation>
            </xs:annotation>
            <xs:attribute name="BirthDate" type="xs:date" use="optional"/>
            <xs:attribute name="BirthPlace" type="StringLength1to35" use="optional"/>
         </xs:attributeGroup>
         <xs:attributeGroup name="AirTravelerGenderGroup">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used to indicate the gender of a person, if known.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="Gender" use="optional">
               <xs:simpleType>
                  <xs:restriction base="xs:NMTOKEN">
                     <xs:enumeration value="Male"/>
                     <xs:enumeration value="Female"/>
                     <xs:enumeration value="Unknown"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:simpleType name="StringLength1to25">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 25</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="25"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="AirTravelerSuplementalInfo">
                     <xs:complexType>
                        <xs:attribute name="TicketNumber" type="xs:string" use="optional"/>
                        <xs:attribute name="Age" type="xs:positiveInteger" use="optional"/>
                        <xs:attribute name="RequiredMilesForAward" type="xs:int" use="optional"/>
                        <xs:attribute name="EligibilityID" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is a simple String attribute for traveler supplementary Eligibility Identification (i.e. ISIC card).</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="AirTravelerAdvancedData">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element minOccurs="0" name="TravelDocuments">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element maxOccurs="unbounded" name="TravelDocument">
                                       <xs:complexType>
                                          <xs:attribute name="DocType">
                                             <xs:annotation>
                                                <xs:documentation xml:lang="en">Indicates the type of document (e.g. Secure Flight Data, Passport, US Alien Registration Card, aso... Visa, Redress Number, Known Traveler).</xs:documentation>
                                             </xs:annotation>
                                             <xs:simpleType>
                                                <xs:restriction base="StringLength1">
                                                   <xs:enumeration value="V"/>
                                                   <xs:enumeration value="R"/>
                                                   <xs:enumeration value="K"/>
                                                   <xs:enumeration value="A"/>
                                                   <xs:enumeration value="C"/>
                                                   <xs:enumeration value="F"/>
                                                   <xs:enumeration value="I"/>
                                                   <xs:enumeration value="P"/>
                                                   <xs:enumeration value="S"/>
                                                </xs:restriction>
                                             </xs:simpleType>
                                          </xs:attribute>
                                          <xs:attribute name="DocID" use="optional">
                                             <xs:annotation>
                                                <xs:documentation xml:lang="en">Unique number assigned by authorities to document.</xs:documentation>
                                             </xs:annotation>
                                             <xs:simpleType>
                                                <xs:restriction base="StringLength1to25">
                                                   <xs:maxLength value="25"/>
                                                </xs:restriction>
                                             </xs:simpleType>
                                          </xs:attribute>
                                          <xs:attributeGroup ref="TravelDocIssueAttributes"/>
                                          <xs:attributeGroup ref="TravelDocNameAttributes"/>
                                          <xs:attributeGroup ref="TravelDocMultiPaxAttributes"/>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element minOccurs="0" name="DestinationAddresses">
                              <xs:annotation>
                                 <xs:documentation>Container of destination addresses used for secure flight SSR DOCA types.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element maxOccurs="unbounded" name="DestinationAddress">
                                       <xs:annotation>
                                          <xs:documentation>Destination address used for secure flight SSR DOCA types.</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:complexContent>
                                             <xs:extension base="DestinationAddressType">
                                                <xs:attribute name="AppliesToAllTravelers" type="xs:boolean">
                                                   <xs:annotation>
                                                      <xs:documentation>Flag indicating that the Destination Address is the same for all travelers in the reservation.</xs:documentation>
                                                   </xs:annotation>
                                                </xs:attribute>
                                             </xs:extension>
                                          </xs:complexContent>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="Nationality" type="StringLength1to3" use="optional"/>
                        <xs:attribute name="CountryOfResidence" type="StringLength1to3" use="optional">
                           <xs:annotation>
                              <xs:documentation>The Country that the traveler lives in.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attributeGroup ref="AirTravelerGenderGroup"/>
                        <xs:attributeGroup ref="AirTravelerBirthAttributes"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:SpecificFlightInfo.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:SpecificFlightInfo.types.extensions.air.rqrs.ota.datalex.com">
         <xs:simpleType name="StringLength1to8">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 8</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="8"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="StringLength1to16">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 16</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="16"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="UpperCaseAlphaLength1to3">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for an Alpha String, length 3 (for letter codes)</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[A-Z]{1,3}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="RPH_Type">
            <xs:annotation>
               <xs:documentation xml:lang="en">(Reference Place Holder) - an index code to identify an instance in a collection of like items.. For example,  used to assign individual passengers or clients to particular itinerary items.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[0-9]{1,8}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="FlightNumberRanges">
                     <xs:annotation>
                        <xs:documentation>Contains a carrier/operating carrier, and an optional range of included/excluded flight numbers for that carrier. Used primarily for promotions, where only certain flight numbers may qualify.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="99" name="FlightNumberRange">
                              <xs:complexType>
                                 <xs:attribute name="OriginDestinationReference" type="RPH_Type" use="optional"/>
                                 <xs:attribute name="Operation" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Note: only set this attribute if there is a flight number range present.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                       <xs:restriction base="xs:string">
                                          <xs:enumeration value="Include"/>
                                          <xs:enumeration value="Exclude"/>
                                       </xs:restriction>
                                    </xs:simpleType>
                                 </xs:attribute>
                                 <xs:attribute name="CarrierCode" type="StringLength1to8" use="required"/>
                                 <xs:attribute name="OperatingCarrierCode" type="StringLength1to8" use="optional"/>
                                 <xs:attribute name="MinFlightNumber" type="xs:positiveInteger" use="optional"/>
                                 <xs:attribute name="MaxFlightNumber" type="xs:positiveInteger" use="optional"/>
                                 <xs:attribute name="AvailabilityClassCode" type="xs:string" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="FareGroup">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element minOccurs="0" name="FareBasisCodes">
                              <xs:annotation>
                                 <xs:documentation>Contains a list of permitted Fare Basis Codes. Priced options which do not contain one of these will be excluded from the returned result set.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element maxOccurs="unbounded" name="FareBasisCode" type="StringLength1to16"/>
                                 </xs:sequence>
                                 <xs:attribute name="Operation" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Note: only set this attribute if there is a flight number range present.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                       <xs:restriction base="xs:string">
                                          <xs:enumeration value="Include"/>
                                          <xs:enumeration value="Exclude"/>
                                       </xs:restriction>
                                    </xs:simpleType>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                           <xs:element minOccurs="0" name="BookingCodes">
                              <xs:annotation>
                                 <xs:documentation>Contains a list of permitted Booking Codes. Priced options which do not contain one of these will be excluded from the returned result set.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element maxOccurs="unbounded" name="BookingCode" type="UpperCaseAlphaLength1to3"/>
                                 </xs:sequence>
                                 <xs:attribute name="Operation" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Note: only set this attribute if there is a flight number range present.</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                       <xs:restriction base="xs:string">
                                          <xs:enumeration value="Include"/>
                                          <xs:enumeration value="Exclude"/>
                                       </xs:restriction>
                                    </xs:simpleType>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="GroupID" type="xs:string" use="optional"/>
                        <xs:attribute name="Default" type="xs:boolean" use="optional"/>
                        <xs:attribute name="Tier" type="xs:string" use="optional"/>
                        <xs:attribute name="BonusMiles" type="xs:integer" use="optional"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:OriginDestinationInformation.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:OriginDestinationInformation.types.extensions.air.rqrs.ota.datalex.com">
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05"/>
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:simpleType name="StringLength1to8">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 8</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="8"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="LocationType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Code and domestic/international flag to describe a location point</xs:documentation>
            </xs:annotation>
            <xs:simpleContent>
               <xs:extension base="xs:string">
                  <xs:attribute name="LocationCode" type="StringLength1to8" use="required"/>
                  <xs:attribute name="IsDomestic" type="xs:boolean" use="required"/>
                  <xs:attribute default="true" name="IsAirport" type="xs:boolean" use="optional"/>
               </xs:extension>
            </xs:simpleContent>
         </xs:complexType>
         <xs:simpleType name="PreferLevelType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used to specify a preference level for something that is or will be requested (e.g. a supplier of a service, a type of service, a form of payment, etc.).</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:NMTOKEN">
               <xs:enumeration value="Only"/>
               <xs:enumeration value="Unacceptable"/>
               <xs:enumeration value="Preferred"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:attributeGroup name="PreferLevelGroup">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used to indicate a level of prefernce for an associate item, only, unacceptable, preferred.</xs:documentation>
            </xs:annotation>
            <xs:attribute default="Preferred" name="PreferLevel" type="PreferLevelType"/>
         </xs:attributeGroup>
         <xs:simpleType name="CabinType">
            <xs:annotation>
               <xs:documentation xml:lang="en">A cabin is either First, Business, Economy or Premium Economy</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:NMTOKEN">
               <xs:enumeration value="First"/>
               <xs:enumeration value="Business"/>
               <xs:enumeration value="Economy"/>
               <xs:enumeration value="PremiumEconomy"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="CabinPrefType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Indicates preferences for choice of airline cabin for a given travel situation.</xs:documentation>
            </xs:annotation>
            <xs:attributeGroup ref="PreferLevelGroup"/>
            <xs:attribute name="Cabin" type="CabinType"/>
         </xs:complexType>
         <xs:element name="ODIServiceInformation">
            <xs:annotation>
               <xs:documentation>Service Operation Information for the requested origin-destination locations and date.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element minOccurs="0" ref="ns7:DaysOfWeek">
                     <xs:annotation>
                        <xs:documentation>Service Operation days of week configured.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element maxOccurs="99" minOccurs="0" name="FlexibleSearchDates" type="xs:date">
                     <xs:annotation>
                        <xs:documentation>Available service dates for requested flexible search.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="odiRPH" type="ns8:RPH_Type" use="optional">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">A have link with original OriginDestinationInformation for which it was created, 
    					as ODIServiceInformation is going to be populated for ALFS response without direct connection with ODI or ODO.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Origin" type="StringLength1to8" use="required">
                  <xs:annotation>
                     <xs:documentation>Requested origin location code.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Destination" type="StringLength1to8" use="required">
                  <xs:annotation>
                     <xs:documentation>Requested destination location code.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="SearchDate" type="xs:date" use="required">
                  <xs:annotation>
                     <xs:documentation>Requested search date.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="HasService" type="xs:boolean" use="required">
                  <xs:annotation>
                     <xs:documentation>If Service Operation is configured for requested locations and date.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="FromDate" type="xs:date" use="optional">
                  <xs:annotation>
                     <xs:documentation>Service Operation start date.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ToDate" type="xs:date" use="optional">
                  <xs:annotation>
                     <xs:documentation>Service Operation end date.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="NextDayOfService" type="xs:date" use="optional">
                  <xs:annotation>
                     <xs:documentation>Next day availble for service.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PrevDayOfService" type="xs:date" use="optional">
                  <xs:annotation>
                     <xs:documentation>Previous day availble for service.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ServiceType" type="StringLength1to8" use="optional">
                  <xs:annotation>
                     <xs:documentation>Additional text information about Service Operation type/category (e.g. Transatlantic).</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="AdditionalOriginLocations">
                     <xs:annotation>
                        <xs:documentation>Contains a list of additional origin cities. If this is a flight specific availability for Continental Airlines, then this is the origin of the O and D that wraps this segment.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="99" name="OriginLocation" type="LocationType"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="AdditionalDestinationLocations">
                     <xs:annotation>
                        <xs:documentation>Contains a list of additional destination cities. If this is a flight specific availability for Continental Airlines, then this is the destination of the O and D that wraps this segment.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="99" name="DestinationLocation" type="LocationType"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="DepartureTimeRanges">
                     <xs:annotation>
                        <xs:documentation>Contains a list of min/max time ranges. Used primarily for promotions, where only certain times of day may qualify. If used, it will override any preferred time of day in the user's original request.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="5" name="DepartureTimeRange">
                              <xs:complexType>
                                 <xs:attribute name="MinTime" type="xs:time" use="required"/>
                                 <xs:attribute name="MaxTime" type="xs:time" use="required"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="DaysOfWeek">
                     <xs:annotation>
                        <xs:documentation>Contains a list of days of the week that flights may depart in this OriginDestination.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="6" name="DayOfWeek">
                              <xs:simpleType>
                                 <xs:restriction base="xs:string">
                                    <xs:enumeration value="Monday"/>
                                    <xs:enumeration value="Tuesday"/>
                                    <xs:enumeration value="Wednesday"/>
                                    <xs:enumeration value="Thursday"/>
                                    <xs:enumeration value="Friday"/>
                                    <xs:enumeration value="Saturday"/>
                                    <xs:enumeration value="Sunday"/>
                                 </xs:restriction>
                              </xs:simpleType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="CabinClass" type="CabinPrefType">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Defines preferred  cabin to be used in a search for complete request.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="LegType">
                     <xs:annotation>
                        <xs:documentation>The type of leg being requested. Valid values are: Air, Surface, Construction, VariableDates</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="AIR"/>
                           <xs:enumeration value="SURFACE"/>
                           <xs:enumeration value="CONSTRUCTION"/>
                           <xs:enumeration value="VARIABLEDATE"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="NoTimeOfDay" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Indicates if no time of day is specified in OrginDateTime or DestinationDateTime elements</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="LengthOfStays">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="LengthOfStay" type="xs:int"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="MarketingCabin">
                     <xs:annotation>
                        <xs:documentation>This is Used for specifying a complex Cabin Class not defined withing Cabin Preferences.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Cabin" type="xs:string" use="required"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ODIServiceInformation"/>
                  <xs:element default="false" minOccurs="0" name="LockDate" type="xs:boolean">
                     <xs:annotation>
                        <xs:documentation>Set this value to indicate that the travel window should not be used; that this particular entry is not flexible.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:groundtransportation.rqrs.datalex.com" xmlns="urn:groundtransportation.rqrs.datalex.com">
         <xs:import namespace="urn:rqrs.cbp.datalex.com"/>
         <xs:import namespace="urn:merch.rqrs.datalex.com"/>
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:import namespace="urn:media.rqrs.datalex.com"/>
         <xs:complexType name="DLX_GroundTransportationSvRSBase">
            <xs:complexContent>
               <xs:extension base="ns1:CBPSvRSBase"/>
            </xs:complexContent>
         </xs:complexType>
         <xs:simpleType name="OTA_CodeType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for codes in the OTA code tables. Possible values of this pattern are 1, 101, 101.EQP, or 101.EQP.X.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[0-9A-Z]{1,3}(\.[A-Z]{3}(\.X){0,1}){0,1}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="StringLength1to64">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 64</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="64"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:attributeGroup name="ErrorWarningAttributeGroup">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used to identify an application error by either text, code, or by an online description and also to give the status, tag, and/or identification of the record that may have caused the error.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="ShortText" type="StringLength1to64" use="optional"/>
            <xs:attribute name="Code" type="OTA_CodeType" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If present, this refers to a table of coded values exchanged between applications to identify errors or warnings. Refer to OTA Code List Error Codes (ERR).</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="DocURL" type="xs:anyURI" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If present, this URL refers to an online description of the error that occurred.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Status" type="StringLength1to64" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If present, recommended values are those enumerated in the OTA_ErrorRS, (NotProcessed | Incomplete | Complete | Unknown) however, the data type is designated as string data, recognizing that trading partners may identify additional status conditions not included in the enumeration.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Tag" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If present, this attribute may identify an unknown or misspelled tag that caused an error in processing. It is recommended that the Tag attribute use XPath notation to identify the location of a tag in the event that more than one tag of the same name is present in the document. Alternatively, the tag name alone can be used to identify missing data [Type=ReqFieldMissing].</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="RecordID" type="StringLength1to64" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If present, this attribute allows for batch processing and the identification of the record that failed amongst a group of records.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:attributeGroup name="LanguageGroup">
            <xs:attribute name="Language" type="xs:language" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Language identification.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:complexType name="FreeTextType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Textual information to provide descriptions and/or additional information.</xs:documentation>
            </xs:annotation>
            <xs:attributeGroup ref="LanguageGroup"/>
         </xs:complexType>
         <xs:complexType name="ErrorType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Standard way to indicate that an error occurred during the processing of an OTA message</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
               <xs:extension base="FreeTextType">
                  <xs:attribute name="Type" type="OTA_CodeType" use="required">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">The Error element MUST contain the Type attribute that uses a recommended set of values to indicate the error type. The validating XSD can expect to accept values that it has NOT been explicitly coded for and process them by using Type ="Unknown".  Refer to OTA Code List Error Warning Type (EWT).</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attributeGroup ref="ErrorWarningAttributeGroup"/>
                  <xs:attribute name="NodeList" type="xs:string">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">An XPath expression that selects all the nodes whose data caused this error.  Further, this expression should have an 	additional contraint which contains the data of the node.  This will provide the offending data back to systems that cannot maintain the original message.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:complexType name="ProductOrOptionDescription">
            <xs:annotation>
               <xs:documentation>Lingual definition of a ground transportation product or option component.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element minOccurs="0" name="ShortDescription" type="xs:string"/>
               <xs:element minOccurs="0" name="LongDescription" type="xs:string"/>
            </xs:sequence>
            <xs:attribute name="LanguageCode" type="xs:string" use="optional"/>
            <xs:attribute name="ShortName" type="xs:string" use="optional"/>
         </xs:complexType>
         <xs:element name="GroundTransportationProductDescription" type="ProductOrOptionDescription">
            <xs:annotation>
               <xs:documentation>Ground Transportation lingual name and description</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="SearchAndDescriptiveAttributes">
            <xs:annotation>
               <xs:documentation>Contains searchable and descriptive user definable attribute information</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element ref="ns7:Attributes"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="GroundServiceType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Service and vehicle information.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element minOccurs="0" name="Languages">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Collection of language details pertaining to the service.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element maxOccurs="10" name="Language">
                           <xs:annotation>
                              <xs:documentation xml:lang="en">Language details pertaining to the service.</xs:documentation>
                           </xs:annotation>
                           <xs:complexType>
                              <xs:attribute name="Language" type="xs:language" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">Language spoken by the driver or the guide.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="PrimaryLangInd" type="xs:boolean" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">If TRUE, this is the primary language spoken by the driver or the guide.</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                           </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
            <xs:attribute name="ChildCarSeatInd" type="xs:boolean" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If TRUE, a child car seat is required or requested.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="PetFriendlyInd" type="xs:boolean" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If TRUE, a vehicle is being requested and/or is available that allows domestic pets.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="FuelEfficientVehInd" type="xs:boolean" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If TRUE, the vehicle is fuel efficient.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MaximumPassengers" type="xs:positiveInteger" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">The maximum quantity of passengers this vehicle can accommodate. NOTE: This figure is based on the quantity of seat belts in the vehicle.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MaximumBaggage" type="xs:positiveInteger" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">The (recommended) maximum baggage that can be stored in the vehicles trunk.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="GreetingSignInd" type="xs:boolean" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If TRUE, a greeting sign is requested.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="GreetingSignName" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">The name to be on the greeting sign.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MeetAndGreetInd" type="xs:boolean" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If TRUE, complementary meet and greet services are requested and/or provided..</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="PersonalGreeterInd" type="xs:boolean" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If TRUE, personal meet and greet is requested (that may incur an additional charge) or available.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="GuideInd" type="xs:boolean" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If TRUE, guide services are requested and/or provided for local area knowledge and information.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ShortDescription">
               <xs:annotation>
                  <xs:documentation xml:lang="en">A short description of the service type and vehicle (up to 250 characters.)</xs:documentation>
               </xs:annotation>
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:minLength value="0"/>
                     <xs:maxLength value="250"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="Notes" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Other notes related to the ground service, such as "Driver will meet you in baggage claim."</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MultilingualInd" type="xs:boolean" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">If TRUE, specific languages are requested and/or provided.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:complexType>
         <xs:complexType name="GroundServiceDetailType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Passenger preferences requested and/or required for a ground transportation service, including service type, disability vehicle and other vehicle preferences.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
               <xs:extension base="GroundServiceType">
                  <xs:attribute name="ServiceType" type="xs:string" use="required">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">The available service type, such as Airport Transfer, Hourly and Simple Transfer.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="VehicleType" type="xs:string" use="required">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">The type of available vehicle. Refer to the OpenTravel Vehicle Category (VEC) code list.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="DisabilityVehicleInd" type="xs:boolean" use="required">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">If TRUE, this vehicle is disability compatible.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="VehicleSize" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">The size of the vehicle. Refer to OpenTravel Size (SIZ) code list.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ServiceLevel" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">The level of service, such as deluxe and VIP. Refer to OpenTravel Level of Service (LOS) code list.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="FranchiseCode" type="xs:string" use="optional"/>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:element name="GroundTransportationOptionDescriptions">
            <xs:annotation>
               <xs:documentation>Contains descriptive text information for a ground transportation option</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="GroundTransportationOptionDescription" type="ProductOrOptionDescription"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="ProviderLinks">
            <xs:annotation>
               <xs:documentation>Provider codes valid in TDP Provider</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element name="ProviderProductCoding">
                     <xs:annotation>
                        <xs:documentation>Provide product coding is the key values to define the component option in the provider's system.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType mixed="true">
                        <xs:complexContent mixed="true">
                           <xs:extension base="xs:anyType"/>
                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="ProviderCode" type="xs:string" use="required"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="GroundTransportationOptionProviderLinks">
            <xs:annotation>
               <xs:documentation>Contains a list of Providers who "provide" this ground transportationoption.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" ref="ProviderLinks"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:attributeGroup name="VehicleMakeModelGroup">
            <xs:annotation>
               <xs:documentation xml:lang="en">Indicates the car identifiers including make and model of the car with the year.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="Name" type="xs:string" use="optional"/>
            <xs:attribute name="Code" type="xs:string" use="optional"/>
            <xs:attribute name="Color" type="xs:string" use="optional"/>
            <xs:attribute name="Make" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">The model year of the vehicle.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ModelYear" type="xs:string" use="optional"/>
         </xs:attributeGroup>
         <xs:element name="GroundTransportationOptionMediaContent">
            <xs:annotation>
               <xs:documentation>Media content connected to the ground transportation option.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="MediaContentUsages" type="ns29:MediaContentUsageType">
                     <xs:annotation>
                        <xs:documentation>collection of media content assignments</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="ServiceChargeType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Identifies the purpose of the charge, that is, the entity to which the charge applies.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
               <xs:extension base="ns7:CustomProperties">
                  <xs:sequence>
                     <xs:element minOccurs="0" name="TaxAmounts">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">A collection of one or more taxes associated with a specific charge.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="ns7:CustomProperties">
                                 <xs:sequence>
                                    <xs:element maxOccurs="99" name="TaxAmount">
                                       <xs:annotation>
                                          <xs:documentation xml:lang="en">Detailed information on one specific tax associated with a specific charge</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:complexContent>
                                             <xs:extension base="ns7:CustomProperties">
                                                <xs:attribute name="Total" type="xs:decimal" use="required"/>
                                                <xs:attribute name="CurrencyCode" type="xs:string" use="required"/>
                                                <xs:attribute name="TaxCode" type="xs:string" use="optional"/>
                                                <xs:attribute name="Description" type="xs:string" use="optional"/>
                                             </xs:extension>
                                          </xs:complexContent>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element minOccurs="0" name="MinMax">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Information on any minimum or maximum amounts, if appropriate.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="ns7:CustomProperties">
                                 <xs:attribute name="MaxCharge" type="xs:decimal" use="optional"/>
                                 <xs:attribute name="MinCharge" type="xs:decimal" use="optional"/>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element maxOccurs="5" minOccurs="0" name="Calculation">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Information on how this charge was calculated, for example, a daily rate multiplied by the number of days, a percentage, etc.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="ns7:CustomProperties">
                                 <xs:attribute name="UnitCharge" type="xs:decimal" use="optional"/>
                                 <xs:attribute name="Quantity" type="xs:int" use="optional"/>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                  </xs:sequence>
                  <xs:attribute name="Amount" type="xs:decimal" use="required"/>
                  <xs:attribute name="CurrencyCode" type="xs:string" use="required"/>
                  <xs:attribute name="TaxInclusive" type="xs:boolean" use="required"/>
                  <xs:attribute name="Description" type="xs:string" use="optional"/>
                  <xs:attribute name="GuaranteedInd" type="xs:boolean" use="optional"/>
                  <xs:attribute default="false" name="IncludedInRate" type="xs:boolean" use="optional">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">An indication if this amount is included in the base vehicle rate, or is an additonal cost.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:element name="GroundTransportationProductOption">
            <xs:annotation>
               <xs:documentation>An Ground Transportation Product Option. Used to describe the various options for the ground transportation. This is the priceable level of a ground transportation product.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element minOccurs="0" ref="SearchAndDescriptiveAttributes"/>
                  <xs:element minOccurs="0" ref="GroundTransportationOptionDescriptions"/>
                  <xs:element minOccurs="0" ref="GroundTransportationOptionMediaContent"/>
                  <xs:element minOccurs="0" ref="GroundTransportationOptionProviderLinks"/>
                  <xs:element minOccurs="0" name="GroundTransportationServiceInfo" type="GroundServiceDetailType">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Available service information.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="GroundTransportationVehicleMakeModel">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Vehicle make and model information.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attributeGroup ref="VehicleMakeModelGroup"/>
                        <xs:attribute name="Description" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="ServiceCharges">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="ServiceCharge">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element minOccurs="0" name="Description">
                                       <xs:complexType>
                                          <xs:attribute name="Name" type="xs:string" use="required"/>
                                          <xs:attribute name="Quantity" type="xs:int" use="optional"/>
                                       </xs:complexType>
                                    </xs:element>
                                    <xs:element name="Charge" type="ServiceChargeType"/>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="LocationsAssociation">
                     <xs:annotation>
                        <xs:documentation>Locations index with which this product option is associated. A product option may be associated with more than one locations.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="LocationReference" type="xs:integer" use="required"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute default="DEFAULT" name="GroundTransportationOptionCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Unique code key identifying the ground transportationoption</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ProviderCode" type="xs:string" use="optional"/>
               <xs:attribute name="PricingSystem" type="xs:string" use="optional"/>
               <xs:attribute name="InventorySystem" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="InformationCategories">
            <xs:annotation>
               <xs:documentation>Descriptive information by catergory used to further describe the activity product or component.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="InformationCategory">
                     <xs:annotation>
                        <xs:documentation>Sections and sub-sections of language based text descriptions. Used to describe products on a topical basis.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element minOccurs="0" name="SectionText">
                              <xs:complexType>
                                 <xs:simpleContent>
                                    <xs:extension base="xs:string"/>
                                 </xs:simpleContent>
                              </xs:complexType>
                           </xs:element>
                           <xs:element maxOccurs="unbounded" minOccurs="0" name="SubCategory">
                              <xs:annotation>
                                 <xs:documentation>Sub categories within the manin category.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element minOccurs="0" name="SubCategoryText" type="xs:string"/>
                                 </xs:sequence>
                                 <xs:attribute name="SectionCode" type="xs:string" use="required"/>
                                 <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
                                 <xs:attribute name="SectionName" type="xs:string" use="required"/>
                                 <xs:attribute name="IncludeOnConfirmation" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>If TRUE, this section and subsection should be included on confirmation documents.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="IncludeOnVoucher" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>If TRUE, this section and subsection should be included on voucher documents.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="SectionCode" type="xs:string" use="required"/>
                        <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
                        <xs:attribute name="SectionName" type="xs:string" use="required"/>
                        <xs:attribute name="IncludeOnConfirmation" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>If TRUE, this section and subsection should be included on confirmation documents.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="IncludeOnVoucher" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>If TRUE, this section and subsection should be included on voucher documents.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="GroundTransportationProductMediaContent">
            <xs:annotation>
               <xs:documentation>Media content linked to the ground transportation product</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="MediaContentUsages" type="ns29:MediaContentUsageType">
                     <xs:annotation>
                        <xs:documentation>collection of media content assignments</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="GroundTransportationProductType">
            <xs:annotation>
               <xs:documentation>Ground transportation locations, descriptions and options.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element maxOccurs="unbounded" name="Locations">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Pickup, interim stops and drop-off details.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:complexContent>
                        <xs:extension base="ns9:GroundLocationsType">
                           <xs:attribute name="LocationReference" type="xs:integer" use="optional"/>
                        </xs:extension>
                     </xs:complexContent>
                  </xs:complexType>
               </xs:element>
               <xs:element minOccurs="0" name="ProductType" type="ns9:ProductTypeType"/>
               <xs:element minOccurs="0" ref="SearchAndDescriptiveAttributes"/>
               <xs:element minOccurs="0" ref="GroundTransportationProductDescription"/>
               <xs:element minOccurs="0" ref="InformationCategories"/>
               <xs:element minOccurs="0" ref="GroundTransportationProductMediaContent"/>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Ratings">
                  <xs:annotation>
                     <xs:documentation>Ratings and reviews</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:attribute name="RatingType" use="required">
                        <xs:annotation>
                           <xs:documentation>Indicates the type author rating given to the hotel.  Values are:
"CONSUMER"  -- Rating given by a people who statyed at the hotel
"SELLER" -- Rating given by the seller of the room stay like an agency or airline.
"ORGANIZATION"  -- A third party organization like national hotel rating organizations and media like Conde Nast
"PROVIDER"  -- Ranking given by the providerr of the hotel.  Either the wholesale merchant or the hotel chain or property themselves.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="CONSUMER"/>
                              <xs:enumeration value="SELLER"/>
                              <xs:enumeration value="ORGANIZATION"/>
                              <xs:enumeration value="PROVIDER"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="RatingOrganizationName" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name of the organization like "Trip Advisor" or "Yelp"</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="Rating" type="xs:float" use="required">
                        <xs:annotation>
                           <xs:documentation>Floating numeric value of the rating given to this hotel.  The rating is only relative to other rating numbers within this organization and can not be compared across organizations.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="RatingMaximum" type="xs:float" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indicates the maximum rating from this rating body (if any) for example: 3 out of 5 where 5 is the maximum.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ReviewCount" type="xs:integer" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indicates the number of reviews given the hotel by this organization or group</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="RatingURL" type="xs:anyURI" use="optional">
                        <xs:annotation>
                           <xs:documentation>A URI (URL) to locate an actual review page.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:complexType>
               </xs:element>
               <xs:element name="GroundTransportationOptions">
                  <xs:annotation>
                     <xs:documentation>Contains ground transportationoptions.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" ref="GroundTransportationProductOption"/>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
            <xs:attribute name="GroundTransportationSequenceNmbr" type="xs:integer" use="required">
               <xs:annotation>
                  <xs:documentation>Corresponding sequence to the ground transportation request</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:complexType>
         <xs:element name="GroundTransportationResponse" type="GroundTransportationProductType">
            <xs:annotation>
               <xs:documentation>The content for all ground transportation products and option are returned.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:complexType name="GroundTransportationResponsesType">
            <xs:annotation>
               <xs:documentation>A container list of ground transportation responses</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element maxOccurs="unbounded" ref="GroundTransportationResponse">
                  <xs:annotation>
                     <xs:documentation>In a response to a direct product search, the content for all ground transportation product and option is returned.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:complexType>
         <xs:element name="GroundTransportationResponses" type="GroundTransportationResponsesType">
            <xs:annotation>
               <xs:documentation>A container of list or detailed content for ground transportation products.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:complexType name="GroundTransportationPricedOptionType">
            <xs:annotation>
               <xs:documentation>Priced ground transportation options.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element name="GroundTransportationTotalPrice">
                  <xs:annotation>
                     <xs:documentation>Contains a total price for the chosen ground transportation options.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element name="Rates">
                           <xs:annotation>
                              <xs:documentation>A collection of rate codes and prices for an Ground Transportation Option</xs:documentation>
                           </xs:annotation>
                           <xs:complexType>
                              <xs:sequence>
                                 <xs:element maxOccurs="unbounded" name="Rate" type="ns9:RateType">
                                    <xs:annotation>
                                       <xs:documentation>A unique code given to the rate plan used to price the option.  Rate plans contain prices by date, policies, etc. and are applied to ground transportation options.  If the pricing system is not able to sell by rate code use DEFAULT</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                              </xs:sequence>
                           </xs:complexType>
                        </xs:element>
                        <xs:element minOccurs="0" ref="ns9:Persona"/>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
               <xs:element maxOccurs="unbounded" name="GroundTransportationOptionPricing">
                  <xs:annotation>
                     <xs:documentation>Contains a set of ground transportation options.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element name="Rates">
                           <xs:annotation>
                              <xs:documentation>A collection of rate codes and prices for an Ground Transportation Option</xs:documentation>
                           </xs:annotation>
                           <xs:complexType>
                              <xs:sequence>
                                 <xs:element maxOccurs="unbounded" name="Rate" type="ns9:RateType">
                                    <xs:annotation>
                                       <xs:documentation>A unique code given to the rate plan used to price the option.  Rate plans contain prices by date, policies, etc. and are applied to ground transportation options.  If the pricing system is not able to sell by rate code use DEFAULT</xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                              </xs:sequence>
                           </xs:complexType>
                        </xs:element>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="LocationsAssociation">
                           <xs:annotation>
                              <xs:documentation>Locations index with which this product option is associated. A product option may be associated with more than one locations.</xs:documentation>
                           </xs:annotation>
                           <xs:complexType>
                              <xs:attribute name="LocationReference" type="xs:integer" use="required"/>
                           </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                     <xs:attribute name="GroundTransportationOptionCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>Unique pricing code returned from the host for the ground transportation option.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ProviderCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>Unique code for the Provider (Supplier) of the ground transportation option. Valid in TDP Provider. Useful if the activity option is provided by more than one provider</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
            <xs:attribute name="GroundTransportationSequenceNmbr" type="xs:integer" use="required"/>
         </xs:complexType>
         <xs:element name="GroundTransportationPricedOption">
            <xs:annotation>
               <xs:documentation>An ground transportation product including the content needed to create a shopping response list and a set of valid priced ground transportation options for the product.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="GroundTransportationPricedOptionType"/>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="GroundTransportationPriceAvailRS">
            <xs:annotation>
               <xs:documentation>Ground Transportation price availability response. Priced options and list criteria are returned in response to a general or direct search request.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element ref="GroundTransportationResponses"/>
                  <xs:element name="GroundTransportationPricedOptions">
                     <xs:annotation>
                        <xs:documentation>A container of ground transportation products including the content needed to create a shopping response list and a set of valid priced ground transportation options for the product.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" ref="GroundTransportationPricedOption"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns9:PresentationGroup"/>
                  <xs:element minOccurs="0" ref="ns9:CommonRequestCriteria"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="SuccessType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Standard way to indicate successful processing of an OTA message. Returning an empty element of this type indicates success.</xs:documentation>
            </xs:annotation>
         </xs:complexType>
         <xs:complexType name="ErrorCodeType">
            <xs:simpleContent>
               <xs:extension base="xs:string">
                  <xs:attribute name="Context" type="xs:integer" use="required"/>
                  <xs:attribute name="MajorCode" type="xs:integer" use="required"/>
                  <xs:attribute name="WarningCode" type="xs:integer" use="required"/>
                  <xs:attribute name="Description" type="xs:string" use="optional"/>
               </xs:extension>
            </xs:simpleContent>
         </xs:complexType>
         <xs:complexType name="MisspeltOrAmbiguousLocationType">
            <xs:annotation>
               <xs:documentation>Any misspelt or ambiguous location suggestions.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="LandmarkName">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Landmark or hotel.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:simpleContent>
                        <xs:extension base="xs:string">
                           <xs:attribute name="LandmarkId" type="xs:string" use="optional"/>
                           <xs:attribute name="LandmarkType" type="xs:string" use="optional"/>
                        </xs:extension>
                     </xs:simpleContent>
                  </xs:complexType>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Position">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">The Position element contains attributes, Latitude, Longitude, and Altitude, used to indicate the geographic location(s).</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:attributeGroup ref="ns9:PositionGroup">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">The geographic coordinates for the search.</xs:documentation>
                        </xs:annotation>
                     </xs:attributeGroup>
                  </xs:complexType>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="Address">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">The physical address of the pickup, stop or dropoff.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:complexContent>
                        <xs:extension base="ns9:AddressInfoType"/>
                     </xs:complexContent>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
         </xs:complexType>
         <xs:element name="MisspeltOrAmbiguousLocationRS">
            <xs:annotation>
               <xs:documentation>Any misspelt or ambiguous location suggestions also includes narrowing an address zip code to a sub zip code.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="MisspeltOrAmbiguousLocationType">
                     <xs:attribute name="SequenceNmbr" type="xs:string" use="required"/>
                     <xs:attribute name="PickupLocation" type="xs:boolean" use="required"/>
                     <xs:attribute name="DropoffLocation" type="xs:boolean" use="required"/>
                     <xs:attribute name="StopSequence" type="xs:positiveInteger" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="WarningCode">
            <xs:complexType>
               <xs:simpleContent>
                  <xs:extension base="xs:string">
                     <xs:attribute name="Context" type="xs:integer" use="required"/>
                     <xs:attribute name="MajorCode" type="xs:integer" use="required"/>
                     <xs:attribute name="WarningCode" type="xs:integer" use="required"/>
                     <xs:attribute name="Description" type="xs:string" use="optional"/>
                  </xs:extension>
               </xs:simpleContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="Warnings">
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="WarningCode"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="DLX_GroundTransportationPriceAvailSvRS">
            <xs:annotation>
               <xs:documentation>Ground transportation service response</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="DLX_GroundTransportationSvRSBase">
                     <xs:choice>
                        <xs:sequence>
                           <xs:element name="Success" type="SuccessType"/>
                           <xs:element minOccurs="0" ref="Warnings"/>
                           <xs:sequence>
                              <xs:element minOccurs="0" ref="GroundTransportationPriceAvailRS"/>
                              <xs:element maxOccurs="unbounded" minOccurs="0" ref="MisspeltOrAmbiguousLocationRS"/>
                           </xs:sequence>
                        </xs:sequence>
                        <xs:element name="Errors">
                           <xs:complexType>
                              <xs:sequence>
                                 <xs:element maxOccurs="99" name="Error">
                                    <xs:complexType>
                                       <xs:complexContent>
                                          <xs:extension base="ErrorType">
                                             <xs:sequence>
                                                <xs:element minOccurs="0" name="ErrorCode" type="ErrorCodeType"/>
                                             </xs:sequence>
                                          </xs:extension>
                                       </xs:complexContent>
                                    </xs:complexType>
                                 </xs:element>
                              </xs:sequence>
                           </xs:complexType>
                        </xs:element>
                     </xs:choice>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="GroundTransportationProduct" type="GroundTransportationProductType">
            <xs:annotation>
               <xs:documentation>A Ground Transportation Product.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:ancillaryaircomponent.rqrs.datalex.com" xmlns="urn:ancillaryaircomponent.rqrs.datalex.com">
         <xs:import namespace="urn:merch.rqrs.datalex.com"/>
         <xs:import namespace="urn:policy.rqrs.datalex.com"/>
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:import namespace="urn:fee.rqrs.tdp.datalex.com"/>
         <xs:import namespace="urn:attributeassign.rqrs.datalex.com"/>
         <xs:simpleType name="TicketingFeesNoChargeType">
            <xs:annotation>
               <xs:documentation>When no charge is set for ticketing fees, what value to use</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="NO_CHARGE_NO_RECEIPT">
                  <xs:annotation>
                     <xs:documentation>Equivalent to no charge value 'X'</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
               <xs:enumeration value="NO_CHARGE_ISSUE_RECEIPT">
                  <xs:annotation>
                     <xs:documentation>Equivalent to no charge value 'Y'</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="InventoryCheckType">
            <xs:annotation>
               <xs:documentation>Specifies the set of operations that should be performed on the ancillary component within the flow. It can be availability + allocate + confirm (AVAILABLE), or allocate + confirm (PREALLOCATE), or just a confirm (FREESALE).</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="FREESALE"/>
               <xs:enumeration value="AVAILABLE"/>
               <xs:enumeration value="PREALLOCATE"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="Inventory">
            <xs:annotation>
               <xs:documentation>Inventory information</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="InventoryCheck" type="InventoryCheckType" use="required">
                  <xs:annotation>
                     <xs:documentation>Specifies the set of operations that should be performed on the ancillary component within the flow.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="AvailableFrom" type="xs:dateTime" use="optional">
                  <xs:annotation>
                     <xs:documentation>The first date when the ancillary component is available</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="AvailableUntil" type="xs:dateTime" use="optional">
                  <xs:annotation>
                     <xs:documentation>The last date when the ancillary component is available</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="InventoryKeyCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Inventory key code (Attribute Type Code assigned to Ancillary Component).</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="isInventoryDefined" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>Indicates if the inventory defined for the inventory managed products.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="FreeSale" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>Indicates free-sale inventory product.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="AllocationRef" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Allocation reference number. By default, it is unique per allocation transaction (i.e. can be multiple meals for multiple legs for a single Itinerary) and it is used alter to book / confirm items, or to cancel expired allocations.It is not used when FreeSale flag is true.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Quantity" type="xs:integer" use="optional">
                  <xs:annotation>
                     <xs:documentation>Quantity of products available or allocated. If InventoryPoolCode is specified, then it is a minimum value between pool capacity left and this product component individual capacity left.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="InventoryPoolCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Inventory pool code (e.g. OVEN 26), indicates if it uses some sort of shared capacity pool as additional restriction, possibly with other components in the response.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="IsOpen" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>Indicates if product is opened for sale</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="AppliedPolicies">
            <xs:annotation>
               <xs:documentation>Contains an array of payment and change/cancellation policies for this item</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns53:PolicyRS"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="DocumentTreatmentTypeType">
            <xs:annotation>
               <xs:documentation>Specifies the document treatment, Y=Standalone J=Attached to Air Ticket.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="Y">
                  <xs:annotation>
                     <xs:documentation>Standalone.</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
               <xs:enumeration value="J">
                  <xs:annotation>
                     <xs:documentation>Attached to Air Ticket.</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="PresentationOrder">
            <xs:annotation>
               <xs:documentation>The presentation group and order or priority within a group</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="PresentationGroup" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>A unique presentation group code used to group priced options.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PresentationGroupSequence" type="xs:integer" use="optional">
                  <xs:annotation>
                     <xs:documentation>The sequence within a presentation group (if a presentation group is used) or the general presentation sequence if no group is used.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="BundleMemberPricedOptions">
            <xs:annotation>
               <xs:documentation>Container for the priced options that relate to this priced option AAC BundleMembers</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="BundleMemberPricedOption">
                     <xs:complexType>
                        <xs:attribute name="AncillaryAirComponentPricedOptionsDocRef" type="xs:IDREF"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="BundleMemberAncillaryAirComponentCode" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="AncillaryAirComponentDescription">
            <xs:annotation>
               <xs:documentation>This is a lingual description of the overall Ancillary Air Component.  Each element represents a different language.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="DescriptionCode" type="xs:string"/>
               </xs:sequence>
               <xs:attribute name="LanguageCode" type="xs:string" use="required">
                  <xs:annotation>
                     <xs:documentation>This is the language code of this AncillaryAirComponentDescription element.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="AncillaryAirComponentName" type="xs:string" use="required">
                  <xs:annotation>
                     <xs:documentation>This is a human readable name of an Ancillary Air Component.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ShortDescription" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>A description relating to an AncillaryAirComponent in the language of LanguageCode.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="URL" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>@deprecated Use MediaContent instead. This is a link to an external resource which should contain further information relating to an Ancillary Air Component.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="LongDescription" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>A detailed description relating to an AncillaryAirComponent in the language of LanguageCode.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="FareFamilyShortDescription" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>The Fare Family Short Description describes the ancillary item when sold as part of a Fare Family.  This descriptions allows for tokens in the form @1 that can be overridden based on what fare family the items is attached to.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="RemoteSystemRemarks">
            <xs:annotation>
               <xs:documentation>This is a set of remarks which are used to record information particular to an Ancillary Air Component in a reservation.  These remarks can be used to populate a PNR.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="RemarkSet">
                     <xs:annotation>
                        <xs:documentation>This is a set of remarks which are used to record information particular to an Ancillary Air Component in a PNR.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:choice>
                           <xs:element name="OtherServiceInformation">
                              <xs:annotation>
                                 <xs:documentation>OSIs hold machine-readable information that should be picked up by the applicable carrier.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="Text" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>The body of this OSI.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="AirlineCode" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>A two letter IATA carrier code for which this OSI is applicable to.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="SpecialServiceRequest">
                              <xs:annotation>
                                 <xs:documentation>SSRs contain requests for special services which require confirmation from an airline.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="AirlineCode" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>A two letter IATA carrier code identifying the airline for which this SSR is applicable.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="Text" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The body of the SSR.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="Code" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>A four letter code relating to this SSR.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="GeneralRemark">
                              <xs:annotation>
                                 <xs:documentation>A General Remark is a plain-text human-readable string added to the reservation.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="Text" type="xs:string">
                                    <xs:annotation>
                                       <xs:documentation>The body of the remark.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:choice>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="RemoteSystemCode" type="xs:string" use="required"/>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="ItineraryChangeActionType">
            <xs:annotation>
               <xs:documentation>Determines the allowable actions for changing an itinerary with this Ancillary Component.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="PROHIBIT">
                  <xs:annotation>
                     <xs:documentation>Indicates that change functionality is not allowed on any itinerary with this ancillary item.</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
               <xs:enumeration value="PROTECT">
                  <xs:annotation>
                     <xs:documentation>Indicates that this ancillary item should be maintained across any change to the itinerary.</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
               <xs:enumeration value="DROP">
                  <xs:annotation>
                     <xs:documentation>Indicates that this ancillary item will be dropped when changing an itinerary.</xs:documentation>
                  </xs:annotation>
               </xs:enumeration>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="AncillaryAirComponentAttribute">
            <xs:annotation>
               <xs:documentation>Attributes associated with this Ancillary Item</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attributeGroup ref="ns55:AttributeAssingElements"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="Availability">
            <xs:annotation>
               <xs:documentation>The availability status of the item</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="IsAvailable" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>Indicates if TRUE, that the item is available per the application settings.  If FALSE it is not available.  If NULL, not an inventoried item.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="InventoryCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>A unique code key that describes the inventory record or identifier used to determine availability. For example, a booking class code or inventory locator.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="QuantityOnHand" type="xs:integer" use="optional">
                  <xs:annotation>
                     <xs:documentation>The currenty on hand quantity of the item.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="RPH_Type">
            <xs:annotation>
               <xs:documentation>Reference Place Holder - an index code to identify an instance in a collection of like items.. For example,  used to assign individual passengers or clients to particular itinerary items.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[0-9]{1,8}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="ProductType">
            <xs:annotation>
               <xs:documentation>Product Type Group and Sub-Group classification of this item.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="ns9:ProductTypeType"/>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:attributeGroup name="AncillaryAirComponentContentAttributes">
            <xs:attribute name="LanguageCode" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>Identifies the language of this media Content. This code along with the url uniquely identifies the Media file.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Url" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>This specifies the location of the file. The url and the language code together identify the Media file.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Sequence" type="xs:nonNegativeInteger" use="optional">
               <xs:annotation>
                  <xs:documentation>A sequence order can be applied to the media content.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ContentUsageCode" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>Identifies the usage of this content. Must already exist in the database.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="LinkToUrl" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Contains the link to the url that can be applied to the Media content. This can contain up to 200 characters.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Description" type="xs:string" use="optional"/>
            <xs:attribute name="FileName" type="xs:string" use="optional"/>
            <xs:attribute name="MimeExtension" type="xs:string" use="optional"/>
            <xs:attribute name="EffectiveDate" type="xs:date" use="optional"/>
            <xs:attribute name="ExpiresDate" type="xs:date" use="optional"/>
         </xs:attributeGroup>
         <xs:element name="AncillaryAirComponentContent">
            <xs:annotation>
               <xs:documentation>The Ancillary Component Media Content and its usage that is assigned to this ancillary item. The Media file, which is identified by the combination of the url and the language code, must exist already. The Content usage must also exist.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attributeGroup ref="AncillaryAirComponentContentAttributes"/>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="TextType">
            <xs:annotation>
               <xs:documentation>The textual content associated with a particular service/provision</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element maxOccurs="unbounded" name="TextLine" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>An individual line of text from the text table</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
            <xs:attribute name="TableNumber" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>The table number associated with this text</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:complexType>
         <xs:complexType name="AncillaryAirComponentPricedOptionsType">
            <xs:complexContent>
               <xs:extension base="ns7:CustomProperties">
                  <xs:sequence>
                     <xs:element maxOccurs="unbounded" ref="AncillaryAirComponentDescription"/>
                     <xs:element minOccurs="0" ref="AppliedPolicies"/>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="RemoteSystemRemarks"/>
                     <xs:element minOccurs="0" ref="ns54:Fees"/>
                     <xs:element minOccurs="0" name="MileageFee">
                        <xs:complexType>
                           <xs:attribute name="Mileage" type="xs:nonNegativeInteger" use="required"/>
                           <xs:attribute name="MileageApplication" use="optional">
                              <xs:simpleType>
                                 <xs:restriction base="xs:string">
                                    <xs:enumeration value="AND_FEE"/>
                                    <xs:enumeration value="OR_FEE"/>
                                 </xs:restriction>
                              </xs:simpleType>
                           </xs:attribute>
                        </xs:complexType>
                     </xs:element>
                     <xs:element maxOccurs="unbounded" minOccurs="0" ref="AncillaryAirComponentContent"/>
                     <xs:element minOccurs="0" ref="ProductType"/>
