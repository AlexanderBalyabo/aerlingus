                                                      </xs:annotation>
                                                      <xs:simpleType>
                                                         <xs:restriction base="xs:string">
                                                            <xs:enumeration value="Ok"/>
                                                            <xs:enumeration value="Departed"/>
                                                            <xs:enumeration value="TooCloseToDeparture"/>
                                                         </xs:restriction>
                                                      </xs:simpleType>
                                                   </xs:attribute>
                                                   <xs:attribute name="AvailabilityStatus" use="optional">
                                                      <xs:annotation>
                                                         <xs:documentation>The availability of this option. Only used in Shop by Schedule.</xs:documentation>
                                                      </xs:annotation>
                                                      <xs:simpleType>
                                                         <xs:restriction base="xs:string">
                                                            <xs:enumeration value="AvailableInSelectedCabin"/>
                                                            <xs:enumeration value="FullInSelectedCabin"/>
                                                            <xs:enumeration value="FullInSelectedCabinOrHigher"/>
                                                            <xs:enumeration value="FullInAllCabins"/>
                                                         </xs:restriction>
                                                      </xs:simpleType>
                                                   </xs:attribute>
                                                   <xs:attribute name="StopCount" type="xs:nonNegativeInteger" use="optional">
                                                      <xs:annotation>
                                                         <xs:documentation>How many times the aircraft stops in this option. That is: stops, change of gauge, connections.</xs:documentation>
                                                      </xs:annotation>
                                                   </xs:attribute>
                                                   <xs:attribute name="TotalTravelDuration" type="xs:duration" use="optional">
                                                      <xs:annotation>
                                                         <xs:documentation>The total elapsed time for this option, including time spent on the ground.</xs:documentation>
                                                      </xs:annotation>
                                                   </xs:attribute>
                                                   <xs:attribute name="AlternateOrigin" type="xs:boolean" use="optional"/>
                                                   <xs:attribute name="AlternateDestination" type="xs:boolean" use="optional"/>
                                                   <xs:attribute name="IndexKey" type="xs:string" use="optional"/>
                                                   <xs:attribute name="EPUEligible" type="xs:boolean" use="optional">
                                                      <xs:annotation>
                                                         <xs:documentation>The flag for EPU eligibility</xs:documentation>
                                                      </xs:annotation>
                                                   </xs:attribute>
                                                   <xs:attribute default="false" name="TravelPreferredCarrierFlag" type="xs:boolean" use="optional"/>
                                                   <xs:attribute default="false" name="POSPreferredCarrierFlag" type="xs:boolean" use="optional"/>
                                                   <xs:attribute name="FareFamilyCode" type="xs:string" use="optional"/>
                                                </xs:complexType>
                                             </xs:element>
                                             <xs:element minOccurs="0" name="ApplicableSpecialCodes">
                                                <xs:complexType>
                                                   <xs:sequence>
                                                      <xs:element maxOccurs="unbounded" name="SpecialCode">
                                                         <xs:complexType>
                                                            <xs:simpleContent>
                                                               <xs:extension base="xs:string">
                                                                  <xs:attribute name="SpecialCodeType" type="SpecialCodeTypes" use="optional"/>
                                                                  <xs:attribute name="Tier" type="xs:string" use="optional"/>
                                                                  <xs:attribute name="BonusMiles" type="xs:integer" use="optional"/>
                                                                  <xs:attribute name="NameContentKey" type="xs:string" use="optional"/>
                                                                  <xs:attribute name="DetailsContentKey" type="xs:string" use="optional"/>
                                                                  <xs:attribute name="GraphicIndLink" type="xs:string" use="optional"/>
                                                               </xs:extension>
                                                            </xs:simpleContent>
                                                         </xs:complexType>
                                                      </xs:element>
                                                   </xs:sequence>
                                                </xs:complexType>
                                             </xs:element>
                                             <xs:element maxOccurs="unbounded" minOccurs="0" name="FlightOptionExceptions">
                                                <xs:complexType>
                                                   <xs:attribute name="ExceptionType" use="required">
                                                      <xs:simpleType>
                                                         <xs:restriction base="xs:string">
                                                            <xs:enumeration value="D"/>
                                                            <xs:enumeration value="L"/>
                                                            <xs:enumeration value="C"/>
                                                            <xs:enumeration value="B"/>
                                                         </xs:restriction>
                                                      </xs:simpleType>
                                                   </xs:attribute>
                                                   <xs:attribute name="ExceptionDescription" type="xs:string" use="required"/>
                                                   <xs:attribute name="ExceptionDetails" type="xs:string" use="optional"/>
                                                </xs:complexType>
                                             </xs:element>
                                             <xs:element minOccurs="0" name="MarketingCabin">
                                                <xs:annotation>
                                                   <xs:documentation>This is used for specifying a complex Cabin Class not defined within Cabin Preferences.</xs:documentation>
                                                </xs:annotation>
                                                <xs:complexType>
                                                   <xs:attribute name="Cabin" type="xs:string" use="required"/>
                                                </xs:complexType>
                                             </xs:element>
                                          </xs:sequence>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns221:BaggageOption"/>
                  <xs:element minOccurs="0" name="PNREnhancements">
                     <xs:annotation>
                        <xs:documentation>This is a collection of PNREnhancements</xs:documentation>
