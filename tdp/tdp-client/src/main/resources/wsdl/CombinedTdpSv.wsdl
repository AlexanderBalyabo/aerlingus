            <xs:annotation>
               <xs:documentation>Dimensions in linear measure in centimeters and Inches</xs:documentation>
            </xs:annotation>
            <xs:attribute name="LinearDimensionInCM" type="xs:decimal" use="optional"/>
            <xs:attribute name="LinearDimensionInInches" type="xs:decimal" use="optional"/>
         </xs:complexType>
         <xs:complexType name="WeightType">
            <xs:annotation>
               <xs:documentation>Weight in Kilos and LBS</xs:documentation>
            </xs:annotation>
            <xs:attribute name="WeightInKilos" type="xs:decimal" use="optional"/>
            <xs:attribute name="WeightInLbs" type="xs:decimal" use="optional"/>
         </xs:complexType>
         <xs:complexType name="CheckedItemType">
            <xs:sequence>
               <xs:element minOccurs="0" name="ActualWeight" type="WeightType">
                  <xs:annotation>
                     <xs:documentation>The checked item's actual weight. Use this element to give teh known weight at check in.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="WeightAllowance" type="WeightType">
                  <xs:annotation>
                     <xs:documentation>The calculated weight allowance for this checked item</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="WeightMaximum" type="WeightType">
                  <xs:annotation>
                     <xs:documentation>The outside maximum weight for this checked item that will be accepted by the airline.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="ActualLinearSize" type="DimensionsType">
                  <xs:annotation>
                     <xs:documentation>The actual known linear size of the checked item.  Linear size is W x L x H</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="SizeAllowance" type="DimensionsType">
                  <xs:annotation>
                     <xs:documentation>The calculated size allowance for this checked item</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="MaximumSize" type="DimensionsType">
                  <xs:annotation>
                     <xs:documentation>The outside maximum size for this checked item that will be accepted by the airline.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
            <xs:attribute name="CheckInSequence" type="xs:integer" use="optional">
               <xs:annotation>
                  <xs:documentation>Order within traveler in which the item is checked, for example 1st bag, second bag, etc.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="CheckedItemTypeCode" use="required">
               <xs:annotation>
                  <xs:documentation>Code that points to a valid checked item type.  Code is valid in CheckedItemType table in TDP.  For example, Suitcase, Bicycle, Kennel, etc.  These item types map to TDP ancillary item codes.</xs:documentation>
               </xs:annotation>
               <xs:simpleType>
                  <xs:restriction base="xs:string"/>
               </xs:simpleType>
            </xs:attribute>
         </xs:complexType>
         <xs:element name="CheckedItem">
            <xs:annotation>
               <xs:documentation>A checked item, its check in sequence, and optionally the item weight and dimensions</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="CheckedItemType"/>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="CheckedItemResponse">
            <xs:annotation>
               <xs:documentation>Checked Item response for one sequence of a checked item request.  The response contains weight and size allowances and embargo information</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element minOccurs="0" ref="CheckedItem"/>
                  <xs:element minOccurs="0" name="EmbargoInformation">
                     <xs:annotation>
                        <xs:documentation>Indicates that a checked item that would have been offered is embargoed, and the reason for the embargo.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" minOccurs="0" name="EmbargoText">
                              <xs:annotation>
                                 <xs:documentation>Language based text describing the embargo reason.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element minOccurs="0" name="LongText" type="xs:string"/>
                                 </xs:sequence>
                                 <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
                                 <xs:attribute name="ShortText" type="xs:string" use="required"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="Embargoed" type="xs:boolean" use="required">
                           <xs:annotation>
                              <xs:documentation>If TRUE indicates that the checked item type in this response if embargoed.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="EmbargoedReasonCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>A unique code indicating the reasons for embargo.  This code is typically filled by the embargo rule.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="BaggageAllowance">
                     <xs:annotation>
                        <xs:documentation>The baggage allowance</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="BaggageAllowancePieces" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>The number of pieces allowed for this checked point</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="BaggageAllowanceWeight" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>The weight limit allowed for this checked point</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="BaggageAllowanceWeightUnit" use="optional">
                           <xs:annotation>
                              <xs:documentation>The unit (Kg or Lb) that the weight limit is in.</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="KILO"/>
                                 <xs:enumeration value="POUND"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="CheckedPointType">
            <xs:annotation>
               <xs:documentation>The valid from and to locations and dates of a checked point of travel</xs:documentation>
            </xs:annotation>
            <xs:attribute name="CheckInOrigin" type="xs:string" use="required"/>
            <xs:attribute name="CheckInDepartDate" type="xs:date" use="required"/>
            <xs:attribute name="CheckInDestination" type="xs:string" use="required"/>
            <xs:attribute name="CheckInOperatingCarrier" type="xs:string" use="optional"/>
            <xs:attribute name="CheckinMarketingCarrier" type="xs:string" use="optional"/>
            <xs:attribute name="ArrivingInOperatingCarrier" type="xs:string" use="optional"/>
            <xs:attribute name="IntlDomConnectionType" use="optional">
               <xs:simpleType>
                  <xs:restriction base="xs:string">
                     <xs:enumeration value="I"/>
                     <xs:enumeration value="B"/>
                     <xs:enumeration value="D"/>
                     <xs:enumeration value="Y"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
         </xs:complexType>
         <xs:element name="CheckedPoint">
            <xs:annotation>
               <xs:documentation>The portion of travel a checked item pricing pertains to.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="CheckedPointType">
                     <xs:sequence>
                        <xs:element ref="CheckedItemResponse"/>
                     </xs:sequence>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
      </xs:schema>
