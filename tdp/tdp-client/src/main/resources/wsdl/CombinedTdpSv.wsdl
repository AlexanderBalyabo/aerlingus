                                    <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                                       <xs:annotation>
                                          <xs:documentation>If amount to apply  is not used this is the percentage of the component price to charge for the tax / fee.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="ServiceFee" type="xs:boolean" use="optional">
                                       <xs:annotation>
                                          <xs:documentation>If TRUE this is a service fee and not a governmental tax.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                 </xs:complexType>
                              </xs:element>
                              <xs:element minOccurs="0" ref="ns67:Provider"/>
                           </xs:sequence>
                           <xs:attribute name="SellingPrice" type="xs:decimal" use="required">
                              <xs:annotation>
                                 <xs:documentation>Retail selling price of this unit increment.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="SellingPriceCurrencyCode" type="xs:string" use="required">
                              <xs:annotation>
                                 <xs:documentation>Selling price currency code.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="RedemptionMiles" type="xs:decimal" use="optional">
                              <xs:annotation>
                                 <xs:documentation>The number of redemption miles.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="OverridePrice" type="xs:decimal" use="optional">
                              <xs:annotation>
                                 <xs:documentation>Override to selling price as stated in a valid price availability message.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="OverridePriceCurrencyCode" type="xs:string" use="optional">
                              <xs:annotation>
                                 <xs:documentation>Override price currency code.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="OverrideInternalAgentSequence" type="xs:positiveInteger" use="optional">
                              <xs:annotation>
                                 <xs:documentation>Reservation internal agent who made the price override.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="OverrideReasonCode" type="xs:string" use="optional">
                              <xs:annotation>
                                 <xs:documentation>User defined reason code for price override.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="OverrideReasonText" type="xs:string" use="optional">
                              <xs:annotation>
                                 <xs:documentation>User defined text reason for the price override.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="OverrideDateTime" type="xs:dateTime" use="optional">
                              <xs:annotation>
                                 <xs:documentation>Date and time of the price override.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="PaidInCurrencyAmount" type="xs:decimal" use="optional">
                              <xs:annotation>
                                 <xs:documentation>The amount to be paid in the provider's currency.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="PaidInCurrencyCode" type="xs:string" use="optional">
                              <xs:annotation>
                                 <xs:documentation>The provider's (i.e. original quote) currency.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                           <xs:attribute name="PaidInCurrencyRate" type="xs:decimal" use="optional">
                              <xs:annotation>
                                 <xs:documentation>The exchange rate from the selling price currency to the provider's currency.</xs:documentation>
                              </xs:annotation>
                           </xs:attribute>
                        </xs:complexType>
                     </xs:element>
                  </xs:sequence>
                  <xs:attribute name="PolicyCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Unique policy code.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="PolicyTypeCode" use="required">
                     <xs:annotation>
                        <xs:documentation>Policy type. Current values are;
GUARANTEE -- Reservation payment guarantee requirements.
CANCELLATION -- Reservation cancellation penalties.
DEPOSIT - Reservation payment requirements.
NOSHOWPENALTY -- Penalties for no-show.
SHORTSTAYPENALTY -- Early check-out penalty.
BOOKINGCHANGE -- Booking change fees.
PARTUSEDREFUND -- Refund fees for partial use of a ticket.
UNUSEDREFUND - Penalties on a full refund.
PAYMENT - The Payment policy.
PREPAY - Policy for PREPAY.
POSTPAY - Policy for POSTPAY.</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="GUARANTEE"/>
                           <xs:enumeration value="CANCELLATION"/>
                           <xs:enumeration value="DEPOSIT"/>
                           <xs:enumeration value="NOSHOWPENALTY"/>
                           <xs:enumeration value="SHORTSTAYPENALTY"/>
                           <xs:enumeration value="BOOKINGCHANGE"/>
                           <xs:enumeration value="PARTUSEDREFUND"/>
                           <xs:enumeration value="UNUSEDREFUND"/>
                           <xs:enumeration value="TICKET_TIME_LIMIT"/>
                           <xs:enumeration value="PAYMENT"/>
                           <xs:enumeration value="PREPAY"/>
                           <xs:enumeration value="POSTPAY"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute default="NONE" name="PolicyRetributionActionTypeCode" use="optional">
                     <xs:annotation>
                        <xs:documentation>Retribution for violating a deposit policy deadline. Current values are:
CANCEL -- Booking will be canceled.
RELEASE -- Inventory will be released.
NONE -- No retribution.</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="CANCEL"/>
                           <xs:enumeration value="RELEASE"/>
                           <xs:enumeration value="NONE"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="PolicyTypeDescription" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Text description of the reservation policy.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="AmountToApply" type="xs:decimal" use="optional">
                     <xs:annotation>
                        <xs:documentation>Amont of a penalty or deposit.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="MilesAmountToApply" type="xs:decimal" use="optional">
                     <xs:annotation>
                        <xs:documentation>Amont miles of a penalty or deposit.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="CurrencyCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Currency of deposit or penalty.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                     <xs:annotation>
                        <xs:documentation>Percentage of reservation value to pay for deposit or cancellation, if AmountToApply is not cacluated.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="MilesPercentToApply" type="xs:decimal" use="optional">
                     <xs:annotation>
                        <xs:documentation>Percentage of reservation value to pay for deposit or cancellation, if MilesAmountToApply is not cacluated.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="DurationBasisTypeCode" use="optional">
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="FULL_DURATION"/>
                           <xs:enumeration value="LAST_UNIT"/>
                           <xs:enumeration value="FIRST_UNIT"/>
                           <xs:enumeration value="FIRST_LAST_UNIT"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="AssessmentBasisUOMCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>If penalty or deposit is based on a number of days (units) the unit of measure code valid in the M3 database.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="AssessmentBasisUOMQuantity" type="xs:integer" use="optional">
                     <xs:annotation>
                        <xs:documentation>If penalty or deposit is based on a number of days (units) the number of units of measure.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="AbsoluteDeadlineDateTime" type="xs:dateTime" use="optional">
                     <xs:annotation>
                        <xs:documentation>Absolute last cancellation or deposit date.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="RelativeDeadlineTimeOfDay" type="xs:time" use="optional">
                     <xs:annotation>
                        <xs:documentation>If a deposit or penalty date is based on an event such as booking or first consumption, this is the time of day to use when calculating the absolute date and time.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="RelativeDeadlineOffsetUOMCode" use="optional">
                     <xs:annotation>
                        <xs:documentation>If a deposit or penalty date is based on an event such as booking or first consumption, this is the base unit of measure, such as days, to use when calculating the absolute date and time.</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="DAYS"/>
                           <xs:enumeration value="HOURS"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="RelativeOffsetQuantity" type="xs:integer" use="optional">
                     <xs:annotation>
                        <xs:documentation>If a deposit or penalty date is based on an event such as booking or first consumption, this is the number of base units of measure to use when calculating the absolute date and time.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="RelativeTimeFrameCode" use="optional">
                     <xs:annotation>
                        <xs:documentation>If a deposit or penalty date is based on an event such as booking or first consumption, this is the indication of which event to use in calculating the absolute date and time.  Current values are:

PRIOR_TO_ARRIVAL
AFTER_BOOKING</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="PRIOR_TO_ARRIVAL"/>
                           <xs:enumeration value="AFTER_BOOKING"/>
                           <xs:enumeration value="AT_BOOKING"/>
                           <xs:enumeration value="ON_ARRIVAL"/>
                           <xs:enumeration value="ON_DEPARTURE"/>
                           <xs:enumeration value="AFTER_TRAVEL_COMMENCED"/>
                           <xs:enumeration value="IROP_AFTER_TRAVEL_COMMENCED"/>
                           <xs:enumeration value="SCHEDULE_CHANGE_AFTER_TRAVEL_COMMENCED"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="AssessmentBasis" use="optional">
                     <xs:annotation>
                        <xs:documentation>If calculating a deposit or penalty based on a percentage, this is the basis.  Current values are

GROSS -- Full amount of the booking including all taxes.
NET -- Booking less taxes.
NET_NET -- Net wholesale cost of the booking (booking less taxes less commissions).</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="GROSS"/>
                           <xs:enumeration value="NET"/>
                           <xs:enumeration value="COST"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="TimeZone" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Time zone of the deadline.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="LanguageCode" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Language code of the text description of the policy.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Text" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Policy text.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="GMTOffsetMinutes" type="xs:integer" use="optional">
                     <xs:annotation>
                        <xs:documentation>GMT Offset of deadline if Time zone is not used.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ProviderAssessed" type="xs:boolean" use="optional">
                     <xs:annotation>
                        <xs:documentation>Indicates this penalty is assesed by the supplier rather than the seller.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="PolicyID" type="xs:ID" use="optional"/>
                  <xs:attribute name="EventSequence" type="xs:positiveInteger" use="optional"/>
                  <xs:attribute name="EventDocRef" type="xs:IDREF" use="optional"/>
                  <xs:attribute name="FormOfPaymentTypeCode" type="FormOfPaymentTypeCodeEnum" use="optional"/>
                  <xs:attribute name="ConsumerPaysProvider" type="xs:boolean" use="optional"/>
                  <xs:attribute default="FareInfoLevel" name="ApplicationLevel" use="optional">
                     <xs:annotation>
                        <xs:documentation>This attribute determines the level at which to apply the penalty in the policy.</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="FareInfoLevel">
                              <xs:annotation>
                                 <xs:documentation>FareInfo level means that penalties defined apply per FareInfo level change of the ticket - 2 fareinfo changes mean 2 penalties, even if they both occur at one time.</xs:documentation>
                              </xs:annotation>
                           </xs:enumeration>
                           <xs:enumeration value="TicketLevel">
                              <xs:annotation>
                                 <xs:documentation>TicketLevel means you need to find one policy across all the Units being changed to determine the Penalties for this change. Policies are still tied to the unit, but the correct unit policy must be determined for the penalty fee.
For Frontier Airlines, they charge a penalty fee per change. So if both directions are changed at the same time, then the application sequence below is used to determine the unit policy to apply.</xs:documentation>
                              </xs:annotation>
                           </xs:enumeration>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ApplicationPriority" type="xs:integer" use="optional">
                     <xs:annotation>
                        <xs:documentation>This determines a priority in which the penalty is applied. If there is more than one policy, this attribute is used to find which one is the one to use.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute default="None" name="ResidualValueApplication" use="optional">
                     <xs:annotation>
                        <xs:documentation>This is an attribute to describe what to do with the residual value of the ticket being changed.</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="None">
                              <xs:annotation>
                                 <xs:documentation>This means there is no residual value of the ticket. This is the default value.</xs:documentation>
                              </xs:annotation>
                           </xs:enumeration>
                           <xs:enumeration value="PenaltyOnly">
                              <xs:annotation>
                                 <xs:documentation>This means the residual value of the ticket can be used to pay the value of the penalty fee implied. Any remaining residual value is lost.</xs:documentation>
                              </xs:annotation>
                           </xs:enumeration>
                           <xs:enumeration value="FullValue">
                              <xs:annotation>
                                 <xs:documentation>This means any remaining value after Fare Difference and Penalties are covered needs to be refunded/credited.</xs:documentation>
                              </xs:annotation>
                           </xs:enumeration>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute default="false" name="GuaranteeNonChangingFares" type="xs:boolean" use="optional">
                     <xs:annotation>
                        <xs:documentation>Boolean for where airlines create a policy that they'll guarantee an existing fare if the customer is not changing that portion of the overall ticketed trip. Default is false.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="TicketingMode" use="optional">
                     <xs:annotation>
                        <xs:documentation>Ticketing Mode. Possible values are:
							REVALIDATE -- the ticket should be re-validated;
							REISSUE -- re-pricing should be done using historical fares/taxes and the ticket exchanged;
							EXCHANGE -- re-pricing should be done using current fares/taxes and the ticket exchanged;
							QMANUAL -- the ticket should be queued.</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="REVALIDATE"/>
                           <xs:enumeration value="REISSUE"/>
                           <xs:enumeration value="EXCHANGE"/>
                           <xs:enumeration value="QMANUAL"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="AllowOverbook" type="xs:boolean" use="optional"/>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:element name="Policy">
            <xs:annotation>
               <xs:documentation>This element contains policy information for cancellation, deposit, and complience policies. The element is used to track the date/time of a policy and the retribution (penalty, or required payment)</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="PolicyType"/>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="InternalStatusType">
            <xs:annotation>
               <xs:documentation>The types that the Internal Status of a reservation component can be</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="ACQUIRED_FROM_HOST"/>
               <xs:enumeration value="BOOKED"/>
               <xs:enumeration value="CANCELLED"/>
               <xs:enumeration value="CHANGED"/>
               <xs:enumeration value="PENDING_BOOKING"/>
               <xs:enumeration value="PENDING_PRICE_CHANGE"/>
               <xs:enumeration value="PENDING_REMOTE_SYNCH"/>
               <xs:enumeration value="PENDING_SCHED_CHANGE"/>
               <xs:enumeration value="THIRD_PARTY_BOOKED"/>
               <xs:enumeration value="THIRD_PARTY_PENDING_BOOKING"/>
               <xs:enumeration value="THIRD_PARTY_CANCELLED"/>
               <xs:enumeration value="TRANSFERRED"/>
               <xs:enumeration value="THIRD_PARTY_CANCELLED"/>
               <xs:enumeration value="THIRD_PARTY_CHANGED"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="SellingPromotion">
            <xs:annotation>
               <xs:documentation>A detail of selling promotions applied to a reservation component.  Multiple promotions may be applied to a single unit of measure or all units</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="Code" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Selling promotion code valid in the M3 database.  Also known as the internal promotion code.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AmountToApply" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Amount of price adjustment to apply.  THIS IS ONLY SET WHEN A FLAT AMOUNT DISCOUNT IS IN USE.  DO NOT USE FOR PERCENTAGE DISCOUNTS.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="RedemptionAmountToApply" type="xs:nonNegativeInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>The total amount of redemption discount (miles) to apply.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Percentage of the unit price to adjust.  Only used for percentage based discounts.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AmountApplied" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Total amount of price adjustments applied on all units of measure of this promotion. This is set for both percentage based and flat amount discounts. It is the final discount amount.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="RedemptionAmountApplied" type="xs:nonNegativeInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Total amount of redemption discounts (miles) applied on all units of measure of this promotion. It is the final discount amount.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute default="true" name="PromotionDisplayed" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>If TRUE, then display this promotion.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ApplyPerNthMultipleOfUOM" type="xs:integer" use="optional">
                        <xs:annotation>
                           <xs:documentation>The number of incremental unit discounts allowed.  For example, total number of free days that can be given.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ShortName" type="xs:string" use="optional"/>
                     <xs:attribute name="ShortDescription" type="xs:string" use="optional"/>
                     <xs:attribute name="LongName" type="xs:string" use="optional"/>
                     <xs:attribute name="LongDescription" type="xs:string" use="optional"/>
                     <xs:attribute name="ApplyOnEachNthUOM" type="xs:integer" use="optional">
                        <xs:annotation>
                           <xs:documentation>If promotional price adjustment applies only on each increment of this number of units of measure,  for example, 7th day free, this value would be 7.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SellingPromotionDocID" type="xs:ID" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique ID supplied by the client when adding a selling promotion and sub-elements in the same request.  The sub-elements will reference this ID in order to relate the two in the database insert transaction.  When adding subsequent sub-elements the client only need reference the sequence of the selling promotion.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SellingPromotionType" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The description of different selling promotion type code. For example, COUPON.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ExternalPromotionCode" type="xs:string">
                        <xs:annotation>
                           <xs:documentation>If a promotion code was supplied by a user, it is stored here.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CertificateCode" type="xs:string">
                        <xs:annotation>
                           <xs:documentation>If a certificate code was supplied by a user, it is supplied here.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CertificateNumber" type="xs:string" use="optional"/>
                     <xs:attribute name="ResvCompCompTypeCode" type="xs:string" use="optional"/>
                     <xs:attribute name="ResvCompInternalStatus" type="InternalStatusType" use="optional"/>
                     <xs:attribute name="ResvCompCreationDateTime" type="xs:dateTime" use="optional"/>
                     <xs:attribute name="ResvHistoriedDateTime" type="xs:dateTime" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="CountryQuote">
            <xs:annotation>
               <xs:documentation>Quote by TDP Country Code and Data of Travel</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="StartCountryCode" type="xs:string" use="required"/>
               <xs:attribute name="StartCountryTravelDate" type="xs:dateTime" use="required"/>
               <xs:attribute name="EndCountryCode" type="xs:string" use="optional"/>
               <xs:attribute name="EndCountryTravelDate" type="xs:dateTime" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="LocationQuote">
            <xs:annotation>
               <xs:documentation>Quote by TDP Location Code and Type and Date of Travel</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="StartLocationCode" type="xs:string" use="required"/>
               <xs:attribute name="StartLocationCodeType" type="xs:string" use="required"/>
               <xs:attribute name="StartTravelDate" type="xs:dateTime" use="required"/>
               <xs:attribute name="EndLocationCode" type="xs:string" use="optional"/>
               <xs:attribute name="EndLocationCodeType" type="xs:string" use="optional"/>
               <xs:attribute name="EndTravelDate" type="xs:dateTime" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="CoveredOptionalAddOns">
            <xs:annotation>
               <xs:documentation>List of optional add-on coverages and value limits.  This element would be available after a previous quote was returned and options were known.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" name="AddOnValue">
                           <xs:annotation>
                              <xs:documentation>List of add on values</xs:documentation>
                           </xs:annotation>
                           <xs:complexType>
                              <xs:complexContent>
                                 <xs:extension base="SequencedType">
                                    <xs:attribute name="AddOnValueCode" type="xs:string" use="optional">
                                       <xs:annotation>
                                          <xs:documentation>The code for this add on value.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="AddOnValueDescription" type="xs:string" use="optional">
                                       <xs:annotation>
                                          <xs:documentation>The description of this add on value.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="AddOnValuePrice" type="xs:decimal" use="optional">
                                       <xs:annotation>
                                          <xs:documentation>The sell amount of this add on value.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="FromCoverageValue" type="xs:decimal" use="optional">
                                       <xs:annotation>
                                          <xs:documentation/>
                                          <xs:documentation>The covered value of this add on value.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="ToCoverageValue" type="xs:decimal" use="optional">
                                       <xs:annotation>
                                          <xs:documentation>The covered value of this add on value.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="CoverageCurrencyCode" type="xs:string" use="optional">
                                       <xs:annotation>
                                          <xs:documentation>The covered value currency code.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="AddOnValueText" type="xs:string" use="optional">
                                       <xs:annotation>
                                          <xs:documentation>The text of this add on value.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="AddOnValueCost" type="xs:decimal" use="optional">
                                       <xs:annotation>
                                          <xs:documentation>The nett amount of this add on value.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                    <xs:attribute name="AddOnValueCostCurrencyCode" type="xs:string" use="optional">
                                       <xs:annotation>
                                          <xs:documentation>The nett currency code for this add on value.</xs:documentation>
                                       </xs:annotation>
                                    </xs:attribute>
                                 </xs:extension>
                              </xs:complexContent>
                           </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                     <xs:attribute name="AddOnCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>The code for this add on.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AddOnDescription" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The description for this add on.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AddOnPrice" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>The sell amount of this add on.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AddOnPriceCurrencyCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The selling currency of this add on.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AllowMultiple" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indicator if multiple is allowed.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="MaxQuantityAllowed" type="xs:int" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indicator if a max quantity is allowed.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AddOnRequiresTextFlag" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>A flag to indicate of a text is required for this add on.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AddOnCost" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>The nett amount of this add on.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AddOnCostCurrencyCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The nett currency of this add on.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="RegionQuote">
            <xs:annotation>
               <xs:documentation>Quote by TDP Region Code and Date Of Travel (Future)</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="StartRegionCode" type="xs:string" use="required"/>
               <xs:attribute name="StartRegionDate" type="xs:dateTime" use="required"/>
               <xs:attribute name="EndRegionCode" type="xs:string" use="optional"/>
               <xs:attribute name="EndRegionDate" type="xs:dateTime" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="InsuranceSegment">
            <xs:annotation>
               <xs:documentation>Contains domain specific information for insurance</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:choice>
                     <xs:element maxOccurs="unbounded" ref="LocationQuote"/>
                     <xs:element maxOccurs="unbounded" ref="RegionQuote"/>
                     <xs:element maxOccurs="unbounded" ref="CountryQuote"/>
                  </xs:choice>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="CoveredOptionalAddOns"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="CoveredBenefit">
                     <xs:annotation>
                        <xs:documentation>List of covered benefits and value limits</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:complexContent>
                           <xs:extension base="SequencedType">
                              <xs:attribute name="BenefitCode" type="xs:string" use="optional"/>
                              <xs:attribute name="BenefitDescription" type="xs:string" use="optional"/>
                              <xs:attribute name="FromCoverageValue" type="xs:decimal" use="optional"/>
                              <xs:attribute name="ToCoverageValue" type="xs:decimal" use="optional"/>
                              <xs:attribute name="CoverageCurrencyCode" type="xs:string" use="optional"/>
                           </xs:extension>
                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="PolicyStartDate" type="xs:dateTime" use="optional">
                  <xs:annotation>
                     <xs:documentation>The start date of the policy.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PolicyEndDate" type="xs:dateTime" use="optional">
                  <xs:annotation>
                     <xs:documentation>The end date of the policy.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TripStartDate" type="xs:dateTime" use="optional">
                  <xs:annotation>
                     <xs:documentation>The trip start date.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TripEndDate" type="xs:dateTime" use="optional">
                  <xs:annotation>
                     <xs:documentation>The trip end date.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TotalTripPrice" type="xs:decimal" use="optional">
                  <xs:annotation>
                     <xs:documentation>The trip price total.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TotalTripPriceCurrencyCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>The trip price currency used.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PolicyType" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>The type of insurance being offered (ONEWAY, ROUNDTRIP or ANNUAL).</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PolicyUrl" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>A link to the insurance policy details.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="BenefitUrl" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>A link to the insurance policy covered benefits.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="InventoryStatusType">
            <xs:annotation>
               <xs:documentation>The types that an inventory status can be in.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:enumeration value="OK"/>
               <xs:enumeration value="UN"/>
               <xs:enumeration value="OR"/>
               <xs:enumeration value="OC"/>
               <xs:enumeration value="WL"/>
               <xs:enumeration value="CX"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="UnitCurrency">
            <xs:annotation>
               <xs:documentation>The currency and rate of exchange at the time of sale.  If the currency code of the price / availability message  is different from the system currency, there will be one element representing the  currency of the price / avail message.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="ConversionDate" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>Date of conversion rate quote.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ConversionRate" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Conversion rate between this currency and the reservation component currency.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CurrencyCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>Currency code valid in the M3 database.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="Amount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>The amount in the original currency.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AmountType" use="required">
                        <xs:annotation>
                           <xs:documentation>This conversion element was targeted against either the SELLING, COST, or OVERRIDE amount attribute of the reservation component unit.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="SELLING"/>
                              <xs:enumeration value="COST"/>
                              <xs:enumeration value="OVERRIDE"/>
                              <xs:enumeration value="COMMOVERRIDE"/>
                              <xs:enumeration value="BASE"/>
                              <xs:enumeration value="EQUIV"/>
                              <xs:enumeration value="ENTITY"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="ContractedExchangeRate" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>If TRUE the conversion rate was contracted and should not be changed per daily rates.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ContractedPrice" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>The rate was derived by a contracted price divided by the unit selling price and is fixed by contract.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="PolicyLink">
            <xs:annotation>
               <xs:documentation>A per base unit policy.  This shows which policies that this particular Unit conforms to.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="PolicySequence" type="xs:positiveInteger" use="optional"/>
               <xs:attribute name="PolicyDocRef" type="xs:IDREF" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="SellingPromotionDetail">
            <xs:annotation>
               <xs:documentation>Base unit of measure detail reference to promotions applied.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="SellingPromotionSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Pointer to selling promotion within this reservation component.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="Amount" type="xs:decimal" use="required">
                        <xs:annotation>
                           <xs:documentation>Amount of the selling promotion attributed to this unit detail.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SellingPromotionDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique reservation component selling promotion.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="TaxFeeDetail">
            <xs:annotation>
               <xs:documentation>Base unit of measure detail reference to taxes and fees paid.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="Amount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Amount of the tax fee for this unit detail breakdown paid to the provider.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ChargeableAmount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>This is the tax amount for unit for this tax code to be invoiced to the customer.  It may vary in amount from the taxes paid (Amount) .</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="TaxFeeSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>A pointer to the Tax Fee of the reservation component.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="TaxFeeDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique tax / fee.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="UnitDetail">
            <xs:annotation>
               <xs:documentation>A per base unit of measure breakdown of the pricing of a reservation component.  Example, for a lodging component it is the pricing for each day's stay.  For a car rental at a weekly rate, this is the daily breakdown.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="TaxFeeDetail"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="SellingPromotionDetail"/>
                     </xs:sequence>
                     <xs:attribute name="StartDateTime" type="xs:dateTime" use="required">
                        <xs:annotation>
                           <xs:documentation>Unit detail start date time.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="EndDateTime" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unit detail end date time.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="UOMCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>Base unit of measure code valid in the M3 database.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="Unit">
            <xs:annotation>
               <xs:documentation>Pricing breakdown in complex units of measure.  If the unit of measure is simple, this is a pricing for one simple unit.  For example, each night of a hotel stay, each week of a car rental.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="UnitCurrency"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ExtraInformation"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="UnitDetail"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="PolicyLink"/>
                     </xs:sequence>
                     <xs:attribute name="StartDateTime" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>Start date/time of this unit increment.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="EndDateTime" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>End date/time of this unit increment.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SellingPrice" type="xs:decimal" use="required">
                        <xs:annotation>
                           <xs:documentation>Retail selling price of this unit increment.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SellingPriceCurrencyCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>Selling price currency code.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverridePrice" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Override to selling price as stated in a valid price availability message.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverridePriceCurrencyCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Override price currency code.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideInternalAgentSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Reservation internal agent who made the price override.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideReasonCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>User defined reason code for price override.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideReasonText" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>User defined text reason for the price override.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideDateTime" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>Date and time of the price override.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="RedemptionMiles" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>The number of redemption miles required to redeem this fare.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="RedemptionCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The code that identifies the redemption award represented by this fare..</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="RedemptionDescription" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>This is the description from the RedemptionAward master table that describes the award represented by this fare.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CostAmount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Wholesale cost of this increment unit to the seller.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CostAmountCurrencyCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Cost currency code.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CommissionAppliedAmount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>If the commission is applied as an amount then give the amount value.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CommissionAppliedPercentage" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>If the commission is applied as a percentage then give the percent value. e.g. (10 = 10%)</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CommissionAppliedEndorsement" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>If there is endorsement text corresponding to the commission applied.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CommissionAppliedCurrencyCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>If there is a currency code corresponding to the commission applied.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CommissionAppliedContractCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Commission contract code in case we need to reapply or revalidate this commission contract at any stage.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CommissionOverrideAmount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Override commission paid by the provider.  Is part of cost (reduction).  To calculate commission percentages: CommissionAmount = ((Sell - Cost) - CommissionOveride).</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CommissionOverrideCurrencyCode" type="xs:string" use="optional"/>
                     <xs:attribute name="AgentNetPrice" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>AgentNet Price.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AgentNetPriceCurrencyCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>AgentNet Price Currency Code.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PricingSource" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indicates for air pricing, BULK, CHARTER, SCHEDULED, REDEMPTION fares.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="BULK"/>
                              <xs:enumeration value="BULK_COMMISSIONABLE"/>
                              <xs:enumeration value="SCHEDULED"/>
                              <xs:enumeration value="CHARTERED"/>
                              <xs:enumeration value="PRIVATE"/>
                              <xs:enumeration value="PRIVATE_NET"/>
                              <xs:enumeration value="REDEMPTION"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="PricingRateCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Pricing rate code or fare basis for this unit increment.  For example the fare basis for inbound flights.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="RateDescription" type="xs:string" use="optional"/>
                     <xs:attribute name="PricingBody" type="xs:string" use="optional"/>
                     <xs:attribute name="UOMCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>Unit of measure code valid in the M3 database.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="QuoteDateTime" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>Date and time quote was issued.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="InventoryStatus" type="InventoryStatusType" use="optional">
                        <xs:annotation>
                           <xs:documentation>If the price availability message invenotry status was overridden this is the inventory status.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideInventoryConfirm" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>If the price availability message invenotry status was overridden this is the confirmation code.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideInventoryDateTime" type="xs:dateTime" use="optional">
                        <xs:annotation>
                           <xs:documentation>If the price availability message invenotry status was overridden this is the date and time.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideInternalAgentDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>If the price availability message invenotry status was overridden this is the client supplied reference to the unique internal agent.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OverrideUser" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>If the price availability message invenotry status was overridden this is the user.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FareStartCityCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>For air fare basis codes this is the start city of the fare.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FareEndCityCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>For air fare basis codes this is the end city of the fare.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FareAirlineCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>For air fare basis codes this is the faring airline.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FareLocationCategoryCode" type="xs:string" use="optional"/>
                     <xs:attribute name="UnitDocID" type="xs:ID" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique ID supplied by the client when adding a unit and sub-elements in the same request.  The sub-elements will reference this ID in order to relate the two in the database insert transaction.  When adding subsequent sub-elements the client only need reference the sequence of the unit.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FareReference" type="xs:string" use="optional"/>
                     <xs:attribute name="FareVersion" type="xs:string" use="optional"/>
                     <xs:attribute name="PricingSystem" type="xs:string" use="optional"/>
                     <xs:attribute name="PlatingCarrier" type="xs:string" use="optional"/>
                     <xs:attribute name="ClosedUserGroupCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Closed user group reference. Needed if the credentials of this closed user group were used to retrieve the original fare quote.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="DirectionIndicator" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indicates the directionality of the fare. e.g. oneway, return, circular, open jaw etc...</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="ONE_WAY"/>
                              <xs:enumeration value="RETURN"/>
                              <xs:enumeration value="CIRCLE"/>
                              <xs:enumeration value="OPEN_JAW"/>
                              <xs:enumeration value="OTHER"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="TicketType" use="optional">
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="PAPER_TICKET"/>
                              <xs:enumeration value="ETICKET"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="SettlementPlan" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Type of settlement plan used..e.g. BlueTicketIndicator</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FareQuoteKey" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Used for Low Cost Carriers to store the key returned in the priced option.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AirFareRulesDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>@deprecated. A reference to the AirFareRules item for this fare.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AirFareRulesSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>@deprecated. The sequence of the AirFareRules item for this fare.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ProviderRulesDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>A reference to the ProviderRules item for this fare.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ProviderRulesSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>The sequence of the ProviderRules item for this fare.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FareFamilyDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>DocRef for the Fare Family element that applies to this unit and possibly to other units in the same direction.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FareFamilySequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>The sequence of the FareFamily which applies to this unit and other units in the same direction.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PriceByFareBasisCode" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>Used to indicate command pricing by fare basis code.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="FareQuoteStatus" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Used to hold any fare quote status value if one is returned by the host system.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="TicketRequired" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>An indicator showing showing a cross ref to associated ticket number of a fare</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="TourCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Identifies a fare for a specific market.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="OriginatingTicketDesignator" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The original Ticket Designator used for matching ATPCO data and for GDS fare retrieval.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ResultingTicketDesignator" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The returned Ticket Designator after processing. May be a Ticket Designator Modifier, or have the TDM appended to the Ticket Designator.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AccountCode" type="xs:string">
                        <xs:annotation>
                           <xs:documentation>The account code used to derive the fare.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="Price">
            <xs:annotation>
               <xs:documentation>Per Unit price breakdown for each Guest</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="TaxFeeDetail"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="SellingPromotionDetail"/>
                     </xs:sequence>
                     <xs:attribute name="Price" type="xs:decimal" use="required">
                        <xs:annotation>
                           <xs:documentation>Selling price in the currency of the reservation component unit.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="NetCost" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Cost in the currency of the reservation component unit.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="UnitSequence" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation>Sequence of the unit this guest breakdown is for.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="UnitDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique reservation component unit.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="TaxFee">
            <xs:annotation>
               <xs:documentation>Contains information about taxes and fees applied to the reservation component.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="TaxFeeDocID" type="xs:ID" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique ID supplied by the client when adding a TaxFee and sub-elements in the same request.  The sub-elements will reference this ID in order to relate the two in the database insert transaction.  When adding subsequent sub-elements the client only need reference the sequence of the TaxFee.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="Amount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Tax Fee total amount to be remitted to the provider.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ChargeableAmount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>This is the total tax amount for this tax code to be invoiced to the customer.  It may vary in amount from the taxes paid (Amount) .</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="Description" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Tax Fee description.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AmountToApply" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Amount to apply per base UOM.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PerBaseUOM" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>If TRUE applies to each base unit of measure.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PerPerson" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>If TRUE applies to each guest on the component.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ServiceFee" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>If TRUE this is a service fee and not a governmental tax.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="TaxFeeCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Unique Tax or Fee code valid in M3 database.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>If amount to apply  is not used this is the percentage of the component price to charge for the tax / fee.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="CurrencyCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Currecny code of the tax / fee.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="TaxCollectTypeCode" use="optional">
                        <xs:annotation>
                           <xs:documentation>TaxCollectionType	Indicates taxes are assessed by the Seller or Vendor.  Current values are:Included -- Included in Rate.UponArrival -- Taxes paid on arrival to taxing body.PreCollected -- Paid to the seller before departure.PropertyCollected -- Collected by vendor.</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="INCLUDED"/>
                              <xs:enumeration value="UPONARRIVAL"/>
                              <xs:enumeration value="PRECOLLECTED"/>
                              <xs:enumeration value="PROPERTYCOLLECTED"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="TaxCollectTypeDescription" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Description of tax collection.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="IsVAT" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>Whether or not this tax fee is a VAT amount.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="IsPartOfBaseFare" type="xs:boolean" use="optional">
                        <xs:annotation>
                           <xs:documentation>Whether or not this tax fee is a part of base fare.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="TaxFeeDirection" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indicates the taxFee direction for Bifa OneWayCombinable Fares.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="AirportCityCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Indicates the airport city code for this tax fee</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="BaseAmount" type="xs:decimal" use="optional">
                        <xs:annotation>
                           <xs:documentation>Original base amount of the taxfee.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="BaseCurrency" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Currency code of the base amount.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="SpecialRequestType"/>
         <xs:element name="GeneralRemark">
            <xs:annotation>
               <xs:documentation>A General Remark is a plain-text human-readable string added to the reservation.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="OtherServiceInformation">
            <xs:annotation>
               <xs:documentation>OSIs hold machine-readable information that should be picked up by the applicable carrier.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="AirlineCode" type="xs:string" use="optional"/>
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                     <xs:attribute name="Status" type="xs:string" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="RemoteSynchStatusType">
            <xs:restriction base="xs:string">
               <xs:enumeration value="PENDING_ADD"/>
               <xs:enumeration value="PENDING_REMOVE"/>
               <xs:enumeration value="PENDING_UPDATE"/>
               <xs:enumeration value="HOST_REMOVE"/>
               <xs:enumeration value="HOST_ADD"/>
               <xs:enumeration value="HOST_MODIFY"/>
               <xs:enumeration value="SYNCHED"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="SpecialServiceRequest">
            <xs:annotation>
               <xs:documentation>Special Service Request</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="Code" type="xs:string" use="required"/>
                     <xs:attribute name="Text" type="xs:string" use="optional"/>
                     <xs:attribute name="AirlineCode" type="xs:string" use="optional"/>
                     <xs:attribute name="Status" type="xs:string" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="UniqueRemark">
            <xs:annotation>
               <xs:documentation>@deprecated Unique Remark</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                     <xs:attribute name="UniqueRemarkCode" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="TicketingRemark">
            <xs:annotation>
               <xs:documentation>A Ticketing Remark is a remark normally printed onto a ticket.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="PhoneRemark">
            <xs:annotation>
               <xs:documentation>A Phone Remark contains additional phone numbers to add to a host.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="RemoteRefType">
            <xs:complexContent>
               <xs:extension base="SequencedType">
                  <xs:attribute name="Name" type="xs:string" use="required"/>
                  <xs:attribute name="Value" type="xs:string" use="required"/>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:element name="RemoteRef" type="RemoteRefType">
            <xs:annotation>
               <xs:documentation>Name Value pair used to link the parent element to an entity in a remote system.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="PassiveInsuranceSegment">
            <xs:annotation>
               <xs:documentation>A formatted remark for insurance segment which sends no message to any vendor.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="AirlineCode" type="xs:string" use="optional"/>
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="CustomerRemark">
            <xs:annotation>
               <xs:documentation>A Customer Remark is a remark used to associate a PNR with related customer details.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="CustomerID">
            <xs:annotation>
               <xs:documentation>A Customer ID allows customers place a specific identification codes onto a host system.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="VendorRemark">
            <xs:annotation>
               <xs:documentation>A Vendor Remark contains remark details specific to a particular vendor.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="VendorCode" type="xs:string" use="required"/>
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="AccountingRemark">
            <xs:annotation>
               <xs:documentation>An accounting Remark is used for back-office referencing.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="ItineraryRemark">
            <xs:annotation>
               <xs:documentation>An Itinerary Remark is a remark used to add the remark.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SpecialRequestType">
                     <xs:attribute name="Text" type="xs:string" use="required"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="SpecialRequest">
            <xs:annotation>
               <xs:documentation>@deprecated.  Use Remark instead. Details of Special Requests which included itinerary remarks, SSR's and OSI's etc...</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:choice>
                           <xs:element ref="AccountingRemark"/>
                           <xs:element ref="CustomerRemark"/>
                           <xs:element ref="ItineraryRemark"/>
                           <xs:element ref="OtherServiceInformation"/>
                           <xs:element ref="PhoneRemark"/>
                           <xs:element ref="GeneralRemark"/>
                           <xs:element ref="SpecialServiceRequest"/>
                           <xs:element ref="TicketingRemark"/>
                           <xs:element ref="UniqueRemark"/>
                           <xs:element ref="VendorRemark"/>
                           <xs:element ref="CustomerID"/>
                           <xs:element ref="PassiveInsuranceSegment"/>
                        </xs:choice>
                        <xs:element minOccurs="0" ref="RemoteRef"/>
                     </xs:sequence>
                     <xs:attribute name="SyncStatus" type="RemoteSynchStatusType" use="optional"/>
                     <xs:attribute default="true" name="SendToVendor" type="xs:boolean" use="optional"/>
                     <xs:attribute name="ShowOnInvoice" type="xs:boolean" use="optional"/>
                     <xs:attribute name="ShowOnConfirmation" type="xs:boolean" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="GroupMember">
            <xs:annotation>
               <xs:documentation>Contains a reservation component that is a member of a reservation group.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:attribute name="ResComponentSequence" type="xs:positiveInteger" use="optional"/>
                     <xs:attribute name="PrimaryComponent" type="xs:boolean" use="optional"/>
                     <xs:attribute name="PackageTemplateSequenceAdvertisedComponentSequence" type="xs:positiveInteger" use="optional"/>
                     <xs:attribute name="ResComponentDocRef" type="xs:IDREF" use="optional">
                        <xs:annotation>
                           <xs:documentation>Client supplied reference to the unique reservation component.  This is only needed when inserting the parent and child elements in the same request.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="Remark">
            <xs:annotation>
               <xs:documentation>Details of Remarks. The SpecialServiceRequest and OtherServiceInformation elements are no longer referenced by the Remark element. These two elements are referenced by  AirSupplementaryElement and need to be removed from the Remark element.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:choice>
                           <xs:element ref="AccountingRemark"/>
                           <xs:element ref="CustomerRemark"/>
                           <xs:element ref="ItineraryRemark"/>
                           <xs:element ref="OtherServiceInformation"/>
                           <xs:element ref="PhoneRemark"/>
                           <xs:element ref="GeneralRemark"/>
                           <xs:element ref="SpecialServiceRequest"/>
                           <xs:element ref="TicketingRemark"/>
                           <xs:element ref="UniqueRemark"/>
                           <xs:element ref="VendorRemark"/>
                           <xs:element ref="CustomerID"/>
                           <xs:element ref="PassiveInsuranceSegment"/>
                        </xs:choice>
                        <xs:element minOccurs="0" ref="RemoteRef"/>
                     </xs:sequence>
                     <xs:attribute name="SyncStatus" type="RemoteSynchStatusType" use="optional"/>
                     <xs:attribute default="true" name="SendToVendor" type="xs:boolean" use="optional"/>
                     <xs:attribute name="ShowOnInvoice" type="xs:boolean" use="optional"/>
                     <xs:attribute name="ShowOnConfirmation" type="xs:boolean" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="AirSupplementaryElement">
            <xs:annotation>
               <xs:documentation>Details of  SSR's and OSI's which can be added to an AIR reservation etc...</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:choice>
                        <xs:element ref="SpecialServiceRequest"/>
                        <xs:element ref="OtherServiceInformation"/>
                        <xs:element ref="PassiveInsuranceSegment"/>
                     </xs:choice>
                     <xs:attribute name="SyncStatus" type="RemoteSynchStatusType" use="optional"/>
                     <xs:attribute default="true" name="SendToVendor" type="xs:boolean" use="optional"/>
                     <xs:attribute name="ShowOnInvoice" type="xs:boolean" use="optional"/>
                     <xs:attribute name="ShowOnConfirmation" type="xs:boolean" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="ResComponentGroup">
            <xs:annotation>
               <xs:documentation>A Container that allows one or more ResComponents to be grouped together. A ResComponent may belong to more than one group.  Reservation component groups indicate such things as components that must be booked in a remote system together or components of a package.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ExtraInformation"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="GroupMember"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="SpecialRequest"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="Remark"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="AirSupplementaryElement"/>
                     </xs:sequence>
                     <xs:attribute name="Description" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Free text description of this Reservation Component Group</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="GroupTypeCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Code indicating the type of Group</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PackageCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Code indicating the Package which this Group belongs to.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PackageDescription" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Description of the Package which this Group belongs to.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PackageTemplateHeaderCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Package Template which this Group belongs to.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PackageHistorySequence" type="xs:positiveInteger" use="optional"/>
                     <xs:attribute name="ResComponentGroupDocID" type="xs:ID">
                        <xs:annotation>
                           <xs:documentation>Unique ID supplied by the client when adding a reservation group and sub-elements  in the same request.  The sub-elements will reference this ID in order to relate the two in the database insert transaction.  When adding subsequent sub-elements the client only need reference the sequence of the resComponentGroup.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="InternalGroupTypeCode" use="required">
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="CHG_GROUP">
                                 <xs:annotation>
                                    <xs:documentation>Ties a previously booked component together with a changed to component to indicate that a change should be sent to the external system.  The previously booked component is always flagged as the primary component in the GroupMembers element.  Once the change is processed on the remote system, the previously booked component's InternalStatus is changed to CANCELLED.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="SELL_TOGETHER">
                                 <xs:annotation>
                                    <xs:documentation>Ties two components together such as a room and a meal.  Indicates that two components will be reserved in the same remote reservation on the remote system.  The booking servant resolves the booking adaptor for each of the components.  If they are different, an error is thrown and the booking process does not proceed. Once components in a SELL_TOGETHER group are booked, they must be cancelled together or changed together as the case may be.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="SELL_IN_SEQUENCE">
                                 <xs:annotation>
                                    <xs:documentation>Normally components selected to be booked across multiple BookRQ elements are processed in parallel.  Components in s SELL_IN_SEQUENCE group are processed in order of their group member sequence.  A subsequent member of a group cannot be booked on the remote sequence until the previous group member booking completes normally.  I any member fails, the process is stopped and it is up to the client to cancel previously booked components or not.  Once booked, the primary (depended on) component can not be cancelled without cancelling the dependent component first.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="PENALTY">
                                 <xs:annotation>
                                    <xs:documentation>When a component is changed or cancelled, penalties may be assessed based on deposit, cancellation, and change rules.  When the client assesses a penalty, it can tie the penalty to the penalized component in a PENALTY group.  The primary component(s) are the originally booked component.  The penalty component is subsequent to the originally booked component.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="ADJUSTMENT">
                                 <xs:annotation>
                                    <xs:documentation>An adjustment component may be entered into a reservation to adjust the price of one or more components.  An ADJUSTMENT group is used to tie the adjusted component(s) to the ajustment component.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="PACKAGE">
                                 <xs:annotation>
                                    <xs:documentation>A package component group ties all components of a package together and points back to the M3 packaging rules associated with the sale.  This allows the package validation utility to be invoked when desired changes or cancellations are made to a package.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="PACKAGE_CHANGE_GROUP">
                                 <xs:annotation>
                                    <xs:documentation>A package change group ties a component that is booked with its replacement in the package.  This allows the exchanging of mandatory elements.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="USER">
                                 <xs:annotation>
                                    <xs:documentation>A USER group indicates a group that is not controlled by the reservation system, but is intended for use by the user.  When this mode is used, the field 'GroupTypeCode' should be used to indicate what kind of user-group this is.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="REMARKS">
                                 <xs:annotation>
                                    <xs:documentation>So we can group remarks that belong to a particular group of res components without affecting re-book/cancel operations.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="ANCILLARY_AIR_COMP">
                                 <xs:annotation>
                                    <xs:documentation>Used to group AAC ResComponents</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="INSURANCE_ADDONS">
                                 <xs:annotation>
                                    <xs:documentation>Used to group Insurance component and covered optional add ons component together</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                              <xs:enumeration value="ANCILLARY_AIR_COMP_BUNDLE">
                                 <xs:annotation>
                                    <xs:documentation>Used to group all the AAC bundle members for a bundled parent AAC.</xs:documentation>
                                 </xs:annotation>
                              </xs:enumeration>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
