               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="SecureID" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Indicates if the itinerary to be fulfilled is ETicketed or not. This is important as the process used for fulfilling an ETicked itinerary is completely different to that for a paper ticket.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="CreditCardAuthorizationDateTime" type="xs:dateTime"/>
                  <xs:element minOccurs="0" name="CreditCardAuthorizationCode" type="xs:string"/>
                  <xs:element minOccurs="0" name="CreditCardAuthorizationSystem" type="xs:string"/>
                  <xs:element minOccurs="0" name="AuthorizationCode" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>This is the authorisation code provided by a 3rd party payment system indicating a particular transaction has been authorised on this card.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="CardVerificationCode" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>The CVC code of the credit card. This is the 3 or 4 digit code found on the back of major credit cards.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
