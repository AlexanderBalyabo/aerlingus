                                 <xs:sequence>
                                    <xs:element minOccurs="0" ref="ns20:RedemptionAttribs"/>
                                 </xs:sequence>
                                 <xs:attribute name="TaxCode" type="StringLength1to16" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Tax Code</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="TaxName" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Name of the tax</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="TaxCountryCode" type="AlphaLength1to2" use="required">
                                    <xs:annotation>
                                       <xs:documentation>The IATA 2 letter country code that levies the tax</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="AirportCityCode" type="xs:string" use="optional"/>
                                 <xs:attribute name="IntDomAssessment" type="xs:string" use="optional"/>
                                 <xs:attribute name="TaxSequence" type="xs:int" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The Tax sequence from the breakdown.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="TaxBox" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The 2 letter Tax Box code used to identify the tax grouping (T1, T2, XT).</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="IsVAT" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>This is an included tax, meaning this tax is included in the charge amount.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="IsSubjectToVAT" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>This is an included tax, meaning this tax is included in the charge amount.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>If amount to apply is not used this is the percentage of the component price to charge for the tax/fee.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="BaseAmount" type="xs:decimal" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Original base amount of the tax.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="BaseCurrency" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Currency code of the base amount.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="FeeSupplementalInfos">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="FeeSupplementalInfo">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element minOccurs="0" ref="ns20:RedemptionAttribs"/>
                                 </xs:sequence>
                                 <xs:attribute name="FeeCode" type="StringLength1to16" use="required"/>
                                 <xs:attribute name="FeeSubCode" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The sub code of the fee.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="FeeName" type="xs:string" use="required"/>
                                 <xs:attribute name="FeeAirlineCode" type="StringLength1to16" use="optional"/>
                                 <xs:attribute name="FeeAppID" type="xs:int" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The application ID of the fee.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="Amount" type="xs:decimal" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The total fee amount.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="CurrencyCode" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The currency of the fee amount.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="IsTaxable" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Indicates if the amount of the fee is taxable.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="AirportCityCode" type="xs:string" use="optional"/>
                                 <xs:attribute name="IntDomAssessment" type="xs:string" use="optional"/>
                                 <xs:attribute name="TaxSequence" type="xs:int" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The Tax sequence from the breakdown.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="TaxBox" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The 2 letter Tax Box code used to identify the fees grouping (T1, T2, XT) in a tax breakdown.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="IsPartOfBaseFare" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                       <xs:documentation source="Indicates whether this Fee is part of the base fare or not."/>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>If amount to apply is not used this is the percentage of the component price to charge for the tax/fee.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="NetFare">
                     <xs:annotation>
                        <xs:documentation>The Published Fare less any applicable commission for this inventory</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                        <xs:attribute default="false" name="Updated" type="xs:boolean" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns20:RedemptionFare"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="DiscountApplied">
                     <xs:annotation>
                        <xs:documentation>The Discount Applied to this fare. Attributes Amount, Currency and Decimal Places are all required if this element is present except when it's an Award, Money and Miles or the fare was filed only as private and therefore we cannot work out the discount amount. It will be echoed back in the response unless a price tolerance failure occurs.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" minOccurs="0" name="CombinablePromotion" type="xs:string">
                              <xs:annotation>
                                 <xs:documentation>Promotion that can be combined for discount.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is a record of the percentage value used to calculate the Amount. It will not be set when the discount is based on a flat amount. Instead AmountToApply is set.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AmountToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is the value of a flat amount discount.  DO NOT SET THIS IF DISCOUNT IS PERCENTAGE BASED.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional">
                           <xs:annotation>
                              <xs:documentation>The total amount of discount that was applied to this fare. This is set for both flat amount discounts and percentage based discounts.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RedemptionAmount" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>The total amount of redemption discount (miles) that was applied to this fare.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional">
                           <xs:annotation>
                              <xs:documentation>The 3 letter currency code of the discount amount</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>The number of decimal places the discount is expressed in</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DiscountCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is the internal promotion code.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DiscountDescription" type="xs:string" use="optional"/>
                        <xs:attribute default="true" name="DiscountDisplayed" type="xs:boolean" use="optional"/>
                        <xs:attribute name="CodeContext" type="xs:string" use="optional"/>
                        <xs:attribute name="EndorsementText" type="xs:string" use="optional"/>
                        <xs:attribute name="RoundingRule" use="optional">
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="ROUND_UP"/>
                                 <xs:enumeration value="ROUND_DOWN"/>
                                 <xs:enumeration value="ROUND_CEILING"/>
                                 <xs:enumeration value="ROUND_FLOOR"/>
                                 <xs:enumeration value="ROUND_HALF_UP"/>
                                 <xs:enumeration value="ROUND_HALF_DOWN"/>
                                 <xs:enumeration value="ROUND_HALF_EVEN"/>
                                 <xs:enumeration value="ROUND_UNNECESSARY"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="AppliedPromoCodes" type="xs:string" use="optional"/>
                        <xs:attribute name="ExternalPromotionCode" type="xs:string">
                           <xs:annotation>
                              <xs:documentation>If a promotion code was supplied by a user, it is stored here.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CertificateCode" type="xs:string">
                           <xs:annotation>
                              <xs:documentation>If a certificate code was supplied by a user, it is supplied here.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="PromotionReference" type="xs:IDREF">
                           <xs:annotation>
                              <xs:documentation>This is an aid to link a DiscountApplied element to a detailed Promotion element.  It permits direct access to the Promotion element.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CertificateReference" type="xs:IDREF">
                           <xs:annotation>
                              <xs:documentation>This is an aid to link a DiscountApplied element to a detailed Certificate element.  It permits direct access to the Certificate element.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute default="false" name="ApplyDiscount" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>Flag will be reset by BRE if Discount passes the BRE validation.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CertificateNumber" type="xs:string"/>
                        <xs:attribute name="RedemptionPercentToApply" type="xs:decimal">
                           <xs:annotation>
                              <xs:documentation>This is the percentage value to apply as a discount for a promotion which is for miles. The actualy mileage value will be in RedemptionAmount.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="CommissionApplied">
                     <xs:complexType>
                        <xs:attribute name="PercentToApply" type="xs:decimal" use="optional"/>
                        <xs:attribute name="AmountToApply" type="xs:decimal" use="optional"/>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                        <xs:attribute name="CommissionCode" type="xs:string" use="optional"/>
                        <xs:attribute name="CommissionDescription" type="xs:string" use="optional"/>
                        <xs:attribute name="CodeContext" type="xs:string" use="optional"/>
                        <xs:attribute name="AppliedPromoCodes" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="ExchangeRate">
                     <xs:complexType>
                        <xs:attribute name="CurrencyFrom" type="ns20:AlphaLength3" use="required"/>
                        <xs:attribute name="CurrencyTo" type="ns20:AlphaLength3" use="required"/>
                        <xs:attribute name="Rate" type="xs:float" use="required"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="PublishedFare">
                     <xs:annotation>
                        <xs:documentation>The original GDS Fare, can be different if Base Fare has a private contract discount applied to it.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="MarkupApplied">
                     <xs:annotation>
                        <xs:documentation>Indicates that there is a markup applied to the original fare</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>percentage markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AmountToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>Flat amount markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional">
                           <xs:annotation>
                              <xs:documentation>Amount for flat amount markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional">
                           <xs:annotation>
                              <xs:documentation>Currency of markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>Decimal places for the currency used</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="MarkupCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Code of markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="MarkupDescription" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Description of the nature of markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CodeContext" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Code context TDP/GDS etc</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RoundingRule" use="optional">
                           <xs:annotation>
                              <xs:documentation>Rounding rule for the currency</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="ROUND_UP"/>
                                 <xs:enumeration value="ROUND_DOWN"/>
                                 <xs:enumeration value="ROUND_CEILING"/>
                                 <xs:enumeration value="ROUND_FLOOR"/>
                                 <xs:enumeration value="ROUND_HALF_UP"/>
                                 <xs:enumeration value="ROUND_HALF_DOWN"/>
                                 <xs:enumeration value="ROUND_HALF_EVEN"/>
                                 <xs:enumeration value="ROUND_UNNECESSARY"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="EnhancementFees">
                     <xs:annotation>
                        <xs:documentation>This is a collection of Enhancement Fees</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="EnhancementFee">
                              <xs:annotation>
                                 <xs:documentation>Enhancement Fee associated to passenger and a segment</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element minOccurs="0" name="FeeTaxes">
                                       <xs:annotation>
                                          <xs:documentation>This is a collection of taxes applied to the Enhancement Fee</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:sequence>
                                             <xs:element maxOccurs="99" name="FeeTax">
                                                <xs:annotation>
                                                   <xs:documentation>Tax(s) applied to the Enhancement Fee</xs:documentation>
                                                </xs:annotation>
                                                <xs:complexType>
                                                   <xs:attribute name="TaxCode" type="StringLength1to16" use="required"/>
                                                   <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
                                                   <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
                                                   <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                                                </xs:complexType>
                                             </xs:element>
                                          </xs:sequence>
                                          <xs:attribute name="NameValues" type="xs:string" use="optional"/>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                                 <xs:attribute name="FeeCode" use="required">
                                    <xs:simpleType>
                                       <xs:restriction base="xs:string">
                                          <xs:enumeration value="EPU"/>
                                          <xs:enumeration value="UPSAT"/>
                                          <xs:enumeration value="AWA"/>
                                          <xs:enumeration value="PLA"/>
                                          <xs:enumeration value="PTEPU"/>
                                          <xs:enumeration value="PCUSST"/>
                                          <xs:enumeration value="ASC"/>
                                       </xs:restriction>
                                    </xs:simpleType>
                                 </xs:attribute>
                                 <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
                                 <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
                                 <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                                 <xs:attribute name="paxRPH" type="RPH_Type" use="optional"/>
                                 <xs:attribute name="segRPH" type="RPH_Type" use="optional"/>
                                 <xs:attribute name="legRPH" type="RPH_Type" use="optional"/>
                                 <xs:attribute name="CCType" type="xs:string" use="optional"/>
                                 <xs:attribute name="CCNumber" type="xs:string" use="optional"/>
                                 <xs:attribute name="PurchaseDate" type="xs:dateTime" use="optional"/>
                                 <xs:attribute name="ReceiptNumber" type="xs:string" use="optional"/>
                                 <xs:attribute name="UpgradeBookingCode" type="xs:string" use="optional"/>
                                 <xs:attribute name="UpgradeCabinCode" type="xs:string" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="NameValues" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareForcingDetails">
                     <xs:annotation>
                        <xs:documentation>The fare forcing details for any fare discripancies</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="TotalFareDifference">
                              <xs:annotation>
                                 <xs:documentation>The difference in total fare between request and response</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="Amount" type="xs:integer"/>
                                 <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3"/>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="RequestedTotalFare" type="RequestedFareType">
                              <xs:annotation>
                                 <xs:documentation>The total fare from request</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="HostTotalFare" type="RequestedFareType">
                              <xs:annotation>
                                 <xs:documentation>The total fare from the host</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:PTC_FareBreakdown.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:PTC_FareBreakdown.types.extensions.air.rqrs.ota.datalex.com">
         <xs:simpleType name="AlphaLength3">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Alphabetic Strings, length exactly 3</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[a-zA-Z]{3}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="Money">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for amounts, max 3 decimals</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:decimal">
               <xs:fractionDigits value="3"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="CurrencyAmountType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Provides a monetary amount and the code of the currency in which this amount is expressed.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="Amount" type="Money" use="optional"/>
            <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional"/>
            <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
         </xs:complexType>
         <xs:simpleType name="StringLength1to16">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 16</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="16"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="AirTaxType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Defines the data fields available for air tax.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="TaxCode" type="StringLength1to16" use="required">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Identifies the code for the tax.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Amount" type="Money" use="optional"/>
            <xs:attribute name="CurrencyCode" type="AlphaLength3" use="optional"/>
            <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
         </xs:complexType>
         <xs:complexType name="RequestedFareType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Holds a base fare, tax, total and currency information on a price</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element name="BaseFare" type="CurrencyAmountType">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">Price of the inventory excluding taxes and fees.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element minOccurs="0" name="Taxes">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">This is a collection of Taxes</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element maxOccurs="99" name="Tax" type="AirTaxType">
                           <xs:annotation>
                              <xs:documentation xml:lang="en">Any individual tax applied to the fare</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                     </xs:sequence>
                  </xs:complexType>
               </xs:element>
               <xs:element name="TotalFare" type="CurrencyAmountType">
                  <xs:annotation>
                     <xs:documentation xml:lang="en">The total price that the passenger would pay (includes fare, taxes, fees)</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:complexType>
         <xs:simpleType name="RPH_Type">
            <xs:annotation>
               <xs:documentation xml:lang="en">(Reference Place Holder) - an index code to identify an instance in a collection of like items.. For example,  used to assign individual passengers or clients to particular itinerary items.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[0-9]{1,8}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="PassengerAssociation">
                     <xs:annotation>
                        <xs:documentation>The passenger type and index with which this fare breakdown is associated. A fare breakdown may be associated with more than one passenger.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" minOccurs="0" name="AlternateApplicablePassengerType">
                              <xs:annotation>
                                 <xs:documentation>As a fare may be applicable to more then the requested passenger type we keep the applicable alternate passenger types for this fare here. We also mark them as qualified or not from the Generic Guest Type settings in the MUI.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="PassengerTypeCode" type="xs:string" use="required"/>
                                 <xs:attribute name="ContextCode" type="xs:string" use="optional"/>
                                 <xs:attribute name="PassengerTypeMinAge" type="xs:int" use="optional"/>
                                 <xs:attribute name="PassengerTypeMaxAge" type="xs:int" use="optional"/>
                                 <xs:attribute name="PassengerTypeAge" type="xs:int" use="optional"/>
                                 <xs:attribute name="QualifiedPassengerType" type="xs:boolean" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                           <xs:element maxOccurs="unbounded" minOccurs="0" name="RemoteSystemFareQuotePTCs">
                              <xs:annotation>
                                 <xs:documentation>This multiplexed element will store the remote fare quote system PTC and the remote system will be the code context.  The remote system PTC code(s) should copied to the reservation and subsequently used for re-price and ticketing operations.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="ContextCode" type="xs:string" use="required"/>
                                 <xs:attribute name="PassengerTypeCode" type="xs:string" use="required"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="PassengerTypeCode" type="xs:string" use="required"/>
                        <xs:attribute name="ContextCode" type="xs:string" use="optional"/>
                        <xs:attribute name="PassengerReference" type="RPH_Type" use="optional"/>
                        <xs:attribute name="PassengerTypeMinAge" type="xs:int" use="optional"/>
                        <xs:attribute name="PassengerTypeMaxAge" type="xs:int" use="optional"/>
                        <xs:attribute name="PassengerTypeAge" type="xs:int" use="optional"/>
                        <xs:attribute name="QualifiedPassengerType" type="xs:boolean" use="optional"/>
                        <xs:attribute name="YouthPassengerType" type="xs:boolean" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="SegmentAssociation">
                     <xs:annotation>
                        <xs:documentation>The FlightSegment RPH number with which this fare is associated. A fare may be associated with more than one segment.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="FlightSegmentReference" type="RPH_Type" use="required"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareForcingDetails">
                     <xs:annotation>
                        <xs:documentation>The fare forcing details for any fare discripancies</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="FareDifference">
                              <xs:annotation>
                                 <xs:documentation>The difference in passenger total fare between request and response</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="Amount" type="xs:integer">
                                    <xs:annotation>
                                       <xs:documentation>The amount of fare difference</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="CurrencyCode" type="AlphaLength3">
                                    <xs:annotation>
                                       <xs:documentation>The currency code of fare difference</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="RequestedFareBreakdown" type="RequestedFareType">
                              <xs:annotation>
                                 <xs:documentation>The PTC_FareBreakdown details from request</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element name="HostFareBreakdown" type="RequestedFareType">
                              <xs:annotation>
                                 <xs:documentation>The PTC_FareBreakdown details from host</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:AirItineraryPricingInfo.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:AirItineraryPricingInfo.types.extensions.air.rqrs.ota.datalex.com">
         <xs:import namespace="urn:media.rqrs.datalex.com"/>
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05"/>
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:import namespace="urn:farefamily.rqrs.datalex.com"/>
         <xs:import namespace="urn:fee.rqrs.tdp.datalex.com"/>
         <xs:import namespace="urn:merch.rqrs.datalex.com"/>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="10" minOccurs="0" name="SolutionGroupTag" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>The name(s) of the ITA Value function(s) this priced itinerary matched on.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="AirItineraryPricingInfoReference">
                     <xs:annotation>
                        <xs:documentation>This hold the reference to the unique air itinerary pricing info</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="UniqueAirItineraryPricingInfoRef" type="xs:nonNegativeInteger" use="required"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="SupplementalPricingInfo">
                     <xs:annotation>
                        <xs:documentation>Contains any additional pricing information relating to the AirItinerary</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="LastDateToTicket" type="xs:dateTime" use="optional">
                           <xs:annotation>
                              <xs:documentation source="Contains the last day to ticket/advance purchasing date"/>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="FareGroupID" type="xs:string" use="optional"/>
                        <xs:attribute name="EPUDirection" use="optional">
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="EPUOutbound"/>
                                 <xs:enumeration value="EPUInbound"/>
                                 <xs:enumeration value="EPUItinerary"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="FlightOrTourIndicator" use="optional">
                           <xs:annotation>
                              <xs:documentation>This field is used by the UI layer to highlight Flight Only or Inclusive Tour options (always FO or IT).</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="FlightOnly"/>
                                 <xs:enumeration value="InclusiveTour"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="FareLatestValidTime" type="xs:dateTime" use="optional">
                           <xs:annotation>
                              <xs:documentation>The pricing is valid until this date. This date will be present if the date was already exprired compared to the first segment departure date.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="HashIndexKey" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>Hash index key to enable the UI to find combinable origin-destination options within a set of priced itineraries.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns7:PricingSystem"/>
                  <xs:element minOccurs="0" name="TaxSource">
                     <xs:annotation>
                        <xs:documentation>System that returned the taxes and fees</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="GDS"/>
                           <xs:enumeration value="TDPCache"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="ExchangeRate">
                     <xs:annotation>
                        <xs:documentation>Represents an exchange rate between two currencies</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="FromCurrency" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>The currency we are converting from</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ToCurrency" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>The currency we are converting to</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Rate" type="xs:decimal" use="required">
                           <xs:annotation>
                              <xs:documentation>The exchange rate between the two currencies with a scale of 6 decimal places</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="BookingFees">
                     <xs:annotation>
                        <xs:documentation>Contains the BookingFees to that can be added to the fares based on the Business Rules condition</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns54:Fees"/>
                        </xs:sequence>
                        <xs:attribute name="Amount" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>depricated</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Description" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>depricated</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>depricated</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DecimalPlaces" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>depricated</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="StoredFareStatus">
                     <xs:annotation>
                        <xs:documentation>If we priced a booked PNR, this element provides information concerning the status of the current stored fare</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="FareGuaranteed" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>Tells us if the fare was still guaranteed at the time of pricing (note: only applicable to certain GDS)</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="NewFareStoredInPNR" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>Tells us if a previous fare had expired (i.e. its guarantee was no longer valid) and a new fare was inserted into the PNR to replace the previous expired fare.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareForcingDetails">
                     <xs:annotation>
                        <xs:documentation>The fare forcing details for any fare discripancies</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="IsFareForceApplied" type="xs:boolean">
                           <xs:annotation>
                              <xs:documentation>The identifier if fare forcing has been applied on this itinerary</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareRestrictions">
                     <xs:annotation>
                        <xs:documentation>Collection of ticketing restrictions</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="FareRestriction">
                              <xs:annotation>
                                 <xs:documentation>Summarize information on the sale and ticketing restrictions on a pricing solution</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="invalidBeforeDepartureDays" type="xs:nonNegativeInteger">
                                    <xs:annotation>
                                       <xs:documentation>Number of days before departure that a fare is not valid</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="invalidAfterDepartureDays" type="xs:nonNegativeInteger">
                                    <xs:annotation>
                                       <xs:documentation>Number of days after departure that a fare is not valid</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="noTicketBeforeDepartureDays" type="xs:nonNegativeInteger">
                                    <xs:annotation>
                                       <xs:documentation>Number of days before departure when a ticket may no longer be issued</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="noTicketAfterReservationDays" type="xs:nonNegativeInteger">
                                    <xs:annotation>
                                       <xs:documentation>Number of days after reservation when a ticket may no longer be issued</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="earliestReturnTime" type="xs:dateTime">
                                    <xs:annotation>
                                       <xs:documentation>Earliest date when passenger(s) may return</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="latestReturnTime" type="xs:dateTime">
                                    <xs:annotation>
                                       <xs:documentation>Latest date when passenger(s) may return</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="eTicketing">
                                    <xs:annotation>
                                       <xs:documentation>Restrictions on e-Ticketing for this pricing solution</xs:documentation>
                                    </xs:annotation>
                                    <xs:simpleType>
                                       <xs:restriction base="xs:string">
                                          <xs:enumeration value="PERMITTED"/>
                                          <xs:enumeration value="PROHIBITED"/>
                                          <xs:enumeration value="REQUIRED"/>
                                       </xs:restriction>
                                    </xs:simpleType>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="MediaContentLink">
                     <xs:annotation>
                        <xs:documentation>Contains the codes to identify Media Content this element relates to</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Code" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>Code to uniquely identify what Media Content this element relates to</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareFamilyUpsellOptions">
                     <xs:annotation>
                        <xs:documentation>Collection of fare family upsell options</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="FareFamilyUpsellOption">
                              <xs:annotation>
                                 <xs:documentation>Fare family upsell information that agregates applicable upsell fare family, fare basis code and index of including flight itinerary</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element name="UpsellPrice">
                                       <xs:annotation>
                                          <xs:documentation>The price difference between original fare family and the upsell one based on price dipslay mode</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:attribute name="Amount" type="ns8:Money" use="required"/>
                                          <xs:attribute name="Currency" type="ns8:AlphaLength3" use="required"/>
                                          <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                                          <xs:attribute name="PricingMode" use="required">
                                             <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                   <xs:enumeration value="TOTAL_FARE"/>
                                                   <xs:enumeration value="TOTAL_FARE_EXCL_TAXES"/>
                                                   <xs:enumeration value="PER_PAX_FARE"/>
                                                   <xs:enumeration value="PER_PAX_FARE_EXCL_TAXES"/>
                                                </xs:restriction>
                                             </xs:simpleType>
                                          </xs:attribute>
                                       </xs:complexType>
                                    </xs:element>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" name="AncillaryAirComponentLink">
                                       <xs:annotation>
                                          <xs:documentation>The link to ancillary air component that exist in up-sell fare family but misssed in the original one</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:attribute name="AncillaryAirComponentCode" type="xs:string" use="required">
                                             <xs:annotation>
                                                <xs:documentation>A unique code representing an ancillary air component.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                          <xs:attribute name="DisplaySequence" type="xs:nonNegativeInteger" use="required">
                                             <xs:annotation>
                                                <xs:documentation>An ancillary air component display sequence. The ancillary air component with lower sequence should be displayed first</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                       </xs:complexType>
                                    </xs:element>
                                    <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns29:MediaContent"/>
                                 </xs:sequence>
                                 <xs:attribute name="FareFamilyCode" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Upsell fare family code</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="PricedItinerarySequence" type="xs:positiveInteger" use="required">
                                    <xs:annotation>
                                       <xs:documentation>The index of including flight itinerary</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="CombinabilityHashIndex" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Up-sell option combinability hash index. Options with the same hash index are combinable</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="FlightHashIndex" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Allows to divide up-sell options into outbound/inbound groups</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="DisplaySequence" type="xs:nonNegativeInteger" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Up-sell option display sequence. The option with lower sequence should be displayed first</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                           <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns13:FareFamilyBenefitsDisplayMatrix"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="AACPricedOptionsReferences">
                     <xs:annotation>
                        <xs:documentation>List of references to AncillaryAirComponentPricedOptions elements that are associated with this PricedItinerary</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="AACPricedOptionsReference">
                              <xs:annotation>
                                 <xs:documentation>Reference to AncillaryAirComponentPricedOptions that is associated with this PricedItinerary</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="AACPricedOptionsDocRef" type="xs:IDREF" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Reference to AncillaryAirComponentPricedOptions</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="AACPricedOptionsSequence" type="xs:nonNegativeInteger" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Sequence of referenced AncillaryAirComponentPricedOptions</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="PromotionCode" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>The promotion code that would be used for url.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns9:Persona"/>
                  <xs:element minOccurs="0" name="Provider">
                     <xs:annotation>
                        <xs:documentation>The provider of this pricing information.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="ProviderCode" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:media.rqrs.datalex.com" xmlns="urn:media.rqrs.datalex.com">
         <xs:import namespace="urn:social.rqrs.datalex.com"/>
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:attributeGroup name="MediaContentAttributes">
            <xs:attribute name="Description" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>This field contains the user-created description for the Media Content Item. This can contain up to 1000 characters.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="FileName" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>The File-URL Information contains important information about the Media Content. The File Name and URL (location of the file) enable the web page to locate the media file. The MIME Extension tells the web designer and the system what type of file the link is (such as .jpg, .txt, .gif, etc.). The FileName can contain up to 100 characters.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="MimeExtension" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>The MIME Extension tells the web designer and the system what type of file the link is (such as .jpg, .txt, .gif, etc.). This should contain only 4 characters.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Url" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>The File-URL Information contains important information about the Media Content. The File Name and URL (location of the file) enable the web page to locate the media file. The MIME Extension tells the web designer and the system what type of file the link is (such as .jpg, .txt, .gif, etc.). The url can contain up to 200 characters.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="EffectiveDate" type="xs:date" use="required">
               <xs:annotation>
                  <xs:documentation>The Usage Dates for Media Content determine the Effective and Expiration Dates for the media file. Usage Dates enable the web designer to create templates which will populate appropriate Media Content based on the time of year the web page displays. For example, a ski resort area may want to display pictures of ski slopes covered with snow and skiers during winter months, but may have off-season pictures of mountains covered with wildflowers and hikers. Setting these dates ensures that the web pages will display according to season.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ExpiresDate" type="xs:date" use="optional">
               <xs:annotation>
                  <xs:documentation>The Usage Dates for Media Content determine the Effective and Expiration Dates for the media file. Usage Dates enable the web designer to create templates which will populate appropriate Media Content based on the time of year the web page displays. For example, a ski resort area may want to display pictures of ski slopes covered with snow and skiers during winter months, but may have off-season pictures of mountains covered with wildflowers and hikers. Setting these dates ensures that the web pages will display according to season.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="LanguageCode" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>This field contains the associated Language for the Media Content Item. This just be an existing system language.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ContentImage" type="xs:base64Binary" use="optional">
               <xs:annotation>
                  <xs:documentation>Not currently used. Can be used to store the image rather than have a link to the location of the image.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="FrequentlyUsed" type="xs:boolean" use="optional"/>
         </xs:attributeGroup>
         <xs:element name="MediaContent">
            <xs:annotation>
               <xs:documentation>The MediaContent object is defined uniquely by its url and language code.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attributeGroup ref="MediaContentAttributes"/>
               <xs:attribute name="MediaContentVersion" type="xs:nonNegativeInteger" use="optional">
                  <xs:annotation>
                     <xs:documentation>This will identify the Media Content version.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="MediaContentUsageType">
            <xs:choice>
               <xs:element name="MediaContentRef">
                  <xs:annotation>
                     <xs:documentation>Defines the media content to be assigned</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:attribute name="Url" type="xs:string" use="required"/>
                     <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
                  </xs:complexType>
               </xs:element>
               <xs:element ref="MediaContent"/>
            </xs:choice>
            <xs:attribute name="ContentUsageCode" type="xs:string" use="required"/>
            <xs:attribute name="Sequence" type="xs:int" use="required"/>
            <xs:attribute name="LinkToUrl" type="xs:string" use="optional"/>
         </xs:complexType>
         <xs:element name="MediaContentUsage" type="MediaContentUsageType">
            <xs:annotation>
               <xs:documentation>Defines the assignment of media content</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="MediaContentUsages">
            <xs:annotation>
               <xs:documentation>collection of media content assignments</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="MediaContentUsage"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:attributeGroup name="AdvertDescriptionAttributes">
            <xs:attribute name="LanguageCode" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>This uniquely identifies the language of this description, and must be a valid code from the system languages.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Text" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>This is a user-defined, free-form descpription of the Advertisement. This can contain up to 2000 characters.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="AltText1" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>This is a user-defined, free-form alternative text for the Advertisement. This can contain up to 4000 characters.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="AltText2" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>This is a user-defined, free-form second alternative text for the Advertisement. This can contain up to 1000 characters.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:element name="AdvertisementDescription">
            <xs:annotation>
               <xs:documentation>There may only be one Advertisement description per language. As the description will be required by the UI, it must be present for each language used by the system.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attributeGroup ref="AdvertDescriptionAttributes"/>
            </xs:complexType>
         </xs:element>
         <xs:attributeGroup name="AdvertisementAttributes">
            <xs:attribute name="AdvertisementCode" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>This uniquely identifies the Advertisement and can contain up to 100 characters.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ConsumptionFrom" type="xs:date" use="optional">
               <xs:annotation>
                  <xs:documentation>The start consume date for this advertisement offering.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ConsumptionTo" type="xs:date" use="optional">
               <xs:annotation>
                  <xs:documentation>The end consume date for this advertisement offering.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="BookingFrom" type="xs:date" use="optional">
               <xs:annotation>
                  <xs:documentation>The start booking date for this Advertisement offering.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="BookingTo" type="xs:date" use="optional">
               <xs:annotation>
                  <xs:documentation>The end booking date for this Advertisement offering.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="SocialConfigurationCode" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Social Configuration attached.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:attributeGroup>
         <xs:attributeGroup name="AdvertisementContentAttributes">
            <xs:attribute name="LanguageCode" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>Identifies the language of this media Content. This code along with the url uniquely identifies the Media file.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Url" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>This specifies the location of the file. The url and the language code together identify the Media file.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Sequence" type="xs:nonNegativeInteger" use="optional">
               <xs:annotation>
                  <xs:documentation>A sequence order can be applied to the media content.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ContentUsageCode" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>Identifies the usage of this content. Must already exist in the database.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="LinkToUrl" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Contains the link to the url that can be applied to the Media content. This can contain up to 200 characters.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Description" type="xs:string" use="optional"/>
            <xs:attribute name="FileName" type="xs:string" use="optional"/>
            <xs:attribute name="MimeExtension" type="xs:string" use="optional"/>
            <xs:attribute name="EffectiveDate" type="xs:date" use="optional"/>
            <xs:attribute name="ExpiresDate" type="xs:date" use="optional"/>
         </xs:attributeGroup>
         <xs:element name="AdvertExtraInformation">
            <xs:annotation>
               <xs:documentation>Name/value pairs to associate with the advert, can be used to fill meta-data to send to a social media channel for example.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="Name" type="xs:string" use="required"/>
               <xs:attribute name="Value" type="xs:string" use="required"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="Advertisement">
            <xs:annotation>
               <xs:documentation>This corresponds to a Media Advertisement. This is uniquely identified by the Advertisement code.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="AdvertMediaOutlet">
                     <xs:annotation>
                        <xs:documentation>This corresponds to one Media Outlet that is assigned to an Advertisement and can have dates applied.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns7:DateCalendarLink"/>
                        </xs:sequence>
                        <xs:attribute name="MediaOutletCode" type="xs:string" use="required"/>
                        <xs:attribute name="MediaOutletName" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="AdvertContent">
                     <xs:annotation>
                        <xs:documentation>The Media content and its usage that is assigned to this advertisement. The Media file, which is identified by the combination of the url and the language code, must exist already. The Content usage must also exist.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attributeGroup ref="AdvertisementContentAttributes"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="AdvertExtraInformation"/>
                  <xs:element maxOccurs="unbounded" ref="AdvertisementDescription"/>
                  <xs:element maxOccurs="1" minOccurs="0" ref="ns82:SocialConfiguration"/>
               </xs:sequence>
               <xs:attributeGroup ref="AdvertisementAttributes"/>
               <xs:attribute name="AdvertisementVersion" type="xs:nonNegativeInteger" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="BookConsumeDateRange">
            <xs:complexType>
               <xs:attribute name="StartBookDate" type="xs:date" use="optional"/>
               <xs:attribute name="StopBookDate" type="xs:date" use="optional"/>
               <xs:attribute name="StartConsumeDate" type="xs:date" use="optional"/>
               <xs:attribute name="StopConsumeDate" type="xs:date" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="AdvertisementUsageType">
            <xs:sequence>
               <xs:choice>
                  <xs:element ref="Advertisement">
                     <xs:annotation>
                        <xs:documentation>Defines an advertisement to be assigned</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="AdvertisementRef">
                     <xs:complexType>
                        <xs:attribute name="AdvertisementCode" type="xs:string" use="required"/>
                     </xs:complexType>
                  </xs:element>
               </xs:choice>
               <xs:element minOccurs="0" ref="BookConsumeDateRange"/>
            </xs:sequence>
         </xs:complexType>
         <xs:element name="AdvertisementUsage" type="AdvertisementUsageType">
            <xs:annotation>
               <xs:documentation>Defines the assignment of an advertisement</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="AdvertisementUsages">
            <xs:annotation>
               <xs:documentation>collection of advertisement assignments</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="AdvertisementUsage"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:farefamily.rqrs.datalex.com" xmlns="urn:farefamily.rqrs.datalex.com">
         <xs:element name="BenefitDescription">
            <xs:annotation>
               <xs:documentation>This is an override lingual description of the fare family benefit for comparison purposes</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="LanguageCode" type="xs:string" use="required">
                  <xs:annotation>
                     <xs:documentation>This is the language code of this  element.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="BenefitName" type="xs:string" use="required">
                  <xs:annotation>
                     <xs:documentation>The name of the benefit</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ShortDescription" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>A short description of the benefit</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="LongDescription" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Long description of the benefit</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="BenefitContent">
            <xs:annotation>
               <xs:documentation>Content related to a benefit.  Could be an Icon or link URL that describes the benefit</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="LanguageCode" type="xs:string" use="required">
                  <xs:annotation>
                     <xs:documentation>Identifies the language of this media Content. This code along with the url uniquely identifies the Media file.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Url" type="xs:string" use="required">
                  <xs:annotation>
                     <xs:documentation>This specifies the location of the file. The url and the language code together identify the Media file.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Sequence" type="xs:nonNegativeInteger" use="optional">
                  <xs:annotation>
                     <xs:documentation>A sequence order can be applied to the media content.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ContentUsageCode" type="xs:string" use="required">
                  <xs:annotation>
                     <xs:documentation>Identifies the usage of this content. Must already exist in the database.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="LinkToUrl" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Contains the link to the url that can be applied to the Media content. This can contain up to 200 characters.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Description" type="xs:string" use="optional"/>
               <xs:attribute name="FileName" type="xs:string" use="optional"/>
               <xs:attribute name="MimeExtension" type="xs:string" use="optional"/>
               <xs:attribute name="EffectiveDate" type="xs:date" use="optional"/>
               <xs:attribute name="ExpiresDate" type="xs:date" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="FareFamilyBenefitsDisplayMatrix">
            <xs:annotation>
               <xs:documentation>Contains a common matrix of fare family benefits compared across fare families.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="Benefit">
                     <xs:annotation>
                        <xs:documentation>The benefit to be compared across fare families</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" ref="BenefitDescription"/>
                           <xs:element maxOccurs="unbounded" minOccurs="0" ref="BenefitContent"/>
                           <xs:element maxOccurs="unbounded" name="FareFamilyBenefitCell">
                              <xs:annotation>
                                 <xs:documentation>The fare family value for the compared benefit.  The values are derived from configuration and rules and are finalized and ready for display.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:choice minOccurs="0">
                                    <xs:element maxOccurs="unbounded" name="CellValue">
                                       <xs:annotation>
                                          <xs:documentation>The final display value for the cell intersection of the benefit and fare family benefit.  One per language.</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:sequence>
                                             <xs:element minOccurs="0" name="ValueShortText" type="xs:string"/>
                                             <xs:element minOccurs="0" name="ValueLongText" type="xs:string"/>
                                          </xs:sequence>
                                          <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
                                       </xs:complexType>
                                    </xs:element>
                                    <xs:element name="NotIncluded" type="xs:boolean">
                                       <xs:annotation>
                                          <xs:documentation>Indicates if True, that the benefit is not provided as part of the fare family</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                 </xs:choice>
                                 <xs:attribute name="FareFamilyCode" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>This is a code which indentifies this Fare Family.  It must be unique across a system.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="MatrixBenefitCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>Unique code identifying the Benefit at the system level</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="MatrixCode" type="xs:string" use="required"/>
               <xs:attribute name="FlightHashIndex" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="OverrideDescriptionValueType">
            <xs:attribute name="FareFamilyShortDescription" type="xs:string" use="required"/>
            <xs:attribute name="LongDescription" type="xs:string" use="optional"/>
            <xs:attribute name="ShortDescription" type="xs:string" use="optional"/>
         </xs:complexType>
         <xs:complexType name="DescriptionOverrideType">
            <xs:annotation>
               <xs:documentation>With a description override, the user may choose to override the entire value of the description or replace tokens in the ancillary description.</xs:documentation>
            </xs:annotation>
            <xs:choice>
               <xs:element maxOccurs="unbounded" name="OverrideToken">
                  <xs:annotation>
                     <xs:documentation>One element is given for each token to be overridden in the description.  The value can be literal or derived ffom a function.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:attribute name="TokenSequence" type="xs:positiveInteger" use="required"/>
                     <xs:attribute name="TokenValue" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>A token value literally fills in the token sequences in the Fare Family Short Description.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="TokenValueFunction" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>A reference to a Java function that derrives the value of the token in the form of ${GETITEMPRICE}</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:complexType>
               </xs:element>
               <xs:element name="OverrideDescriptionValue" type="OverrideDescriptionValueType">
                  <xs:annotation>
                     <xs:documentation>The description is overridden completely.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:choice>
            <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
         </xs:complexType>
         <xs:complexType name="FareFamilyBenefitLinkType">
            <xs:annotation>
               <xs:documentation>Links a fare family to an ancillary component.</xs:documentation>
            </xs:annotation>
            <xs:sequence>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="AncillaryComponentOverrideDescriptionToken" type="DescriptionOverrideType">
                  <xs:annotation>
                     <xs:documentation>The user can override the Fare Family Description of an ancillary item when it is used in conjunction with a specific fare family</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element maxOccurs="unbounded" minOccurs="0" name="ComparisonMatrixID">
                  <xs:annotation>
                     <xs:documentation>The user can link a fare family / ancillary component to a comparison matrix.  This element contains the unique matrix code and benefit code within the matrix.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:sequence>
                        <xs:element minOccurs="0" name="ComparisonCellValue" type="DescriptionOverrideType">
                           <xs:annotation>
                              <xs:documentation>The value to be displayed in the comparison cell on a fare family matrix.</xs:documentation>
                           </xs:annotation>
                        </xs:element>
                     </xs:sequence>
                     <xs:attribute name="MatrixCode" type="xs:string" use="required"/>
                     <xs:attribute name="MatrixBenefitCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>Unique code identifying the Benefit at the system level</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:complexType>
               </xs:element>
            </xs:sequence>
            <xs:attribute name="FromEffectiveDisplayDate" type="xs:dateTime" use="required"/>
            <xs:attribute name="AncillaryAirComponentCode" type="xs:string" use="required">
               <xs:annotation>
                  <xs:documentation>A unique code representing an Ancillary Air Component.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
         </xs:complexType>
         <xs:element name="FareFamily">
            <xs:annotation>
               <xs:documentation>This element is the parent for a particular Fare Family.  It encapsulates a description and any associated features, such as Remarks, Cloning and Combinability.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" name="FareFamilyOverallDescription">
                     <xs:annotation>
                        <xs:documentation>This is a lingual description of the overall Fare Family.  Each element represents a different language.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="LanguageCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>This is the language code of this FareFamilyOverallDescription element.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ShortDescription" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>This attribute can be used in the blurb for a Fare Family.  It should be concise.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="URL" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is a link to an external resource which should contain further information relating to a Fare Family.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="FareFamilyName" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>This is a human readable name of a Fare Family.  It should be used in a User Interface in order to categorize fares.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="CloningRule">
                     <xs:annotation>
                        <xs:documentation>This element is a list of other FareFamilyCodes which may be cloned into this fare family.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Sequence" type="xs:integer" use="required">
                           <xs:annotation>
                              <xs:documentation>This is the order in which this FareFamilyCode can be combined.  I.e. if this is set to 1, then this will be the first Fare Family Code considered for cloning.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="FareFamilyCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>This code should match a FareFamilyCode from another existing fare family.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="Combinability">
                     <xs:annotation>
                        <xs:documentation>This element describes rules relating to how this fare family can be combined with other fare families.  If this element is not present, then combining across fare families should be prohibited.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:choice>
                           <xs:annotation>
                              <xs:documentation>Either a CombinableRule is present indicating what the fare family can be combined with, or the CombinationAnyAll element is present indicating no restriction on combinability.</xs:documentation>
                           </xs:annotation>
                           <xs:element maxOccurs="unbounded" name="CombinabilityRule">
                              <xs:annotation>
                                 <xs:documentation>This is a list of other fare family codes which are combinable with this fare family.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="FareFamilyCode" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>This code should match a FareFamilyCode from another existing fare family.  It means that FareFamily@FareFamilyCode can be combined with it.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="CombinationAnyAll">
                              <xs:annotation>
                                 <xs:documentation>If present, this indicates that this fare family can be combined with all other fare families.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType/>
                           </xs:element>
                        </xs:choice>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="RemoteSystemRemarks">
                     <xs:annotation>
                        <xs:documentation>This describes all remarks associated with this FareFamily for a specific Remote System.  All sub elements reelate to the RemoteSystemCode specified in this element.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="RemarkSet">
                              <xs:annotation>
                                 <xs:documentation>This is a set of remarks which are used to record information particular to a fare family in a PNR.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:choice>
                                    <xs:element name="OtherServiceInformation">
                                       <xs:annotation>
                                          <xs:documentation>OSIs hold machine-readable information that should be picked up by the applicable carrier.</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:attribute name="Text" type="xs:string" use="required">
                                             <xs:annotation>
                                                <xs:documentation>The body of this OSI.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                          <xs:attribute name="AirlineCode" type="xs:string" use="optional">
                                             <xs:annotation>
                                                <xs:documentation>A two letter IATA carrier code for which this OSI is applicable to.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                       </xs:complexType>
                                    </xs:element>
                                    <xs:element name="SpecialServiceRequest">
                                       <xs:annotation>
                                          <xs:documentation>SSRs contain requests for special services which require confirmation from an airline.</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:attribute name="AirlineCode" type="xs:string" use="optional">
                                             <xs:annotation>
                                                <xs:documentation>A two letter IATA carrier code identifying the airline for which this SSR is applicable.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                          <xs:attribute name="Text" type="xs:string" use="optional">
                                             <xs:annotation>
                                                <xs:documentation>The body of the SSR.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                          <xs:attribute name="Code" type="xs:string" use="required">
                                             <xs:annotation>
                                                <xs:documentation>A four letter code relating to this SSR.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                       </xs:complexType>
                                    </xs:element>
                                    <xs:element name="GeneralRemark">
                                       <xs:annotation>
                                          <xs:documentation>A General Remark is a plain-text human-readable string added to the reservation.</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:attribute name="Text" type="xs:string">
                                             <xs:annotation>
                                                <xs:documentation>The body of the remark.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:choice>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="RemoteSystemCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>This is the code of the remote system into which the remark set is to be inserted.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="FareFamilyPolicyGroup">
                     <xs:annotation>
                        <xs:documentation>This is a list of codes of system level policy groups (created using the policy manage service) applicable to this fare family.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="PolicyGroupCode" type="xs:string" use="required"/>
                        <xs:attribute name="PolicyGroupType" type="xs:string" use="required"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="FareFamilyBenefitLink" type="FareFamilyBenefitLinkType">
                     <xs:annotation>
                        <xs:documentation>A Fare Family Benefit ties an Ancillary Air Component to a Fare Family and can override the description of the items.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareFamilyMediaContent">
                     <xs:annotation>
                        <xs:documentation>Content related to a benefit.  Could be an Icon or link URL that describes the benefit</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="LanguageCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>Identifies the language of this media Content. This code along with the url uniquely identifies the Media file.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Url" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>This specifies the location of the file. The url and the language code together identify the Media file.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Sequence" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>A sequence order can be applied to the media content.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ContentUsageCode" type="xs:string" use="required">
                           <xs:annotation>
                              <xs:documentation>Identifies the usage of this content. Must already exist in the database.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="LinkToUrl" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Contains the link to the url that can be applied to the Media content. This can contain up to 200 characters.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Description" type="xs:string" use="optional"/>
                        <xs:attribute name="FileName" type="xs:string" use="optional"/>
                        <xs:attribute name="MimeExtension" type="xs:string" use="optional"/>
                        <xs:attribute name="EffectiveDate" type="xs:date" use="optional"/>
                        <xs:attribute name="ExpiresDate" type="xs:date" use="optional"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="FareFamilyCode" type="xs:string" use="required">
                  <xs:annotation>
                     <xs:documentation>This is a code which indentifies this Fare Family.  It must be unique across a system.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="HideIfNoFares" type="xs:boolean" use="required">
                  <xs:annotation>
                     <xs:documentation>Set this to true if a Fare Family is to be removed from a response when there is no Priced Itinerary available in the Fare Family.  This is used at shopping time.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="FareFamilyVersion" type="xs:nonNegativeInteger" use="required">
                  <xs:annotation>
                     <xs:documentation>This will identify the FareFamily version in a database.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ProviderCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>This will uniquely identify the Provider and is limited to 100 characters.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:fee.rqrs.tdp.datalex.com" xmlns="urn:fee.rqrs.tdp.datalex.com">
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:element name="TaxFee">
            <xs:annotation>
               <xs:documentation>Tax that applies to a Fee.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="TaxFeeDocID" type="xs:ID" use="optional">
                  <xs:annotation>
                     <xs:documentation>Unique ID supplied by the client when adding a TaxFee and sub-elements in the same request.  The sub-elements will reference this ID in order to relate the two in the database insert transaction.  When adding subsequent sub-elements the client only need reference the sequence of the TaxFee.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Amount" type="xs:decimal" use="optional">
                  <xs:annotation>
                     <xs:documentation>Tax Fee total amount.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Description" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Tax Fee description.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="AmountToApply" type="xs:decimal" use="optional">
                  <xs:annotation>
                     <xs:documentation>Amount to apply per base UOM.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PerBaseUOM" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>If TRUE applies to each base unit of measure.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PerPerson" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>If TRUE applies to each guest on the component.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="ServiceFee" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>If TRUE this is a service fee and not a governmental tax.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TaxFeeCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Unique Tax or Fee code valid in M3 database.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                  <xs:annotation>
                     <xs:documentation>If amount to apply  is not used this is the percentage of the component price to charge for the tax / fee.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="CurrencyCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Currecny code of the tax / fee.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TaxCollectTypeCode" use="optional">
                  <xs:annotation>
                     <xs:documentation>TaxCollectionType	Indicates taxes are assessed by the Seller or Vendor.  Current values are:Included -- Included in Rate.UponArrival -- Taxes paid on arrival to taxing body.PreCollected -- Paid to the seller before departure.PropertyCollected -- Collected by vendor.</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="INCLUDED"/>
                        <xs:enumeration value="UPONARRIVAL"/>
                        <xs:enumeration value="PRECOLLECTED"/>
                        <xs:enumeration value="PROPERTYCOLLECTED"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="TaxCollectTypeDescription" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Description of tax collection.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="IsVAT" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>Whether or not this tax fee is a VAT amount.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="TaxFees">
            <xs:annotation>
               <xs:documentation>Contains information about taxes that apply to that Fee.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" ref="TaxFee"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="Fee">
            <xs:annotation>
               <xs:documentation>Represents a single applicable fee</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element minOccurs="0" ref="TaxFees"/>
                  <xs:element minOccurs="0" name="AppliedPromotions">
                     <xs:annotation>
                        <xs:documentation>Contains an array of promotional price adjustments applied to the selling price.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="AppliedPromotion" type="ns7:DiscountAppliedType">
                              <xs:annotation>
                                 <xs:documentation>The promotional price change (discount/markup) applied to the selling price.  The selling price contains this discount amount.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="Commissions">
                     <xs:annotation>
                        <xs:documentation>Contains an array of agent commissions earned.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="Commission" type="ns7:CommissionAppliedType">
                              <xs:annotation>
                                 <xs:documentation>Agent commission earned on this item.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="Code" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>The fee code</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Description" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Description of the fee</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Amount" type="xs:decimal" use="required">
                  <xs:annotation>
                     <xs:documentation>The fee amount</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="PromoCode" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Promotional code</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="DiscountApplied" type="xs:decimal" use="optional">
                  <xs:annotation>
                     <xs:documentation>Promotional discount amount</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="Currency" use="optional">
                  <xs:annotation>
                     <xs:documentation>The ISO currency code of the fee amount</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:pattern value="[A-Z]{3}"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="Quantity" type="xs:decimal" use="optional">
                  <xs:annotation>
                     <xs:documentation>Used to specify a quantity expressed in units of measure for the fee price paid.  For example $9.00 For 30KG of baggage</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="UnitOfMeasure" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>Used to specify the price per a unit of measure.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="AssessBy" use="optional">
                  <xs:annotation>
                     <xs:documentation>Tells the client to charge this fee for each ITINERARY (once for all options in an air itinerary) PASSENGER (once per passenger) ORIGIN_DESTINATION (once for each Origin Destination) SEGMENT (once for each air segment)  OD_PASSENGER (once for each Origin Destination per passenger) SEG_PASSENGER (once for each passenger segment)</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="ITINERARY"/>
                        <xs:enumeration value="PASSENGER"/>
                        <xs:enumeration value="ORIGIN_DESTINATION"/>
                        <xs:enumeration value="SEGMENT"/>
                        <xs:enumeration value="OD_PASSENGER"/>
                        <xs:enumeration value="SEG_PASSENGER"/>
                        <xs:enumeration value="ITEM"/>
                        <xs:enumeration value="CHECKED_PORTION"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="Mandatory" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>TRUE = The priced option requires you yo pay this fee.  FALSE=The priced option is optional.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TaxIncluded" type="xs:boolean" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="Fees">
            <xs:annotation>
               <xs:documentation>Contains a set of fees of a given type, e.g. seat fees</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="Fee"/>
               </xs:sequence>
               <xs:attribute name="Type" type="xs:string" use="optional">
                  <xs:annotation>
                     <xs:documentation>The type of fee</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="SubType" type="xs:string" use="optional"/>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:AirItinerary.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:AirItinerary.types.extensions.air.rqrs.ota.datalex.com">
         <xs:simpleType name="RPH_Type">
            <xs:annotation>
               <xs:documentation xml:lang="en">(Reference Place Holder) - an index code to identify an instance in a collection of like items.. For example,  used to assign individual passengers or clients to particular itinerary items.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[0-9]{1,8}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="StringLength1to16">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 16</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="16"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="PNREnhancement">
            <xs:annotation>
               <xs:documentation>Identifies type of upsell, its status and additional information (such as EPU account code)</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="Type" use="required">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="EPU"/>
                        <xs:enumeration value="UPSAT"/>
                        <xs:enumeration value="AWA"/>
                        <xs:enumeration value="PLA"/>
                        <xs:enumeration value="PTEPU"/>
                        <xs:enumeration value="PCUSST"/>
                        <xs:enumeration value="ASC"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="AdditionalInfo" type="StringLength1to16" use="optional"/>
               <xs:attribute name="paxRPH" type="RPH_Type" use="optional"/>
               <xs:attribute name="Status" use="required">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="ELIGIBLE"/>
                        <xs:enumeration value="PURCHASED"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="ItinTripType">
            <xs:restriction base="xs:string">
               <xs:enumeration value="OW"/>
               <xs:enumeration value="RT"/>
               <xs:enumeration value="CT"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="ItineraryAttributes">
                     <xs:complexType>
                        <xs:attribute name="Hash" type="xs:string" use="optional"/>
                        <xs:attribute name="PackageDisplaySequence" type="xs:integer" use="optional"/>
                        <xs:attribute name="Advertised" type="xs:boolean" use="optional"/>
                        <xs:attribute name="TravelPrefRanking" type="xs:string" use="optional"/>
                        <xs:attribute name="POSPrefRanking" type="xs:string" use="optional"/>
                        <xs:attribute default="false" name="PreferredFlight" type="xs:boolean" use="optional"/>
                        <xs:attribute name="TripType" type="ItinTripType" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="PNREnhancements">
                     <xs:annotation>
                        <xs:documentation>This is a collection of PNREnhancements</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="99" ref="PNREnhancement"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="ODReferences">
                     <xs:annotation>
                        <xs:documentation>(United Specific) Collection of  unique origin destination references</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="ODReference">
                              <xs:annotation>
                                 <xs:documentation>(United Specific) Unique origin destination reference</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:attribute name="RPH" type="RPH_Type" use="required"/>
                                 <xs:attribute name="UniqueODRef" type="xs:nonNegativeInteger" use="required"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="AlternateItineraries">
                     <xs:annotation>
                        <xs:documentation>Holder for alternate itineraries for the same price. Used in 'special offer' month calendar shopping flow where many itineraries for the same price are returned. usually itineraries share the same fare basis and carrier.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="OriginDestinationOption">
                              <xs:annotation>
                                 <xs:documentation>A list of alternate options.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element maxOccurs="unbounded" name="FlightSegment">
                                       <xs:annotation>
                                          <xs:documentation>Contains data relating to each flight in an option.</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:sequence>
                                             <xs:element name="DepartureAirport" type="xs:string">
                                                <xs:annotation>
                                                   <xs:documentation>Departure point of a flight segment.</xs:documentation>
                                                </xs:annotation>
                                             </xs:element>
                                             <xs:element name="ArrivalAirport" type="xs:string">
                                                <xs:annotation>
                                                   <xs:documentation xml:lang="en">Arrival point of flight segment.</xs:documentation>
                                                </xs:annotation>
                                             </xs:element>
                                             <xs:element minOccurs="0" name="OperatingAirline" type="xs:string">
                                                <xs:annotation>
                                                   <xs:documentation xml:lang="en">The operating airline of the flight if it is a codeshare  flight.</xs:documentation>
                                                </xs:annotation>
                                             </xs:element>
                                             <xs:element maxOccurs="2" minOccurs="0" name="Equipment" type="xs:string">
                                                <xs:annotation>
                                                   <xs:documentation xml:lang="en">The type of equipment  used for the  flight..</xs:documentation>
                                                </xs:annotation>
                                             </xs:element>
                                             <xs:element minOccurs="0" name="MarketingAirline" type="xs:string">
                                                <xs:annotation>
                                                   <xs:documentation xml:lang="en">The marketing airline. This is required for use with scheduled airline messages but may be omitted for requests by tour operators.</xs:documentation>
                                                </xs:annotation>
                                             </xs:element>
                                             <xs:element minOccurs="0" name="MarriageGrp" type="xs:string">
                                                <xs:annotation>
                                                   <xs:documentation xml:lang="en">Many airlines link connection flights together by terming them married segments.  When two or more segments are married, they must be processed as one unit. The segments must be moved, cancelled, and/or priced together. The value of the marriage group must be the same for all segments.</xs:documentation>
                                                </xs:annotation>
                                             </xs:element>
                                          </xs:sequence>
                                          <xs:attribute name="NameValues" type="xs:string" use="optional"/>
                                          <xs:attribute name="DepartureDateTime" type="xs:dateTime" use="required"/>
                                          <xs:attribute name="ArrivalDateTime" type="xs:dateTime" use="optional"/>
                                          <xs:attribute name="StopQuantity" type="xs:nonNegativeInteger" use="optional">
                                             <xs:annotation>
                                                <xs:documentation>The number of stops the flight makes.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                          <xs:attribute name="RPH" type="xs:string"/>
                                          <xs:attribute name="InfoSource" type="xs:string" use="optional"/>
                                          <xs:attribute name="FlightNumber" type="xs:string" use="optional">
                                             <xs:annotation>
                                                <xs:documentation>The flight number of this flight.  This is required for use with scheduled airline messages but may be omitted for requests by tour operators.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                          <xs:attribute name="TourOperatorFlightID" type="xs:string" use="optional">
                                             <xs:annotation>
                                                <xs:documentation>ID of a flight in the tour operartor's inventory.  This flight is not necessarily in the inventory of an airline.  Rather, it is a code created by tour operators.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                          <xs:attribute name="ResBookDesigCode" type="xs:string" use="optional">
                                             <xs:annotation>
                                                <xs:documentation>Specific booking class for this segment.</xs:documentation>
                                             </xs:annotation>
                                          </xs:attribute>
                                          <xs:attribute name="ActionCode" use="optional">
                                             <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                   <xs:enumeration value="OK"/>
                                                   <xs:enumeration value="Waitlist"/>
                                                   <xs:enumeration value="Other"/>
                                                </xs:restriction>
                                             </xs:simpleType>
                                          </xs:attribute>
                                          <xs:attribute name="NumberInParty" type="xs:positiveInteger" use="optional"/>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:OriginDestinationOption.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:OriginDestinationOption.types.extensions.air.rqrs.ota.datalex.com">
         <xs:import namespace="urn:FlightSegment.types.extensions.air.rqrs.ota.datalex.com"/>
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:import namespace="urn:FareInfo.types.extensions.air.rqrs.ota.datalex.com"/>
         <xs:simpleType name="RPH_Type">
            <xs:annotation>
               <xs:documentation xml:lang="en">(Reference Place Holder) - an index code to identify an instance in a collection of like items.. For example,  used to assign individual passengers or clients to particular itinerary items.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[0-9]{1,8}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="SpecialCodeTypes">
            <xs:restriction base="xs:string">
               <xs:enumeration value="ECERT"/>
               <xs:enumeration value="TSF"/>
               <xs:enumeration value="BCC"/>
               <xs:enumeration value="UPGRADE"/>
               <xs:enumeration value="CVA"/>
               <xs:enumeration value="DISCENG"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="StringLength1to16">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 16</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="16"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="PNREnhancement">
            <xs:annotation>
               <xs:documentation>Identifies type of upsell, its status and additional information (such as EPU account code)</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="Type" use="required">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="EPU"/>
                        <xs:enumeration value="UPSAT"/>
                        <xs:enumeration value="AWA"/>
                        <xs:enumeration value="PLA"/>
                        <xs:enumeration value="PTEPU"/>
                        <xs:enumeration value="PCUSST"/>
                        <xs:enumeration value="ASC"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="AdditionalInfo" type="StringLength1to16" use="optional"/>
               <xs:attribute name="paxRPH" type="RPH_Type" use="optional"/>
               <xs:attribute name="Status" use="required">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="ELIGIBLE"/>
                        <xs:enumeration value="PURCHASED"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="TPA_Extension">
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="OriginDestinationOptionAttributes">
                     <xs:complexType>
                        <xs:attribute name="Hash" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Uniquely identifies a set of flight segments.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DepartureStatus" use="optional">
                           <xs:annotation>
                              <xs:documentation>Whether or not the user is allowed to select this option, based solely on the time of departure.</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="Ok"/>
                                 <xs:enumeration value="Departed"/>
                                 <xs:enumeration value="TooCloseToDeparture"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="AvailabilityStatus" use="optional">
                           <xs:annotation>
                              <xs:documentation>The availability of this option. Only used in Shop by Schedule.</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="AvailableInSelectedCabin"/>
                                 <xs:enumeration value="FullInSelectedCabin"/>
                                 <xs:enumeration value="FullInSelectedCabinOrHigher"/>
                                 <xs:enumeration value="FullInAllCabins"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="StopCount" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
