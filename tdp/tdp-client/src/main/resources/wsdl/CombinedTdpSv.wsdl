         <xs:import namespace="urn:parking.rqrs.datalex.com"/>
         <xs:import namespace="urn:reservation.rqrs.datalex.com"/>
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05/profile"/>
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05/car"/>
         <xs:import namespace="urn:ancillaryaircomponent.rqrs.datalex.com"/>
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05"/>
         <xs:import namespace="urn:activity.rqrs.datalex.com"/>
         <xs:import namespace="urn:insurance.rqrs.xsd.datalex.com"/>
         <xs:import namespace="urn:groundtransportation.rqrs.datalex.com"/>
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:import namespace="urn:util.cbp.rqrs.ota.matrix.datalex.com"/>
         <xs:import namespace="urn:rqrs.cbp.datalex.com"/>
         <xs:import namespace="urn:Errors.types.extensions.air.rqrs.ota.datalex.com"/>
         <xs:complexType name="CustomerDataAddType">
            <xs:choice>
               <xs:element name="ProfileID" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>If the client passes a unique profile ID, TDP will retrieve the profile from a supported adaptor and create the customer from the data,a nd link to the profile.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element ref="ns6:ProfileInfo"/>
               <xs:element ref="ns33:Customer"/>
            </xs:choice>
            <xs:attribute name="isTraveler" type="xs:boolean" use="optional"/>
         </xs:complexType>
         <xs:element name="AddCustomer">
            <xs:annotation>
               <xs:documentation>Contains either a Customer or a Profile Id with which a Customer can be identified.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="CustomerDataAddType">
                     <xs:attribute name="CustomerDocID" type="xs:ID" use="required">
                        <xs:annotation>
                           <xs:documentation>Reference pointer to this customer within this message for additions of both customer and objects that reference customer at the same time.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="GuestTypeCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>A guest type code that is configured for TDP, like 'ADT', or '10.AQC'. It may be in any travel domain. Note: only required here if customer is also a traveler.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ComponentTypeCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The travel domain associated with the GuestTypeCode, like 'AIR' or 'LODGING'. Note: only required here if customer is also a traveler.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="PricedItinerary">
            <xs:annotation>
               <xs:documentation xml:lang="en">Container for priced itinerary type.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="ns8:PricedItineraryType">
                     <xs:attribute name="MoneyMilesCombinationIndex" type="xs:positiveInteger" use="optional">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Index of the selected money and miles combination. Only prices from this combination will be persisted to reservation.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="TravelerRemoteRef" type="ns33:RemoteRefType">
            <xs:annotation>
               <xs:documentation>Contains the unique identifier to the traveler on the remote host. For example, name-numbers. This can be used alternately with TravelerReference</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="TravelerReference">
            <xs:annotation>
               <xs:documentation>For this priced option, references either an existing TDP Reservation Traveler Sequence, or a Traveler in this message.  The client may also include a document or ticket issued outside of TDP.  Adding a document in this way indicates that the component for this traveler was paid for outside of TDP.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element minOccurs="0" ref="ns33:Document"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="TravelerRemoteRef">
                     <xs:annotation>
                        <xs:documentation>Contains the unique identifier to the traveler on the remote host. For example, name-numbers. This can be used alternately with TravelerReference</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="TravelerSequence" type="xs:positiveInteger" use="optional">
                  <xs:annotation>
                     <xs:documentation>A unique Traveler Sequence in a TDP Reservation</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="TravelerDocRef" type="xs:IDREF" use="optional">
                  <xs:annotation>
                     <xs:documentation>Reference to an ID to a traveler in this message.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="IsDriver" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>For car rentals indicated that this traveler is the driver</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="DateOfBirth" type="xs:date" use="optional"/>
               <xs:attribute name="Age" type="xs:nonNegativeInteger" use="optional"/>
               <xs:attribute name="Nationality" type="xs:string" use="optional"/>
               <xs:attribute name="IsLead" type="xs:boolean" use="optional">
                  <xs:annotation>
                     <xs:documentation>In certain travel domains like ground transportation, used to indicate this is the lead traveller who supplied contact info</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="CustomerRemoteRef" type="ns33:RemoteRefType">
            <xs:annotation>
               <xs:documentation>Contains the unique identifier to the customer on the remote host.  For example, name-numbers. This can be used alternately with CustomerReference</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="CustomerReference">
            <xs:annotation>
               <xs:documentation>For this priced option, references either an existing TDP Reservation Customer Sequence, or a Customer in this message.  The client may also include a payment form to use for payment of this component, or that was used for a component purchased outside of TDP.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns33:Payment"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" ref="CustomerRemoteRef">
                     <xs:annotation>
                        <xs:documentation>Contains the unique identifier to the customer on the remote host.  For example, name-numbers. This can be used alternately with CustomerReference</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="BillingAddress" type="ns7:AddressType">
                     <xs:annotation>
                        <xs:documentation>The address that is valid to be used with the payment form.  This may differ from the customer's address.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="CustomerEmailAddress" type="ns7:EmailType">
                     <xs:annotation>
                        <xs:documentation>This e-mail address will replace any customer e-mail address(es) already present in the reservation, as TDP assumes this new address will be used for all future purchase confirmations.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
               <xs:attribute name="CustomerSequence" type="xs:positiveInteger" use="optional">
                  <xs:annotation>
                     <xs:documentation>A unique Customer Sequence in a TDP Reservation</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="CustomerDocRef" type="xs:IDREF" use="optional">
                  <xs:annotation>
                     <xs:documentation>Reference to an ID to a customer in this message.</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="Traveler">
            <xs:annotation>
               <xs:documentation>The guest will be called traveler in the facades.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="ns33:GuestType"/>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="TravelerDataAddType">
            <xs:choice>
               <xs:element name="ProfileID" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>If the client passes a unique profile ID, TDP will retrieve the profile from a supported adaptor and create the customer from the data,a nd link to the profile.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element ref="ns6:ProfileInfo"/>
               <xs:element ref="Traveler"/>
            </xs:choice>
            <xs:attribute name="isCustomer" type="xs:boolean" use="optional"/>
         </xs:complexType>
         <xs:element name="AddTraveler">
            <xs:annotation>
               <xs:documentation>Contains either a Traveler or a Profile Id with which a Traveler can be identified. Include one element for each Traveler you intend to add.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="TravelerDataAddType">
                     <xs:attribute name="TravelerDocID" type="xs:ID" use="required">
                        <xs:annotation>
                           <xs:documentation>Reference pointer to this traveler within this message for additions of both traveler and objects that reference traveler at the same time.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="GuestTypeCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>A guest type code that is configured for TDP, like 'ADT', or '10.AQC'. It may be in any travel domain.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ComponentTypeCode" type="xs:string" use="required">
                        <xs:annotation>
                           <xs:documentation>The travel domain associated with the GuestTypeCode, like 'AIR' or 'LODGING'.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="HostBookingReference">
            <xs:annotation>
               <xs:documentation>When supplied by the client, indicates that the component was booked outside of TDP.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="ns33:RemoteRefType">
                     <xs:attribute name="RemoteRefType" use="optional">
                        <xs:simpleType>
                           <xs:restriction base="xs:string">
                              <xs:enumeration value="HOST_PNR_REF"/>
                              <xs:enumeration value="GDS_PNR_REF"/>
                              <xs:enumeration value="BOOKING_SYSTEM_CONFIRMATION"/>
                              <xs:enumeration value="BOOKING_SYSTEM_CANCEL_CONFIRMATION"/>
                              <xs:enumeration value="PROVIDER_CONFIRMATION"/>
                              <xs:enumeration value="PROVIDER_CANCEL_CONFIRMATION"/>
                              <xs:enumeration value="HOST_QUEUE_NUMBER"/>
                              <xs:enumeration value="HOST_PSEUDO_CITY_CODE"/>
                              <xs:enumeration value="HOST_QUEUE_CATEGORY"/>
                              <xs:enumeration value="GDS_FARE_QUOTE_REF"/>
                              <xs:enumeration value="TICKETING_FOLDER_NUMBER"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="ReservationCodeType">
            <xs:restriction base="xs:string"/>
         </xs:simpleType>
         <xs:complexType name="TDPReservationBaseSvRQType">
            <xs:complexContent>
               <xs:extension base="ns1:CBPSvRQBase">
                  <xs:sequence minOccurs="0">
                     <xs:element ref="ns33:ReservationUpdateSvRQ"/>
                  </xs:sequence>
                  <xs:attribute name="Username" type="xs:string" use="required">
                     <xs:annotation>
                        <xs:documentation>Username is a token that denotes a unique user and session.  When TDP creates or logically locks a reservation, the username used to lock the reservation is used to subsequently add to or change the contents of the reservation and restrict others from updates while the user has the logical lock. 

Pass the username if you have a lock on the reservation.  If you do not have a lock on the reservation, an exception will be issued.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="Source" type="xs:string" use="required"/>
                  <xs:attribute name="Description" type="xs:string" use="optional"/>
                  <xs:attribute name="Status" type="xs:string" use="optional"/>
                  <xs:attribute default="true" name="ReturnReservationInResponseFlag" type="xs:boolean" use="optional"/>
                  <xs:attribute default="false" name="Update" type="xs:boolean" use="optional"/>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:element name="ReservationAddSvRQ">
            <xs:annotation>
               <xs:documentation>This service provides controller logic that aggregates the detailed TDP Reservation services necessary to create a reservation, and add new components, customers, travelers, and associated information.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="TDPReservationBaseSvRQType">
                     <xs:sequence>
                        <xs:element name="AddRequest">
                           <xs:annotation>
                              <xs:documentation>Contains a choice between adding new components directly or retrieving from a remote host.</xs:documentation>
                           </xs:annotation>
                           <xs:complexType>
                              <xs:sequence>
                                 <xs:element name="AddComponentsRQ">
                                    <xs:annotation>
                                       <xs:documentation>Add new components, customers, travelers, payments and documents to a new or existing TDP reservation</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                       <xs:sequence>
                                          <xs:element maxOccurs="unbounded" minOccurs="0" ref="AddCustomer"/>
                                          <xs:element maxOccurs="unbounded" minOccurs="0" ref="AddTraveler"/>
                                          <xs:element maxOccurs="unbounded" name="AddPricedOption">
                                             <xs:annotation>
                                                <xs:documentation>Conatins a choice of adding priced options from vairous travel domains.  Make one add request for each added priced option.</xs:documentation>
                                             </xs:annotation>
                                             <xs:complexType>
                                                <xs:sequence>
                                                   <xs:element name="PricedOption">
                                                      <xs:annotation>
                                                         <xs:documentation>Contains a choice of priced options from various domains, referenced to customer, travelers, payments, and remote references</xs:documentation>
                                                      </xs:annotation>
                                                      <xs:complexType>
                                                         <xs:sequence>
                                                            <xs:choice>
                                                               <xs:element name="AirOption">
                                                                  <xs:annotation>
                                                                     <xs:documentation>An Air Priced Option including Fare Family references</xs:documentation>
                                                                  </xs:annotation>
                                                                  <xs:complexType>
                                                                     <xs:sequence>
                                                                        <xs:element ref="PricedItinerary"/>
                                                                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns12:FareFamilySummary"/>
                                                                     </xs:sequence>
                                                                  </xs:complexType>
                                                               </xs:element>
                                                               <xs:element name="HotelOption">
                                                                  <xs:annotation>
                                                                     <xs:documentation>A Hotel Priced Option including room extras and special requests</xs:documentation>
                                                                  </xs:annotation>
                                                                  <xs:complexType>
                                                                     <xs:sequence>
                                                                        <xs:element ref="ns4:RoomStay"/>
                                                                        <xs:element maxOccurs="unbounded" minOccurs="0" name="RoomExtra">
                                                                           <xs:annotation>
                                                                              <xs:documentation>This represents the type of room extra that is to be availed together with the room. This room extra should have corresponding details in the AdditionalGuestAmount. Any room extra from the AdditionalGuestAmount that is not indicated here will not be saved.</xs:documentation>
                                                                           </xs:annotation>
                                                                           <xs:complexType>
                                                                              <xs:attribute name="Code" type="ns4:OTA_CodeType" use="required">
                                                                                 <xs:annotation>
                                                                                    <xs:documentation>OTA Type code for special requests e.g. Extra bed, extra cots</xs:documentation>
                                                                                 </xs:annotation>
                                                                              </xs:attribute>
                                                                              <xs:attribute name="Quantity" type="ns4:Numeric1to999" use="required">
                                                                                 <xs:annotation>
                                                                                    <xs:documentation>Number of the extras required</xs:documentation>
                                                                                 </xs:annotation>
                                                                              </xs:attribute>
                                                                              <xs:attribute name="Description" type="xs:string" use="optional">
                                                                                 <xs:annotation>
                                                                                    <xs:documentation>Description of the room extra, cot or extra bed for example</xs:documentation>
                                                                                 </xs:annotation>
                                                                              </xs:attribute>
                                                                           </xs:complexType>
                                                                        </xs:element>
                                                                        <xs:element minOccurs="0" name="SpecialRequests" type="ns4:SpecialRequestType">
                                                                           <xs:annotation>
                                                                              <xs:documentation>Remarks to be added to rescomponent</xs:documentation>
                                                                           </xs:annotation>
                                                                        </xs:element>
                                                                     </xs:sequence>
                                                                  </xs:complexType>
                                                               </xs:element>
                                                               <xs:element name="CarOption">
                                                                  <xs:annotation>
                                                                     <xs:documentation>A Car Priced Option</xs:documentation>
                                                                  </xs:annotation>
                                                                  <xs:complexType>
                                                                     <xs:sequence>
                                                                        <xs:element ref="ns31:VehRentalCore"/>
                                                                        <xs:element ref="ns31:VehVendorAvail"/>
                                                                     </xs:sequence>
                                                                  </xs:complexType>
                                                               </xs:element>
                                                               <xs:element name="InsuranceOption">
                                                                  <xs:annotation>
                                                                     <xs:documentation>An Insurance Priced Option</xs:documentation>
                                                                  </xs:annotation>
                                                                  <xs:complexType>
                                                                     <xs:sequence>
                                                                        <xs:element ref="ns23:InsurancePriceOption"/>
                                                                        <xs:element minOccurs="0" ref="ns33:ResComponentGroup"/>
                                                                     </xs:sequence>
                                                                  </xs:complexType>
                                                               </xs:element>
                                                               <xs:element name="AncillaryAirComponentOption">
                                                                  <xs:annotation>
                                                                     <xs:documentation>An Ancillary Air Priced Option including Air Component reference.</xs:documentation>
                                                                  </xs:annotation>
                                                                  <xs:complexType>
                                                                     <xs:sequence>
                                                                        <xs:element ref="ns14:AncillaryAirComponentPricedOptions"/>
                                                                        <xs:element minOccurs="0" name="RelatedAirReservationComponent">
                                                                           <xs:annotation>
                                                                              <xs:documentation>If the ancillary item is flight related, and the reservation already contains air component(s), this identifies the referenced air component.  Use the AirComponentSequence when referencing an existing reservation component.  Use the Air OptionDocRef when refering to a air option in this message.</xs:documentation>
                                                                           </xs:annotation>
                                                                           <xs:complexType>
                                                                              <xs:attribute name="AirComponentSequence" type="xs:positiveInteger" use="optional">
                                                                                 <xs:annotation>
                                                                                    <xs:documentation>Unique reservation sequence for the referenced air component within the TDP reservation.</xs:documentation>
                                                                                 </xs:annotation>
                                                                              </xs:attribute>
                                                                              <xs:attribute name="AirOptionDocRef" type="xs:IDREF" use="optional">
                                                                                 <xs:annotation>
                                                                                    <xs:documentation>Use the Air OptionDocRef when refering to a air option in this message.</xs:documentation>
                                                                                 </xs:annotation>
                                                                              </xs:attribute>
                                                                           </xs:complexType>
                                                                        </xs:element>
                                                                     </xs:sequence>
                                                                  </xs:complexType>
                                                               </xs:element>
                                                               <xs:element name="ActivityTourOption">
                                                                  <xs:annotation>
                                                                     <xs:documentation>An activity tour priced option and descriptive content necessary for the reservation.</xs:documentation>
                                                                  </xs:annotation>
                                                                  <xs:complexType>
                                                                     <xs:sequence>
                                                                        <xs:element ref="ns24:DetailedContent"/>
                                                                        <xs:element ref="ns24:ActivityTourPricedOption"/>
                                                                     </xs:sequence>
                                                                  </xs:complexType>
                                                               </xs:element>
                                                               <xs:element name="GroundTransportationOption">
                                                                  <xs:annotation>
                                                                     <xs:documentation>A ground transport priced option and product details.</xs:documentation>
                                                                  </xs:annotation>
                                                                  <xs:complexType>
                                                                     <xs:sequence>
                                                                        <xs:element ref="ns26:GroundTransportationProduct"/>
                                                                        <xs:element ref="ns26:GroundTransportationPricedOption"/>
                                                                     </xs:sequence>
                                                                  </xs:complexType>
                                                               </xs:element>
                                                               <xs:element name="ParkingOption">
                                                                  <xs:annotation>
                                                                     <xs:documentation>A parking priced option and parking lot details.</xs:documentation>
                                                                  </xs:annotation>
                                                                  <xs:complexType>
                                                                     <xs:sequence>
                                                                        <xs:element ref="ns25:ParkingLot"/>
                                                                        <xs:element ref="ns25:ParkingPricedOption"/>
                                                                     </xs:sequence>
                                                                  </xs:complexType>
                                                               </xs:element>
                                                               <xs:element name="PackageOption">
                                                                  <xs:annotation>
                                                                     <xs:documentation>A group of options that are priced as a package.</xs:documentation>
                                                                  </xs:annotation>
                                                                  <xs:complexType>
                                                                     <xs:sequence>
                                                                        <xs:element name="PackageDetails">
                                                                           <xs:complexType>
                                                                              <xs:attribute name="PackageCode" type="xs:string" use="optional">
                                                                                 <xs:annotation>
                                                                                    <xs:documentation>The package code.</xs:documentation>
                                                                                 </xs:annotation>
                                                                              </xs:attribute>
                                                                              <xs:attribute name="PackageDescription" type="xs:string" use="optional">
                                                                                 <xs:annotation>
                                                                                    <xs:documentation>Description of the package.</xs:documentation>
                                                                                 </xs:annotation>
                                                                              </xs:attribute>
                                                                              <xs:attribute name="PackageTemplateHeaderCode" type="xs:string" use="optional">
                                                                                 <xs:annotation>
                                                                                    <xs:documentation>Package template which this package belongs to.</xs:documentation>
                                                                                 </xs:annotation>
                                                                              </xs:attribute>
                                                                              <xs:attribute name="PackageHistorySequence" type="xs:positiveInteger" use="optional"/>
                                                                           </xs:complexType>
                                                                        </xs:element>
                                                                        <xs:element maxOccurs="unbounded" name="PackagePricedOption">
                                                                           <xs:complexType>
                                                                              <xs:attribute name="AddPricedOptionDocRef" type="xs:IDREF" use="required">
                                                                                 <xs:annotation>
                                                                                    <xs:documentation>A pointer to a priced option being included in this package.</xs:documentation>
                                                                                 </xs:annotation>
                                                                              </xs:attribute>
                                                                           </xs:complexType>
                                                                        </xs:element>
                                                                     </xs:sequence>
                                                                  </xs:complexType>
                                                               </xs:element>
                                                            </xs:choice>
                                                            <xs:element maxOccurs="unbounded" minOccurs="0" ref="TravelerReference"/>
                                                            <xs:element minOccurs="0" ref="CustomerReference"/>
                                                            <xs:element minOccurs="0" ref="HostBookingReference"/>
                                                            <xs:element maxOccurs="unbounded" minOccurs="0" ref="ns7:ExtraInformation"/>
                                                         </xs:sequence>
                                                      </xs:complexType>
                                                   </xs:element>
                                                </xs:sequence>
                                                <xs:attribute name="AddPricedOptionDocID" type="xs:ID" use="optional">
                                                   <xs:annotation>
                                                      <xs:documentation>Referencable ID for this option when adding new and correlating with other elements in this message</xs:documentation>
                                                   </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute name="ResultingComponentStatus" type="xs:string" use="optional">
                                                   <xs:annotation>
                                                      <xs:documentation>TDP will set the Reservation Component Status of this component to the status supplied. This is NOT the component's Internal Status.</xs:documentation>
                                                   </xs:annotation>
                                                </xs:attribute>
                                                <xs:attribute default="false" name="BookAfterAdd" type="xs:boolean" use="optional">
                                                   <xs:annotation>
                                                      <xs:documentation>If TRUE, the component will be booked after addition to the reservation given that all the necessary information is available.</xs:documentation>
                                                   </xs:annotation>
                                                </xs:attribute>
                                             </xs:complexType>
                                          </xs:element>
                                       </xs:sequence>
                                    </xs:complexType>
                                 </xs:element>
                              </xs:sequence>
                           </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                     <xs:attribute name="ReservationCode" type="ReservationCodeType" use="optional">
                        <xs:annotation>
                           <xs:documentation>If you are adding additional components, customers, travelers, or other information to a TDP reservation, you must pass the unique reservation code you wish to add to.  
If you do not supply a TDP unique reservation code, the service will create a new TDP reservation.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="ResultingReservationStatus" type="xs:string" use="optional"/>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="TDPReservationBaseSvRSType">
            <xs:complexContent>
               <xs:extension base="ns1:CBPSvRSBase">
                  <xs:sequence>
                     <xs:choice>
                        <xs:sequence>
                           <xs:element name="Success">
                              <xs:complexType>
                                 <xs:complexContent>
                                    <xs:extension base="ns8:SuccessType"/>
                                 </xs:complexContent>
                              </xs:complexType>
                           </xs:element>
                           <xs:element minOccurs="0" name="Warnings">
                              <xs:complexType>
                                 <xs:complexContent>
                                    <xs:extension base="ns8:WarningsType"/>
                                 </xs:complexContent>
                              </xs:complexType>
                           </xs:element>
                           <xs:element minOccurs="0" name="ReservationData">
                              <xs:annotation>
                                 <xs:documentation>Successfull Reservation response contain either reservation data or the reservation code..</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:choice>
                                    <xs:element name="ReservationCode" type="xs:string"/>
                                    <xs:element ref="ns33:Reservation"/>
                                    <xs:element ref="ns33:ReservationUpdateSvRQ"/>
                                    <xs:element minOccurs="0" name="ScheduleChangeRejectWindowDays">
                                       <xs:annotation>
                                          <xs:documentation>The attributes of the Element would be set by the Result of the BRE where they are configured.</xs:documentation>
                                       </xs:annotation>
                                       <xs:complexType>
                                          <xs:attribute name="DaysBefore" type="xs:string" use="optional"/>
                                          <xs:attribute name="DaysAfter" type="xs:string" use="optional"/>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:choice>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                        <xs:element name="Errors">
                           <xs:complexType>
                              <xs:complexContent>
                                 <xs:extension base="ns8:ErrorsType">
                                    <xs:sequence>
                                       <xs:element minOccurs="0" ref="ns19:TPA_Extension"/>
                                    </xs:sequence>
                                 </xs:extension>
                              </xs:complexContent>
                           </xs:complexType>
                        </xs:element>
                     </xs:choice>
                  </xs:sequence>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:element name="ComponentExtraInformation" type="ns7:ExtraInformationType">
            <xs:annotation>
               <xs:documentation>This is an echo back of the ExtraInformation elements that were added to the reservation component.  This is good for linking identifiers from the calling system with TDP components.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="ReservationAddSvRS">
            <xs:annotation>
               <xs:documentation>Contains responses to ReservationAddSvRQ requests.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="TDPReservationBaseSvRSType">
                     <xs:sequence minOccurs="0">
                        <xs:element name="AddRS">
                           <xs:complexType>
                              <xs:sequence>
                                 <xs:element maxOccurs="unbounded" minOccurs="0" name="CustomerAdded">
                                    <xs:annotation>
                                       <xs:documentation>Contains the unique sequence for a customer sucessfully added</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                       <xs:sequence>
                                          <xs:element maxOccurs="unbounded" minOccurs="0" ref="CustomerRemoteRef"/>
                                       </xs:sequence>
                                       <xs:attribute name="ReservationCustomerSequence" type="xs:positiveInteger" use="required">
                                          <xs:annotation>
                                             <xs:documentation>The unique identifying Customer sequence within the TDP reservation.  Use this sequence when referencing the customer in subsequent reservation calls.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                    </xs:complexType>
                                 </xs:element>
                                 <xs:element maxOccurs="unbounded" minOccurs="0" name="TravelerAdded">
                                    <xs:annotation>
                                       <xs:documentation>Contains the unique sequence for a traveler successfully added</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                       <xs:sequence>
                                          <xs:element maxOccurs="unbounded" minOccurs="0" ref="TravelerRemoteRef"/>
                                       </xs:sequence>
                                       <xs:attribute name="ReservationTravelerSequence" type="xs:positiveInteger" use="required">
                                          <xs:annotation>
                                             <xs:documentation>The unique identifying Traveler sequence within the TDP reservation.  Use this sequence when referencing the traveler in subsequent reservation calls.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                    </xs:complexType>
                                 </xs:element>
                                 <xs:element maxOccurs="unbounded" name="PricedOptionAdded">
                                    <xs:annotation>
                                       <xs:documentation>Contains an indication that a component was successfully added to the TDP reservation and some identifiers.</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                       <xs:sequence>
                                          <xs:element minOccurs="0" ref="HostBookingReference"/>
                                          <xs:element maxOccurs="unbounded" minOccurs="0" ref="ComponentExtraInformation"/>
                                       </xs:sequence>
                                       <xs:attribute name="ReservationComponentSequence" type="xs:positiveInteger" use="required">
                                          <xs:annotation>
                                             <xs:documentation>The unique sequence of the travel component in the TDP reservation.  Use this sequence to refer to the component on subsequent calls.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                       <xs:attribute name="ReservationComponentTypeCode" type="xs:string" use="required">
                                          <xs:annotation>
                                             <xs:documentation>Component Type Code valid in the TDP database.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                       <xs:attribute name="ComponentSubTypeCode" type="xs:string" use="optional">
                                          <xs:annotation>
                                             <xs:documentation>Component Sub Type Code valid in the TDP database.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                       <xs:attribute name="ReservationComponentStatus" type="xs:string" use="optional">
                                          <xs:annotation>
                                             <xs:documentation>The resulting status of the TDP reservation component.  This is the user-defined status, not the internal status.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                       <xs:attribute name="ReservationComponentGroupSequence" type="xs:positiveInteger" use="optional">
                                          <xs:annotation>
                                             <xs:documentation>If this component is grouped with other components as part of a bundle or package, then this is the sequence of the ResComponentGroup the component belongs to.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                       <xs:attribute name="PackageCode" type="xs:string" use="optional">
                                          <xs:annotation>
                                             <xs:documentation>The package code if this component is part of a package.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                    </xs:complexType>
                                 </xs:element>
                              </xs:sequence>
                           </xs:complexType>
                        </xs:element>
                     </xs:sequence>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
         <xs:element name="ReservationCancelRemoveSvRQ">
            <xs:annotation>
               <xs:documentation>This service provides controller logic for removing or cancelling reservation components.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:complexContent>
                  <xs:extension base="TDPReservationBaseSvRQType">
                     <xs:sequence>
                        <xs:element name="CancelRemoveRQ">
                           <xs:complexType>
                              <xs:choice>
                                 <xs:element maxOccurs="unbounded" name="RetrievePoliciesAndStatus">
                                    <xs:annotation>
                                       <xs:documentation>Before cancelling or removing components, you can retrieve the cancellation policies that apply to it, and also check whether the component can be removed or cancelled.</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                       <xs:attribute name="ReservationComponentSequence" type="xs:positiveInteger" use="optional">
                                          <xs:annotation>
                                             <xs:documentation>The unique sequence of the travel component in the TDP reservation.  Use this sequence to refer to the component on subsequent calls.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                       <xs:attribute name="AllReservationComponents" type="xs:boolean" use="optional">
                                          <xs:annotation>
                                             <xs:documentation>Specify that operation pertains to all reservation components.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                    </xs:complexType>
                                 </xs:element>
                                 <xs:element maxOccurs="unbounded" name="CancelRemoveComponent">
                                    <xs:annotation>
                                       <xs:documentation>If the component was booked, it will be cancelled on the remote system; else, it will just be removed from the reservation.</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                       <xs:attribute name="ReservationComponentSequence" type="xs:positiveInteger" use="optional">
                                          <xs:annotation>
                                             <xs:documentation>The unique sequence of the travel component in the TDP reservation.  Use this sequence to refer to the component on subsequent calls.</xs:documentation>
                                          </xs:annotation>
                                       </xs:attribute>
                                       <xs:attribute name="AllReservationComponents" type="xs:boolean" use="optional">
                                          <xs:annotation>
                                             <xs:documentation>Specify that operation pertains to all reservation components.</xs:documentation>
