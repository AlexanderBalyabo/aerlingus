                     <xs:documentation>SocialContent Type unique identifier</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="SocialContentType" type="SocialContentTypeEnum">
                  <xs:annotation>
                     <xs:documentation>SocialContent type value</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="VariableName" type="xs:string">
                  <xs:annotation>
                     <xs:documentation>Velocity template variable name</xs:documentation>
                  </xs:annotation>
               </xs:attribute>
               <xs:attribute name="MetaTagName" type="xs:string">
                  <xs:annotation>
