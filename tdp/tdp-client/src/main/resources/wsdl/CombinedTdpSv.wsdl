                     </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="concourseTerminal">
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:annotation>
                              <xs:documentation xml:lang="en">Edifact representation: an..2</xs:documentation>
                           </xs:annotation>
                           <xs:minLength value="1"/>
                           <xs:maxLength value="2"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="StringLength1to16">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 16</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="16"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="PNREnhancement">
            <xs:annotation>
               <xs:documentation>Identifies type of upsell, its status and additional information (such as EPU account code)</xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:attribute name="Type" use="required">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="EPU"/>
                        <xs:enumeration value="UPSAT"/>
                        <xs:enumeration value="AWA"/>
                        <xs:enumeration value="PLA"/>
                        <xs:enumeration value="PTEPU"/>
                        <xs:enumeration value="PCUSST"/>
                        <xs:enumeration value="ASC"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute name="AdditionalInfo" type="StringLength1to16" use="optional"/>
               <xs:attribute name="paxRPH" type="RPH_Type" use="optional"/>
               <xs:attribute name="Status" use="required">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:enumeration value="PURCHASED"/>
                        <xs:enumeration value="ELIGIBLE"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:element name="CabinType" type="xs:string">
            <xs:annotation>
               <xs:documentation>Identifier for which type of cabin this is. Valid
values are F, C, or Y.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="arrivalStation">
            <xs:complexType>
               <xs:sequence>
                  <xs:element minOccurs="0" name="gate">
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:annotation>
                              <xs:documentation xml:lang="en">Edifact representation: an..6</xs:documentation>
                           </xs:annotation>
                           <xs:minLength value="1"/>
                           <xs:maxLength value="6"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="terminal">
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:annotation>
                              <xs:documentation xml:lang="en">Edifact representation: an..2</xs:documentation>
                           </xs:annotation>
                           <xs:minLength value="1"/>
                           <xs:maxLength value="2"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="concourseTerminal">
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:annotation>
                              <xs:documentation xml:lang="en">Edifact representation: an..2</xs:documentation>
                           </xs:annotation>
                           <xs:minLength value="1"/>
                           <xs:maxLength value="2"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
         <xs:element name="ArrivalAirport">
            <xs:complexType mixed="true">
               <xs:annotation>
                  <xs:documentation>Used to inject name value pairs into elements (internal use only).</xs:documentation>
               </xs:annotation>
               <xs:attribute name="LocationCode" use="required">
                  <xs:annotation>
                     <xs:documentation>The IATA 3 letter arrival airport code of the flight segment.
Example: ORD</xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="8"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
               <xs:attribute default="IATA" name="CodeContext" use="optional">
                  <xs:simpleType>
                     <xs:restriction base="xs:string">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="8"/>
                     </xs:restriction>
                  </xs:simpleType>
               </xs:attribute>
            </xs:complexType>
         </xs:element>
         <xs:simpleType name="StringLength3">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Strings, length exactly 3</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="3"/>
               <xs:maxLength value="3"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="Equipment">
            <xs:complexType>
               <xs:attribute name="AirEquipType" type="StringLength3" use="optional"/>
               <xs:attribute name="ChangeofGauge" type="xs:boolean" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="FlightSegmentSupplementalInfo">
                     <xs:annotation>
                        <xs:documentation>A group of attributes that apply to this FlightSegment</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="MealService" type="xs:boolean" use="optional"/>
                        <xs:attribute name="UpgradeEligible" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>Indicates whether the flight segment has a booking code that is upgrade eligible. Note: this attribute will only be present if the user has selected an upgrade option.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DepartureTimeZoneOffset" type="xs:string" use="optional"/>
                        <xs:attribute name="ArrivalTimeZoneOffset" type="xs:string" use="optional"/>
                        <xs:attribute name="DifferentDayOfArrival" type="xs:integer" use="optional">
                           <xs:annotation>
                              <xs:documentation>Can have one of the four following values:
-1 - arrives previous day
0 - arrives same day
1 - arrives next day
2 - arrives 2 days from departur</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DayChangeIndDeparture" type="xs:integer" use="optional"/>
                        <xs:attribute name="DayChangeIndArrival" type="xs:integer" use="optional"/>
                        <xs:attribute name="FlightSegmentStatusCode" type="xs:string" use="optional"/>
                        <xs:attribute name="PricingFlightSegmentStatusCode" use="optional">
                           <xs:annotation>
                              <xs:documentation>Indicates the status of the FlightSegment after a pricing transaction.</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="Available">
                                    <xs:annotation>
                                       <xs:documentation>Flight segment was successfully verified in a pricing transaction and can be used in a Reservation.</xs:documentation>
                                    </xs:annotation>
                                 </xs:enumeration>
                                 <xs:enumeration value="Waitlist">
                                    <xs:annotation>
                                       <xs:documentation>Flight segment was determined to be invalid by a pricing transaction and cannot be used in a Reservation. This is due to the booking class being on waitlist.</xs:documentation>
                                    </xs:annotation>
                                 </xs:enumeration>
                                 <xs:enumeration value="NotAvailable">
                                    <xs:annotation>
                                       <xs:documentation>Flight segment was determined to be invalid by a pricing transaction and cannot be used in a Reservation. This could be due to the booking class being sold out or the flight number does not operate on specified date.</xs:documentation>
                                    </xs:annotation>
                                 </xs:enumeration>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="OperationalDisclosure" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>The full name of the operating carrier</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CabinCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Specifies the cabin associated with the booking code (as one carrier may designate 'R' as being in the First class cabin, where another may allocate this booking code to the Economy cabin). The cabin is denoted by a single letter:</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CabinDescription" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is a string that describes the cabin or sub-cabin associated with the booking code.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CabinDescriptionKey" type="xs:string" use="optional"/>
                        <xs:attribute name="FlightMiles" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>The distance traveled for this flight segment</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="FrequentFlierMiles" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>the miles accrued on the frequent flier programme for this flight segment</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ElapsedFlyingTime" type="xs:duration" use="optional">
                           <xs:annotation>
                              <xs:documentation>The total flying time for this segment (including time spent on the ground for stops).</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="OnTimeRate" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>A figure expressing the flight's on-time performance as a percentage.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="InternationalItineraryIndicator" type="xs:boolean" use="optional"/>
                        <xs:attribute name="BaggageAllowance" type="xs:string" use="optional"/>
                        <xs:attribute name="CabinUpsellEligible" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is a boolean flag which indicates whether or not a particular Orig and Dest is eligible for a cabin upsell</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="EligibleForModify" type="xs:boolean" use="optional"/>
                        <xs:attribute name="ScheduleChanged" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>A flag indicating if the flight segment schedule had changed</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="FlightSegmentStatusText" type="xs:string" use="optional"/>
                        <xs:attribute name="EPUAccountCode" type="xs:string" use="optional"/>
                        <xs:attribute name="FlownIndicator" use="optional">
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="NA"/>
                                 <xs:enumeration value="DEPARTED"/>
                                 <xs:enumeration value="NOTDEPARTED"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="MarriageIndex" type="xs:string" use="optional"/>
                        <xs:attribute name="NumberOfCabins" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>Number of cabins (eg., 1, 2 or 3) in the flight</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="WIFISupport" use="optional">
                           <xs:annotation>
                              <xs:documentation>Flag to indicate if aircraft is wifi capable or not</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:minLength value="1"/>
                                 <xs:maxLength value="2"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="99" minOccurs="0" name="Comment" type="xs:string"/>
                  <xs:element maxOccurs="10" minOccurs="0" name="Availability">
                     <xs:annotation>
                        <xs:documentation>Each Availability element holds the booking codes/seat counts for a single cabin.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:annotation>
                           <xs:documentation xml:lang="en">A more compact form of BookingClassAvailability. Holds availability per cabin in "F4|A2|..." format.</xs:documentation>
                        </xs:annotation>
                        <xs:complexContent>
                           <xs:extension base="ns7:CustomProperties">
                              <xs:attribute name="CabinCode" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>Specifies the cabin associated with the booking codes (as one carrier may designate 'R' as being in the First class cabin, where another may allocate this booking code to the Economy cabin).</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="CabinDescription" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>This is a string that describes the cabin or sub-cabin associated with the booking codes</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                              <xs:attribute name="Details" type="xs:string" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>This is a string containing the booking codes and associated seat counts for this cabin</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                           </xs:extension>
                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="99" minOccurs="0" name="BookingClassAvailability">
                     <xs:complexType>
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Holds booking class and available seats quantity.</xs:documentation>
                        </xs:annotation>
                        <xs:complexContent>
                           <xs:extension base="ns7:CustomProperties">
                              <xs:attribute name="ResBookDesigCode" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">Reservation Booking Designator (RBD) code (e.g. Y).</xs:documentation>
                                 </xs:annotation>
                                 <xs:simpleType>
                                    <xs:annotation>
                                       <xs:documentation xml:lang="en">Used for an Alpha String, length 2 (for letter codes)</xs:documentation>
                                    </xs:annotation>
                                    <xs:restriction base="xs:string">
                                       <xs:minLength value="1"/>
                                       <xs:maxLength value="2"/>
                                    </xs:restriction>
                                 </xs:simpleType>
                              </xs:attribute>
                              <xs:attribute name="ResBookDesigQuantity" use="optional">
                                 <xs:annotation>
                                    <xs:documentation xml:lang="en">Seat quantity available for this Reservation Booking Designator (RBD).</xs:documentation>
                                 </xs:annotation>
                                 <xs:simpleType>
                                    <xs:annotation>
                                       <xs:documentation xml:lang="en">Used for Numeric Strings, minimum length 1, maximum length 3.</xs:documentation>
                                    </xs:annotation>
                                    <xs:restriction base="xs:string">
                                       <xs:pattern value="[0-9]{1,3}"/>
                                    </xs:restriction>
                                 </xs:simpleType>
                              </xs:attribute>
                              <xs:attribute name="ResBookDesigStatusCode" type="ResBookDesigStatusCodeType" use="optional"/>
                              <xs:attribute name="CabinCode" type="xs:string" use="optional"/>
                              <xs:attribute name="CabinDescription" type="xs:string" use="optional"/>
                              <xs:attribute name="LocalResBookDesigQuantity" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>The quantity on this RBD for fares where availability varies for "point-to-point" vs the full itinerary</xs:documentation>
                                 </xs:annotation>
                                 <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                       <xs:pattern value="[0-9]{1,3}"/>
                                    </xs:restriction>
                                 </xs:simpleType>
                              </xs:attribute>
                              <xs:attribute name="LocalResBookDesigStatusCode" type="ResBookDesigStatusCodeType" use="optional">
                                 <xs:annotation>
                                    <xs:documentation>The status code on this RBD for fares where availability varies for "point-to-point" vs the full itinerary</xs:documentation>
                                 </xs:annotation>
                              </xs:attribute>
                           </xs:extension>
                        </xs:complexContent>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="FlightLeg">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element ref="DepartureAirport"/>
                           <xs:element ref="ArrivalAirport"/>
                           <xs:element minOccurs="0" ref="Equipment"/>
                           <xs:element minOccurs="0" ref="MealServices"/>
                           <xs:element minOccurs="0" name="CabinInfo">
                              <xs:annotation>
                                 <xs:documentation>Cabin information
e.g. type, class, capacity, psgr counts</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element maxOccurs="9" minOccurs="0" name="CabinInfoItem">
                                       <xs:complexType>
                                          <xs:sequence>
                                             <xs:element minOccurs="0" ref="CabinType"/>
                                             <xs:element minOccurs="0" ref="BookClass"/>
                                             <xs:element minOccurs="0" ref="CabinCapacity"/>
                                             <xs:element minOccurs="0" ref="BookedPsgrCnt"/>
                                             <xs:element minOccurs="0" ref="StandbyPsgrCnt"/>
                                             <xs:element minOccurs="0" ref="MaxSellCnt"/>
                                             <xs:element minOccurs="0" ref="SeatWithholdCnt"/>
                                          </xs:sequence>
                                       </xs:complexType>
                                    </xs:element>
                                 </xs:sequence>
                                 <xs:attribute name="NumberOfCabins" type="xs:nonNegativeInteger" use="required"/>
                              </xs:complexType>
                           </xs:element>
                           <xs:element minOccurs="0" name="PNREnhancements">
                              <xs:annotation>
                                 <xs:documentation>This is a collection of PNREnhancements</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element maxOccurs="99" ref="PNREnhancement"/>
                                 </xs:sequence>
                              </xs:complexType>
                           </xs:element>
                           <xs:element minOccurs="0" ref="ns7:Notes"/>
                        </xs:sequence>
                        <xs:attribute name="RPH" type="RPH_Type" use="required">
                           <xs:annotation>
                              <xs:documentation>The reference number of this flight leg. This is a unique index for each flight leg of a segment. The first index is zero and every additional flight leg is an increment of one from the previous.
Example: The first flight leg of a segment will have this value set to 0, the next flight leg is 1 and so on for every flight leg</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DepartureDateTime" type="xs:dateTime" use="optional">
                           <xs:annotation>
                              <xs:documentation>The departure date and time for this flight segment in a yyyy-mm-ddThh:mm:ss format.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ArrivalDateTime" type="xs:dateTime" use="optional">
                           <xs:annotation>
                              <xs:documentation>The arrival date and time for this flight segment in a yyyy-mm-ddThh:mm:ss format.
Example: 2005-12-25T23:00:00</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="FlightMiles" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>Miles traveled in this flight segment.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="FrequentFlierMiles" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>Frequent Flier miles earned for this flight leg.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="LegType">
                     <xs:annotation>
                        <xs:documentation>The type of leg being requested from BIFA. Valid values are: Air, Surface, Construction, VariableDates</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="AIR"/>
                           <xs:enumeration value="SURFACE"/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="EticketEligibility">
                     <xs:annotation>
                        <xs:documentation>Indicates if flight is electronic ticketing eligible. Possible values: E - Carrier has electronic ticketing and schedule may be eligible. X - Carrier has electronic ticketing and schedule is not eligible. Bank - No data regarding electronic ticketing was received from carrier</xs:documentation>
                     </xs:annotation>
                     <xs:simpleType>
                        <xs:restriction base="xs:string">
                           <xs:enumeration value="E"/>
                           <xs:enumeration value="X"/>
                           <xs:enumeration value="Y"/>
                           <xs:enumeration value=" "/>
                        </xs:restriction>
                     </xs:simpleType>
                  </xs:element>
                  <xs:element minOccurs="0" name="SegmentCreationDate" type="xs:dateTime">
                     <xs:annotation>
                        <xs:documentation>Contains the date flight segment is added to the PNR</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="PNREnhancements">
                     <xs:annotation>
                        <xs:documentation>This is a collection of PNREnhancements</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="99" ref="PNREnhancement"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="OperatingCarrierRBD" type="xs:string">
                     <xs:annotation>
                        <xs:documentation>If available, the RBD of the operating carrier can be provided.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
                  <xs:element minOccurs="0" name="GoverningCarrier" type="xs:string"/>
                  <xs:element minOccurs="0" name="TerminalInformation">
                     <xs:annotation>
                        <xs:documentation>Contains Terminal Information</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element minOccurs="0" ref="departureStation"/>
                           <xs:element minOccurs="0" ref="arrivalStation"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:Equipment.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:Equipment.types.extensions.air.rqrs.ota.datalex.com">
         <xs:complexType name="LocationType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Code and domestic/international flag to describe a location point</xs:documentation>
            </xs:annotation>
            <xs:simpleContent>
               <xs:extension base="xs:string">
                  <xs:attribute name="LocationCode" type="xs:string" use="required"/>
               </xs:extension>
            </xs:simpleContent>
         </xs:complexType>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="ChangeAirport" type="LocationType">
                     <xs:annotation>
                        <xs:documentation>The airport where a change of gauge occurs.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:OperationTime.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:OperationTime.types.extensions.air.rqrs.ota.datalex.com">
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:complexContent mixed="false">
                  <xs:extension base="xs:anyType"/>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:rqrs.cbp.datalex.com" xmlns="urn:rqrs.cbp.datalex.com">
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
         <xs:import namespace="urn:common.rqrs.cbp.datalex.com"/>
         <xs:complexType name="CBPSvRQBase">
            <xs:annotation>
               <xs:documentation>The base type for CBP requests.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
               <xs:extension base="ns7:SvRQBase">
                  <xs:sequence>
                     <xs:element minOccurs="0" ref="ns34:SessionInfo"/>
                     <xs:element minOccurs="0" ref="ns7:MultivariateTest"/>
                  </xs:sequence>
                  <xs:attribute name="ServiceName" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The matrix service name to invoke with the request xml</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ClientId" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The ID which identifies the client and is used to select the servant configuration for this request</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="BusinessFlowStep" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The current step in the client's business flow: used to to select the servant configuration for this request</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="UserSessionId" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The ID which identifies the corresponding users flow requests.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
         <xs:complexType name="CBPSvRSBase">
            <xs:annotation>
               <xs:documentation>The base type for CBP responses.</xs:documentation>
            </xs:annotation>
            <xs:complexContent>
               <xs:extension base="ns7:SvRSBase">
                  <xs:sequence>
                     <xs:element minOccurs="0" ref="ns7:MultivariateTest"/>
                  </xs:sequence>
                  <xs:attribute name="ClientId" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The ID which identifies the client which made the corresponding request.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ResultFilter" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>Filter to be used in TDP Retailer for applying specific processing to the response</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="UserSessionId" type="xs:string" use="optional">
                     <xs:annotation>
                        <xs:documentation>The ID which identifies the corresponding users flow requests.</xs:documentation>
                     </xs:annotation>
                  </xs:attribute>
               </xs:extension>
            </xs:complexContent>
         </xs:complexType>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:common.rqrs.cbp.datalex.com" xmlns="urn:common.rqrs.cbp.datalex.com">
