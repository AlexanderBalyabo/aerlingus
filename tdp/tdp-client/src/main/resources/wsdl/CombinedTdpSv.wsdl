                     <xs:sequence>
                        <xs:element name="Text" type="xs:string"/>
                     </xs:sequence>
                     <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
                     <xs:attribute name="PolicyName" type="xs:string" use="required"/>
                  </xs:complexType>
               </xs:element>
