      <xs:schema elementFormDefault="qualified" targetNamespace="urn:PassengerFare.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:PassengerFare.types.extensions.air.rqrs.ota.datalex.com">
         <xs:import namespace="urn:common.air.rqrs.datalex.com"/>
         <xs:simpleType name="AlphaLength1to2">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for an Alpha String, length 1 to 2</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[a-zA-Z]{1,2}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="StringLength1to32">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 32</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="32"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="StringLength1to16">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 16</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="16"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:element name="TPA_Extension">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="TaxSupplementalInfos">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="TaxSupplementalInfo">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element minOccurs="0" ref="ns20:RedemptionAttribs"/>
                                 </xs:sequence>
                                 <xs:attribute name="TaxCode" type="StringLength1to16" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Tax Code</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="TaxName" type="xs:string" use="required">
                                    <xs:annotation>
                                       <xs:documentation>Name of the tax</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="TaxCountryCode" type="AlphaLength1to2" use="required">
                                    <xs:annotation>
                                       <xs:documentation>The IATA 2 letter country code that levies the tax</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="AirportCityCode" type="xs:string" use="optional"/>
                                 <xs:attribute name="IntDomAssessment" type="xs:string" use="optional"/>
                                 <xs:attribute name="TaxSequence" type="xs:int" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The Tax sequence from the breakdown.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="TaxBox" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The 2 letter Tax Box code used to identify the tax grouping (T1, T2, XT).</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="IsVAT" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>This is an included tax, meaning this tax is included in the charge amount.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="IsSubjectToVAT" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>This indicates if a tax is subject to VAT.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>If amount to apply is not used this is the percentage of the component price to charge for the tax/fee.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="BaseAmount" type="xs:decimal" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Original base amount of the tax.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="BaseCurrency" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Currency code of the base amount.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="FeeSupplementalInfos">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" name="FeeSupplementalInfo">
                              <xs:complexType>
                                 <xs:sequence>
                                    <xs:element minOccurs="0" ref="ns20:RedemptionAttribs"/>
                                 </xs:sequence>
                                 <xs:attribute name="FeeCode" type="StringLength1to16" use="required"/>
                                 <xs:attribute name="FeeSubCode" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The sub code of the fee.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="FeeName" type="xs:string" use="required"/>
                                 <xs:attribute name="FeeAirlineCode" type="StringLength1to16" use="required"/>
                                 <xs:attribute name="FeeAppID" type="xs:int" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The application ID of the fee.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="Amount" type="xs:decimal" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The total fee amount.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="CurrencyCode" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The currency of the fee amount.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="IsTaxable" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>Indicates if the amount of the fee is taxable.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="AirportCityCode" type="xs:string" use="optional"/>
                                 <xs:attribute name="IntDomAssessment" type="xs:string" use="optional"/>
                                 <xs:attribute name="TaxSequence" type="xs:int" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The Tax sequence from the breakdown.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="TaxBox" type="xs:string" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>The 2 letter Tax Box code used to identify the fees grouping (T1, T2, XT) in a tax breakdown.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="IsPartOfBaseFare" type="xs:boolean" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>This indicates whether the fee is a part of base fare or not.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                                 <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                                    <xs:annotation>
                                       <xs:documentation>If amount to apply is not used this is the percentage of the component price to charge for the tax/fee.</xs:documentation>
                                    </xs:annotation>
                                 </xs:attribute>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="NetFare">
                     <xs:annotation>
                        <xs:documentation>The Published Fare less any applicable commission for this inventory</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" ref="ns20:RedemptionFare"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="DiscountApplied">
                     <xs:annotation>
                        <xs:documentation>The Discount Applied to this fare. Attributes Amount, Currency and Decimal Places are all required if this element is present except when it's an Award, Money and Miles or the fare was filed only as private and therefore we cannot work out the discount amount. It will be echoed back in the response unless a price tolerance failure occurs.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="unbounded" minOccurs="0" name="CombinablePromotion" type="xs:string">
                              <xs:annotation>
                                 <xs:documentation>Promotion that can be combined for discount.</xs:documentation>
                              </xs:annotation>
                           </xs:element>
                        </xs:sequence>
                        <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is a record of the percentage value used to calculate the Amount. It will not be set when the discount is based on a flat amount. Instead AmountToApply is set.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AmountToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is the value of a flat amount discount.  DO NOT SET THIS IF DISCOUNT IS PERCENTAGE BASED.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional">
                           <xs:annotation>
                              <xs:documentation>The total amount of discount that was applied to this fare. This is set for both flat amount discounts and percentage based discounts.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RedemptionAmount" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>The total amount of redemption discount (miles) that was applied to this fare.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional">
                           <xs:annotation>
                              <xs:documentation>The 3 letter currency code of the discount amount</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>The number of decimal places the discount is expressed in</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DiscountCode" type="StringLength1to32" use="optional">
                           <xs:annotation>
                              <xs:documentation>This is the internal promotion code.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute default="true" name="DiscountDisplayed" type="xs:boolean" use="optional"/>
                        <xs:attribute name="CodeContext" type="StringLength1to32" use="optional"/>
                        <xs:attribute name="DiscountDescription" type="xs:string" use="optional"/>
                        <xs:attribute name="EndorsementText" type="xs:string" use="optional"/>
                        <xs:attribute name="RoundingRule" use="optional">
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="ROUND_UP"/>
                                 <xs:enumeration value="ROUND_DOWN"/>
                                 <xs:enumeration value="ROUND_CEILING"/>
                                 <xs:enumeration value="ROUND_FLOOR"/>
                                 <xs:enumeration value="ROUND_HALF_UP"/>
                                 <xs:enumeration value="ROUND_HALF_DOWN"/>
                                 <xs:enumeration value="ROUND_HALF_EVEN"/>
                                 <xs:enumeration value="ROUND_UNNECESSARY"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="AppliedPromoCodes" type="xs:string" use="optional"/>
                        <xs:attribute name="ExternalPromotionCode" type="xs:string">
                           <xs:annotation>
                              <xs:documentation>If a promotion code was supplied by a user, it is stored here.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CertificateCode" type="xs:string">
                           <xs:annotation>
                              <xs:documentation>If a certificate code was supplied by a user, it is supplied here.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="PromotionReference" type="xs:IDREF">
                           <xs:annotation>
                              <xs:documentation>This is an aid to link a DiscountApplied element to a detailed Promotion element.  It permits direct access to the Promotion element.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CertificateReference" type="xs:IDREF">
                           <xs:annotation>
                              <xs:documentation>This is an aid to link a DiscountApplied element to a detailed Certificate element.  It permits direct access to the Certificate element.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute default="false" name="ApplyDiscount" type="xs:boolean" use="optional">
                           <xs:annotation>
                              <xs:documentation>Flag will be reset by BRE if Discount passes the BRE validation.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CertificateNumber" type="xs:string"/>
                        <xs:attribute name="RedemptionPercentToApply" type="xs:decimal">
                           <xs:annotation>
                              <xs:documentation>This is the percentage value to apply as a discount for a promotion which is for miles. The actualy mileage value will be in RedemptionAmount.</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="MarkupApplied">
                     <xs:annotation>
                        <xs:documentation>Indicates that there is a markup applied to the original fare</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="PercentToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>percentage markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="AmountToApply" type="xs:decimal" use="optional">
                           <xs:annotation>
                              <xs:documentation>Flat amount markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional">
                           <xs:annotation>
                              <xs:documentation>Amount for flat amount markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional">
                           <xs:annotation>
                              <xs:documentation>Currency of markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional">
                           <xs:annotation>
                              <xs:documentation>Decimal places for the currency used</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="MarkupCode" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Code of markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="MarkupDescription" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Description of the nature of markup</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="CodeContext" type="xs:string" use="optional">
                           <xs:annotation>
                              <xs:documentation>Code context TDP/GDS etc</xs:documentation>
                           </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="RoundingRule" use="optional">
                           <xs:annotation>
                              <xs:documentation>Rounding rule for the currency</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="ROUND_UP"/>
                                 <xs:enumeration value="ROUND_DOWN"/>
                                 <xs:enumeration value="ROUND_CEILING"/>
                                 <xs:enumeration value="ROUND_FLOOR"/>
                                 <xs:enumeration value="ROUND_HALF_UP"/>
                                 <xs:enumeration value="ROUND_HALF_DOWN"/>
                                 <xs:enumeration value="ROUND_HALF_EVEN"/>
                                 <xs:enumeration value="ROUND_UNNECESSARY"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="FareNotes">
                     <xs:annotation>
                        <xs:documentation>Contains a list of fare notes pertaining to this passenger type</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element maxOccurs="10" name="FareNote">
                              <xs:complexType>
                                 <xs:attribute name="NoteName" type="xs:string" use="optional"/>
                                 <xs:attribute name="NoteText" type="xs:string" use="optional"/>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="LinearFareConstruction" type="xs:string"/>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="CommissionApplied">
                     <xs:complexType>
                        <xs:attribute name="PercentToApply" type="xs:decimal" use="optional"/>
                        <xs:attribute name="AmountToApply" type="xs:decimal" use="optional"/>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                        <xs:attribute name="CommissionCode" type="StringLength1to32" use="optional"/>
                        <xs:attribute name="CodeContext" type="StringLength1to32" use="optional"/>
                        <xs:attribute name="CommissionDescription" type="xs:string" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="CabinUpsellFee">
                     <xs:annotation>
                        <xs:documentation>The cabin upsell fee.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element minOccurs="0" name="PublishedFare">
                     <xs:annotation>
                        <xs:documentation>The original GDS Fare, can be different if Base Fare has a private contract discount applied to it.</xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
                        <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
                        <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
                     </xs:complexType>
                  </xs:element>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="ExchangeRate">
                     <xs:complexType>
                        <xs:attribute name="CurrencyFrom" type="ns20:AlphaLength3" use="required"/>
                        <xs:attribute name="CurrencyTo" type="ns20:AlphaLength3" use="required"/>
                        <xs:attribute name="Rate" type="xs:float" use="required"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:ItinTotalFare.types.extensions.air.rqrs.ota.datalex.com" xmlns="urn:ItinTotalFare.types.extensions.air.rqrs.ota.datalex.com">
         <xs:import namespace="urn:common.air.rqrs.datalex.com"/>
         <xs:simpleType name="StringLength1to16">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for Character Strings, length 1 to 16</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:minLength value="1"/>
               <xs:maxLength value="16"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="AlphaLength1to2">
            <xs:annotation>
               <xs:documentation xml:lang="en">Used for an Alpha String, length 1 to 2</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[a-zA-Z]{1,2}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:simpleType name="RPH_Type">
            <xs:annotation>
               <xs:documentation xml:lang="en">(Reference Place Holder) - an index code to identify an instance in a collection of like items.. For example,  used to assign individual passengers or clients to particular itinerary items.</xs:documentation>
            </xs:annotation>
            <xs:restriction base="xs:string">
               <xs:pattern value="[0-9]{1,8}"/>
            </xs:restriction>
         </xs:simpleType>
         <xs:complexType name="CurrencyAmountType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Provides a monetary amount and the code of the currency in which this amount is expressed.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
            <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
            <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
         </xs:complexType>
         <xs:complexType name="AirTaxType">
            <xs:annotation>
               <xs:documentation xml:lang="en">Defines the data fields available for air tax.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="TaxCode" type="StringLength1to16" use="required">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Identifies the code for the tax.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="Amount" type="ns20:Money" use="optional"/>
            <xs:attribute name="CurrencyCode" type="ns20:AlphaLength3" use="optional"/>
            <xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional"/>
         </xs:complexType>
         <xs:complexType name="RequestedFareType">
