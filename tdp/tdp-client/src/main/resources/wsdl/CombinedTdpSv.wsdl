                        </xs:annotation>
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="CurrencyAmountType">
                                 <xs:sequence>
                                    <xs:element minOccurs="0" ref="ns218:TPA_Extension"/>
                                 </xs:sequence>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element minOccurs="0" name="EquivFare">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">Price of the inventory excluding taxes and fees in the payable currency.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="CurrencyAmountType">
                                 <xs:sequence>
                                    <xs:element minOccurs="0" ref="ns219:TPA_Extension"/>
                                 </xs:sequence>
                              </xs:extension>
                           </xs:complexContent>
                        </xs:complexType>
                     </xs:element>
                     <xs:element minOccurs="0" ref="Taxes">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">This is a collection of Taxes</xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element minOccurs="0" name="Fees">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">This is a collection of Fees</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                           <xs:complexContent>
                              <xs:extension base="ns7:CustomProperties">
                                 <xs:sequence>
                                    <xs:element maxOccurs="9" ref="Fee">
