               <xs:attribute name="BaggageAllowanceWeight" type="xs:decimal" use="optional"/>
               <xs:attribute name="BaggageAllowanceWeightInPounds" type="xs:boolean" use="optional"/>
               <xs:attribute name="ChangeOfGaugeSeatPreference1" type="SeatCharacteristicType" use="optional"/>
               <xs:attribute name="ChangeOfGaugeSeatPreference2" type="SeatCharacteristicType" use="optional"/>
               <xs:attribute name="ChangeOfGaugeSeatPreference3" type="SeatCharacteristicType" use="optional"/>
               <xs:attribute name="ChangeOfGaugeSeatNumber" type="xs:string" use="optional"/>
               <xs:attribute name="ChangeOfGaugeSeatSynchStatus" type="RemoteSynchStatusType" use="optional"/>
               <xs:attribute name="MarketingClassificationCode" type="xs:string">
