      <xs:schema elementFormDefault="qualified" targetNamespace="urn:guestroom.types.extensions.hotel.rqrs.ota.datalex.com" xmlns="urn:guestroom.types.extensions.hotel.rqrs.ota.datalex.com">
         <xs:import namespace="urn:common.rqrs.datalex.com"/>
