      <xs:schema elementFormDefault="qualified" targetNamespace="urn:CancelPenalty.types.extensions.hotel.rqrs.ota.datalex.com" xmlns="urn:CancelPenalty.types.extensions.hotel.rqrs.ota.datalex.com">
         <xs:import namespace="http://www.opentravel.org/OTA/2003/05/hotel"/>
         <xs:complexType name="NewDeadlineType">
            <xs:annotation>
               <xs:documentation xml:lang="en">The absolute deadline or amount of offset time before a deadline for a payment of cancel goes into effect.</xs:documentation>
            </xs:annotation>
            <xs:attribute name="AbsoluteDeadline" type="ns4:TimeOrDateTimeType" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">Defines the absolute deadline.  Either this or the offset attributes may be used.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="OffsetTimeUnit" type="ns4:TimeUnitType" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">The units of time, e.g.: days, hours, etc., that apply to the deadline.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="OffsetUnitMultiplier" type="ns4:Numeric0to999" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">The number of units of DeadlineTimeUnit.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="FromUnit" type="ns4:Numeric0to999" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">The number of units of type OffsetTimeUnit where the deadline starts From.  for example if this unit is 5 and OffsetTimeUnit is Day, this will mean that from 5 days till ToUnit, this cancellation will charge apply</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="ToUnit" type="ns4:Numeric0to999" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">The number of units of DeadlineTimeUnit where the deadline Ends. If this field is not set and a corresponding FromUnit exists, this will mean that the end unit doesn't exist for example No Cancellation charge from day = 90 will have a value in FromUnit but no value in ToUnit</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="OffsetDropTime" use="optional">
               <xs:annotation>
                  <xs:documentation xml:lang="en">An enumerated type indicating when the deadline drop time goes into effect.</xs:documentation>
               </xs:annotation>
               <xs:simpleType>
                  <xs:restriction base="ns4:StringLength1to16">
                     <xs:enumeration value="BeforeArrival"/>
                     <xs:enumeration value="AfterBooking"/>
                  </xs:restriction>
               </xs:simpleType>
            </xs:attribute>
         </xs:complexType>
         <xs:element name="TPA_Extensions">
            <xs:annotation>
               <xs:documentation>TPA_Extension definition that every element/type references</xs:documentation>
            </xs:annotation>
            <xs:complexType mixed="false">
               <xs:sequence>
                  <xs:element minOccurs="0" name="CancelDeadline" type="NewDeadlineType">
                     <xs:annotation>
                        <xs:documentation xml:lang="en">Cancellation deadline, absolute or relative.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:schema>
