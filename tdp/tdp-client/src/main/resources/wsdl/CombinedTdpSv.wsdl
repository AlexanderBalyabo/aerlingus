                     </xs:annotation>
                  </xs:attribute>
               </xs:extension>
            </xs:simpleContent>
         </xs:complexType>
         <xs:element name="ConnectionLocation">
            <xs:complexType>
               <xs:simpleContent>
                  <xs:extension base="LocationType">
                     <xs:attribute default="true" name="Inclusive" type="xs:boolean" use="optional"/>
                     <xs:attributeGroup ref="PreferLevelGroup">
                        <xs:annotation>
                           <xs:documentation xml:lang="en">The preference level for the connection point - only, unacceptable, 	preferred.</xs:documentation>
                        </xs:annotation>
                     </xs:attributeGroup>
                     <xs:attribute name="MinChangeTime" type="xs:nonNegativeInteger" use="optional">
