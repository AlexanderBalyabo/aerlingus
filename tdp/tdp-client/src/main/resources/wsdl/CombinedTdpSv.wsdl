                        <xs:attribute name="AwardTimeFrame" use="optional">
                           <xs:annotation>
                              <xs:documentation>Indicates when the points are earned, at Booking Time or at End of Consumption</xs:documentation>
                           </xs:annotation>
                           <xs:simpleType>
                              <xs:restriction base="xs:string">
