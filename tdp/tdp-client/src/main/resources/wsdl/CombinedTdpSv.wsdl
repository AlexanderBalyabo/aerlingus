                           <xs:restriction base="xs:string">
                              <xs:enumeration value="Consumer"/>
                              <xs:enumeration value="Agent"/>
                              <xs:enumeration value="TravelAgency"/>
                              <xs:enumeration value="Corporate"/>
                           </xs:restriction>
                        </xs:simpleType>
                     </xs:attribute>
                     <xs:attribute name="UserName" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The User the session is established on</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="TimeZone" type="xs:string" use="optional"/>
                     <xs:attribute name="CurrencyCode" type="xs:string" use="optional"/>
                     <xs:attribute name="StorefrontCode" type="xs:string" use="optional"/>
                     <xs:attribute name="SourceURL" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The URL the session is established on, like: www.asite.com/adir</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SourceIP" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The IP address of the browser like: 198.10.223.7</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SourceCountryCode" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>The 2-letter country code of the ISP, like: IE</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SessionKey" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>A unique string of hex digits identifying this session, like: F90AB437...</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                  </xs:extension>
               </xs:complexContent>
            </xs:complexType>
         </xs:element>
      </xs:schema>
      <xs:schema elementFormDefault="qualified" targetNamespace="urn:profiles.internal.rqrs.cbs.datalex.com" xmlns="urn:profiles.internal.rqrs.cbs.datalex.com">
