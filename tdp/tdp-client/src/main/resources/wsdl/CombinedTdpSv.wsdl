                           <xs:simpleType>
                              <xs:restriction base="xs:string">
                                 <xs:enumeration value="CANCELLATION"/>
                                 <xs:enumeration value="NOSHOW"/>
                              </xs:restriction>
                           </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="LanguageCode" type="xs:string" use="required"/>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
