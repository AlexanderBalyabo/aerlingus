               <xs:complexContent>
                  <xs:extension base="SequencedType">
                     <xs:sequence>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ExtraInformation"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="DocumentValue"/>
                        <xs:element maxOccurs="unbounded" minOccurs="0" ref="ProviderPaymentLink"/>
                     </xs:sequence>
                     <xs:attribute name="NameOnDocument" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Name on document.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="PrimaryDocNumber" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Document unique number.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="SecondaryDocNumber" type="xs:string" use="optional">
                        <xs:annotation>
                           <xs:documentation>Exchanged document used in payment for this document.</xs:documentation>
                        </xs:annotation>
                     </xs:attribute>
                     <xs:attribute name="IssuedCityCode" type="xs:string" use="optional">
