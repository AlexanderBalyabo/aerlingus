               <xs:attribute name="AgeAtFirstConsume" type="xs:integer" use="optional"/>
               <xs:attribute name="DateOfBirth" type="xs:date" use="optional"/>
               <xs:attribute name="OverrideFirstName" type="xs:string" use="optional"/>
               <xs:attribute name="OverrideMiddleName" type="xs:string" use="optional"/>
               <xs:attribute name="OverrideLastName" type="xs:string" use="optional"/>
               <xs:attribute name="OverrideCompleteName" type="xs:string" use="optional"/>
               <xs:attribute name="OverrideNameSuffix" type="xs:string" use="optional"/>
               <xs:attribute name="OverrideTitle" type="xs:string" use="optional"/>
               <xs:attribute name="OverrideSalutation" type="xs:string" use="optional"/>
               <xs:attribute name="GuestSequence" type="xs:positiveInteger" use="optional"/>
               <xs:attribute name="GuestDocRef" type="xs:IDREF" use="optional"/>
            </xs:complexType>
         </xs:element>
         <xs:complexType name="EventModType">
            <xs:attribute name="ConfirmationTrackingCode" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>Used to log the confirmation or tracking code from a remote system.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="EventCompletedDateTime" type="xs:dateTime" use="optional">
               <xs:annotation>
                  <xs:documentation>This is the timestamp of when the event was completed. For system-managed events, this is set by M3 for client managed events, this is set by the client.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="NumberOfRetries" type="xs:integer" use="optional">
               <xs:annotation>
                  <xs:documentation>Set to the number of times the client wishes the event processing to be retried upon failure.  Each failure decrements the count by 1.  At zero retries an error is thrown. Currently for client use only.  Uses 0 by default.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="EventOccurrenceDateTime" type="xs:dateTime" use="optional">
               <xs:annotation>
                  <xs:documentation>The date and time the event is scheduled to occur.  If this is a system tracking type record this would the time tracking began. This value should be expressed in the time zone of the M3 reservation system.  It is important to note that if a deposit date / time is being set, that the deadline from the provider be converted (if necessary) to the local system time zone.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="AssignedToInternalAgentSequence" type="xs:integer" use="optional">
               <xs:annotation>
                  <xs:documentation>The identification of the reservation  agent that has been assigned this event.	Client supplied.  Events can be used to assign processing from one agent to another.  This is the agent that has been assigned the event.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
            <xs:attribute name="AssignedToInternalAgentDocRef" type="xs:IDREF" use="optional"/>
            <xs:attribute name="Description" type="xs:string" use="optional"/>
            <xs:attribute name="ResultingStatus" type="xs:string" use="optional">
               <xs:annotation>
                  <xs:documentation>The status code to be assigned to the reservation or reservation component at successful completion of the event.</xs:documentation>
               </xs:annotation>
            </xs:attribute>
